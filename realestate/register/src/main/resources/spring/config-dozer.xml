<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
       default-autowire="byName" default-lazy-init="false">

    <bean id="acceptDozerMapper" class="cn.gtmap.realestate.common.core.dozer.DozerBeanMapper">
        <property name="mappingFiles">
            <list>
                <value>dozer/dozerConfiguration.xml</value>
                <value>dozer/AcceptBdcQlr.xml</value>
                <value>dozer/AcceptBdcXm.xml</value>
                <value>dozer/AcceptBdcXmLsgx.xml</value>
                <value>dozer/AcceptBdcCshFwkg.xml</value>
            </list>
        </property>
    </bean>
</beans>