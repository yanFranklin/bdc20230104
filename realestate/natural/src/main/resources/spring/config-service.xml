<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/tx
           http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

    <bean id="naturalBeanFactory"
          class="cn.gtmap.realestate.natural.config.NaturalBeanFactory">
        <!--自然资源业务填报信息服务-->
        <property name="zrzyYwxxServiceSet">
            <set>
                <!--自然资源项目信息-->
                <ref bean="zrzyXmServiceImpl"/>
                <!--自然资源登记单元信息-->
                <ref bean="zrzyDjdyServiceImpl"/>
                <!--自然资源水流信息-->
                <ref bean="zrzySzyServiceImpl"/>
                <!--自然资源湿地信息-->
                <ref bean="zrzySdServiceImpl"/>
                <!--自然资源森林信息-->
                <ref bean="zrzySlServiceImpl"/>
                <!--自然资源草原信息-->
                <ref bean="zrzyCyServiceImpl"/>
                <!--自然资源荒地信息-->
                <ref bean="zrzyHdServiceImpl"/>
                <!--自然资源海域信息-->
                <ref bean="zrzyHyServiceImpl"/>
                <!--自然资源无居民海岛信息-->
                <ref bean="zrzyWjmhdServiceImpl"/>
                <!--自然资源探明储量矿产资源信息-->
                <ref bean="zrzyTmclkczyServiceImpl"/>
                <!--自然资源公共管制关联信息-->
                <ref bean="zrzyGggzGlxxServiceImpl"/>
                <!--自然资源不动产权利关联信息-->
                <ref bean="zrzyBdcqlGlxxServiceImpl"/>
                <!--自然资源矿业权关联信息-->
                <ref bean="zrzyKyqGlxxServiceImpl"/>
                <!--自然资源取水权关联信息-->
                <ref bean="zrzyQsqGlxxServiceImpl"/>
                <!--自然资源排污权关联信息-->
                <ref bean="zrzyPwqGlxxServiceImpl"/>


            </set>
        </property>

        <!--自然资源业务初始化服务-->
        <property name="zrzyYwxxInitServiceSet">
            <set>
                <!--自然资源项目信息-->
                <ref bean="zrzyXmServiceImpl"/>
                <!--自然资源项目历史信息-->
                <ref bean="zrzyXmLsgxServiceImpl"/>
                <!--自然资源登记单元信息-->
                <ref bean="zrzyDjdyServiceImpl"/>
                <!--自然资源水流信息-->
                <ref bean="zrzySzyServiceImpl"/>
                <!--自然资源湿地信息-->
                <ref bean="zrzySdServiceImpl"/>
                <!--自然资源森林信息-->
                <ref bean="zrzySlServiceImpl"/>
                <!--自然资源草原信息-->
                <ref bean="zrzyCyServiceImpl"/>
                <!--自然资源荒地信息-->
                <ref bean="zrzyHdServiceImpl"/>
                <!--自然资源海域信息-->
                <ref bean="zrzyHyServiceImpl"/>
                <!--自然资源无居民海岛信息-->
                <ref bean="zrzyWjmhdServiceImpl"/>
                <!--自然资源探明储量矿产资源信息-->
                <ref bean="zrzyTmclkczyServiceImpl"/>
                <!--自然资源公共管制关联信息-->
                <ref bean="zrzyGggzGlxxServiceImpl"/>
                <!--自然资源不动产权利关联信息-->
                <ref bean="zrzyBdcqlGlxxServiceImpl"/>
                <!--自然资源矿业权关联信息-->
                <ref bean="zrzyKyqGlxxServiceImpl"/>
                <!--自然资源取水权关联信息-->
                <ref bean="zrzyQsqGlxxServiceImpl"/>
                <!--自然资源排污权关联信息-->
                <ref bean="zrzyPwqGlxxServiceImpl"/>


            </set>
        </property>
    </bean>


</beans>