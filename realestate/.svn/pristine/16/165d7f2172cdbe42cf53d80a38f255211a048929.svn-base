<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="dozerMapper" class="cn.gtmap.realestate.common.core.dozer.DozerBeanMapper">
        <property name="mappingFiles">
            <list>
                <value>conf/dozer/dozerConfiguration.xml</value>
                <value>conf/dozer/ycsl/YcslXm.xml</value>
                <value>conf/dozer/ycsl/YcslGxr.xml</value>
                <value>conf/dozer/wwsq/GxwwClxx.xml</value>
                <value>conf/dozer/wwsq/GxwwSqr.xml</value>
                <value>conf/dozer/wwsq/GxWwSqxm.xml</value>
                <value>conf/dozer/wwsq/GxWwSqxx.xml</value>
                <value>conf/dozer/wwsq/GxwwSwxx.xml</value>
            </list>
        </property>
    </bean>
    <bean id="dozerSameNullTMapper" class="cn.gtmap.realestate.common.core.dozer.DozerBeanMapper">
        <property name="mappingFiles">
            <list>
                <value>conf/dozer/dozerSameNullTMapper.xml</value>
            </list>
        </property>
    </bean>
    <bean id="dozerSameNullFMapper" class="cn.gtmap.realestate.common.core.dozer.DozerBeanMapper">
        <property name="mappingFiles">
            <list>
                <value>conf/dozer/dozerSameNullFMapper.xml</value>
            </list>
        </property>
    </bean>
</beans>