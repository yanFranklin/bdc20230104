        SELECT
        count(*) as sl,
        x.djlx as djlx,
        z.mc as lxmc
        FROM bdc_xm x
        LEFT JOIN bdc_zd_djlx z
        ON x.djlx = z.dm
        where x.djlx is not null
        <if test="kssj !=null and kssj != ''">
