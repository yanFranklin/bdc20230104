package cn.gtmap.realestate.supervise.core.qo;

/**
 * @author <a href="mailto:zhuyong@gtmap.cn">zhuyong</a>
 * @version 1.0, 2019/2/12
 * @description 组织机构查询QO
 */
public class BdcXtOrgQO {
    /**
     * 机构ID
     */
    private String id;

    /**
     * 机构父级目录ID
     */
    private String pid;

    /**
     * 机构名称
     */
    private String name;


    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getPid() {
        return pid;
    }

    public void setPid(String pid) {
        this.pid = pid;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return "BdcXtOrgQO{" +
                "id='" + id + '\'' +
                ", pid='" + pid + '\'' +
                ", name='" + name + '\'' +
                '}';
    }
}
