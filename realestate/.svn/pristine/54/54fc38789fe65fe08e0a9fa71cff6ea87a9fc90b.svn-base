<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>质检单信息</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="../../static/lib/font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css">

    <!--下拉框需要-->
    <link rel="stylesheet" href="../../static/lib/form-select/formSelects-v4.css">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/application.css">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/form.css">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/form-tab.css"/>
    <link rel="stylesheet" href="../../static/lib/bdcui/css/table.css">
    <link rel="stylesheet" href="../../static/lib/form-select/formSelects-v4.css"/>
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css" />
    <link rel="stylesheet" href="../../static/lib/bdcui/css/form-tab.css"/>

</head>
<body>
<div class="bdc-percentage-container">
<div class="bdc-search-content">
    <div class="bdc-table-box">
        <form class="layui-form sl-from" id="all">
            <!--内容区-->
            <div class="bdc-inner-content">
                <div class="layui-tab-content" id="contentTable">
                    <div class="layui-tab-item layui-show">
                        <div class="form-margin-area">
                            <div id="zjdxx"></div>
                            <div id="zjxx"></div>

                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

</div>
</div>
</body>


<script src="../../static/lib/jquery/jquery.min.js"></script>
<script src="../../static/lib/layui/layui.js"></script>
<!--添加水印-->
<script src="../../static/lib/bdcui/js/common.js"></script>
<script src="../../static/js/common.js"></script>
<script src="../../static/js/commonSearch.js"></script>
<script src="../../static/js/formCenter.js"></script>
<script src="../../static/js/changzhou/zj/zjdxx.js"></script>

<!--zjd-->
<script id="zjdTpl" type="text/html">
    {{# if(d){ }}
    <form class="layui-form setOverflow" id="bdcXmForm">
        <div class="basic-info">
            <div class="title-sign bdc-title-sign-btn">
                <p>质检单信息</p>
            </div>
            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label change-label-width bdc-label-newline">质检单编号</label>
                    <div class="layui-input-inline">
                        <input type="text" autocomplete="off" disabled="off"  class="layui-input zjxx" name="zjdbh" id="zjdbh"
                               value="{{d.zjdbh || ''}}">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">质检人</label>
                    <div class="layui-input-inline">
                        <input type="text" autocomplete="off" disabled="off" class="layui-input zjxx" name="zjr" id="zjr"
                               value="{{d.zjr || ''}}">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">质检时间</label>
                    <div class="layui-input-inline">
                        <input type="text" autocomplete="off" disabled="off" class="layui-input zjxx" name="zjdcjsj" id="zjdcjsj"
                               value="{{d.zjdcjsj || ''}}">
                    </div>
                </div>
            </div>
        </div>
    </form>
    {{#  } }}

</script>



<!--zj-->
<script id="zjTpl" type="text/html">
    <div class="title-sign">
        <p>质检详情</p>
    </div>
    <div class="basic-info">
        <form name="sqrForm" class="common-form sl-from layui-form">
            <table class="layui-table" border="1">
                <tr class="gray-tr">
                    <td class="set-back-color set-center">序号</td>
                    <td class="set-back-color set-center">登记类型</td>
                    <td class="set-back-color set-center">受理编号</td>
                    <td class="set-back-color set-center">不动产坐落</td>
                    <td class="set-back-color set-center">质检发现问题</td>
                    <td class="set-back-color set-center">受理人</td>
                    <td class="set-back-color set-center">登簿审核人</td>
                    <td class="set-back-color set-center">抽检人员</td>
                    <td class="set-back-color set-center">备注</td>
                    <td class="set-back-color set-center" style="min-width: 120px">操作</td>
                </tr>
                {{# if(!d || d.length == 0){ }}
                <tr class="bdc-table-cotent bdc-table-none">
                    <td colspan="10">
                        <div class="layui-none"><img src="../../static/lib/bdcui/images/table-none.png" alt="">无数据</div>
                    </td>
                </tr>
                {{# } else { }}
                {{# layui.each(d, function(index, value){ }}
                <tr class="bdc-table-cotent">
                    <td>{{ index+1 }}</td>
                    <td>{{ getZdMc('djlx', value.djlx) }}</td>
                    <td>{{ value.slbh || '' }}</td>
                    <td>{{ value.zl || ''}}</td>
                    <td title="{{ value.spyj || '' }}">{{ value.spyj || '' }}</td>
                    <td>{{ value.slr || '' }}</td>
                    <td>{{ value.dbr || '' }}</td>
                    <td>{{ value.hdr || '' }}</td>
                    <td title="{{ value.bz || '' }}">{{ value.bz || '' }}</td>
                    <td >      <span class="layui-btn layui-btn-xs bdc-major-btn" lay-event="zjDad" onclick="openZjDad('{{ value.zjid }}')">质量检查档案袋</span>
                    </td>
                </tr>
                {{# }); }}
                {{# }}}
            </table>
        </form>
    </div>
</script>

</html>