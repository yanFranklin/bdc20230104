package cn.gtmap.realestate.common.core.dto.config;


import io.swagger.annotations.ApiModelProperty;

/**
 * 字典值编辑实体
 * @author wangyinghao
 */
public class BdcZdChangeDTO {

    @ApiModelProperty(value = "字典表名称")
    private String zdmc;

    @ApiModelProperty(value = "字典项标识")
    private Integer dm;

    @ApiModelProperty(value = "字典项原标识")
    private Integer orgdm;

    @ApiModelProperty(value = "字典项配置值")
    private String mc;

    public String getZdmc() {
        return zdmc;
    }

    public void setZdmc(String zdmc) {
        this.zdmc = zdmc;
    }

    public Integer getDm() {
        return dm;
    }

    public void setDm(Integer dm) {
        this.dm = dm;
    }

    public Integer getOrgdm() {
        return orgdm;
    }

    public void setOrgdm(Integer orgdm) {
        this.orgdm = orgdm;
    }

    public String getMc() {
        return mc;
    }

    public void setMc(String mc) {
        this.mc = mc;
    }
}
