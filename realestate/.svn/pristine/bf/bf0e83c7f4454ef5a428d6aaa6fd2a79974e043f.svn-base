<!--
    4 职责权能监管-授权信息管理台账
-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <title>职责权能监管-授权信息管理台账</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../static/lib/form-select/formSelects-v4.css">

    <link rel="stylesheet" href="../../static/lib/bdcui/css/common.css"/>
    <link rel="stylesheet" href="../../static/lib/bdcui/css/table.css"/>
    <link rel="stylesheet" href="../../static/lib/bdcui/css/form-tab.css"/>
    <link rel="stylesheet" href="../../static/lib/bdcui/css/search.css"/>
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css"/>
    <link rel="stylesheet" href="../../static/lib/registerui/css/percentage-layout.css"/>
    <link rel="stylesheet" href="../../static/css/zssyjg/yzhtjtz.css"/>
</head>
<body>
    <div class="bdc-percentage-container">
        <div class="bdc-search-content">
            <div class="bdc-search-box">
                <form class="layui-form">
                    <div class="layui-form-item pf-form-item">
                        <div class="layui-inline">
                            <label class="layui-form-label">授权人员</label>
                            <div class="layui-input-inline">
                                <input type="text" id="sqry" name="sqry" autocomplete="off" placeholder="请输入" class="layui-input search">
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label">审批文件</label>
                            <div class="layui-input-inline">
                                <input type="text" id="spwjmc" name="spwjmc" autocomplete="off" placeholder="请输入" class="layui-input search">
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label">授权说明</label>
                            <div class="layui-input-inline">
                                <input type="text" id="sqsm" name="sqsm" autocomplete="off" placeholder="请输入" class="layui-input search">
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label">操作人</label>
                            <div class="layui-input-inline">
                                <input type="text" id="czr" name="czr" autocomplete="off" placeholder="请输入" class="layui-input search">
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label">授权时间(起)</label>
                            <div class="layui-input-inline">
                                <input id="sqsjq" name="sqsjq" autocomplete="off" class="layui-input search required" type="text"  placeholder="请选择"/>
                            </div>
                        </div>

                        <div class="layui-inline">
                            <label class="layui-form-label">授权时间(止)</label>
                            <div class="layui-input-inline">
                                <input id="sqsjz" name="sqsjz" autocomplete="off" class="layui-input search required" type="text" placeholder="请选择"/>
                            </div>
                        </div>

                        <div class="layui-inline bdc-filter">
                            <label class="layui-form-label">授权类型</label>
                            <div class="layui-input-inline">
                                <select lay-filter="sqlx" id="sqlx" name="sqlx" lay-search="" class="search">
                                    <option value="">请选择</option>
                                    <option value="1">新增</option>
                                    <option value="2">调岗</option>
                                    <option value="3">离职</option>
                                    <option value="4">临时授权</option>
                                </select>
                                <i class="layui-icon layui-icon-close bdc-hide"></i>
                            </div>
                        </div>

                        <div class="pf-senior-show bdc-hide">
                            <div class="layui-inline">
                                <label class="layui-form-label">所属单位</label>
                                <div class="layui-input-inline">
                                    <input type="text" id="bmmc" name="bmmc" autocomplete="off" placeholder="请输入" class="layui-input search">
                                </div>
                            </div>
                            <div class="layui-inline">
                                <label class="layui-form-label">所属科室</label>
                                <div class="layui-input-inline">
                                    <input type="text" id="ks" name="ks" autocomplete="off" placeholder="请输入" class="layui-input search">
                                </div>
                            </div>
                        </div>

                        <div class="layui-inline">
                            <button id="search" class="layui-btn bdc-major-btn" lay-submit="" lay-filter="search">查询</button>
                            <button id="reset" type="reset" class="layui-btn bdc-secondary-btn reset-btn">重置</button>
                            <a href="javascript:" class="layui-btn layui-btn-normal bdc-secondary-btn"
                               id="seniorSearch">高级查询</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>

        <!--表格-->
        <div class="bdc-table-box">
            <table class="layui-table" id="pageTable" lay-filter="pageTable"></table>
        </div>

        <!--表格上面功能按钮-->
        <script type="text/html" id="toolbar">
            <div class="layui-btn-container">
                <button class="layui-btn layui-btn-sm bdc-major-btn" lay-event="add" id="add">新增</button>
                <button class="layui-btn layui-btn-sm bdc-table-second-btn" lay-event="edit" id="edit">编辑</button>
                <button class="layui-btn layui-btn-sm layui-btn-danger bdc-delete-btn" lay-event="deleteSpwj" id="deleteSpwj">删除</button>
            </div>
        </script>

        <script type="text/html" id="barAction">
            <a class="layui-btn bdc-major-btn layui-btn-xs" lay-event="downloadSpwj" id="downloadSpwj">查看审批文件</a>
        </script>
    </div>

    <script src="../../static/lib/jquery/jquery.min.js"></script>
    <script src="../../static/lib/layui/layui.js"></script>
    <script src="../../static/lib/easyui/jquery.easyui.min.js"></script>
    <script src="../../static/lib/bdcui/js/common.js"></script>
    <script src="../../static/js/common.js"></script>
    <script src="../../static/js/formCenter.js"></script>
    <script src="../../static/js/zzqnjg/sqxxtz.js"></script>
</body>
</html>