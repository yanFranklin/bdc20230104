package cn.gtmap.realestate.common.core.dto.exchange.nantong.yrb.zlfrwjs.request;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;
import java.util.Date;

/**
 * @author <a href="mailto:haungjian@gtmap.cn">huangjian</a>
 * @Date 2022/8/12
 * @description 差额征收抵扣信息
 */
@XmlAccessorType(XmlAccessType.PROPERTY)
@XmlRootElement(name = "CEDKXX")
public class YrbZlfCedkxxRequest {


    /**
     * dkxxxh : 12344
     * dkxxfl : 抵扣分类
     * dkpjlx : 票据类型
     * dkxxdm : 代码
     * dkxxhm : 号码
     * dkxxkprq : 开票日期
     * dkxxpmje : 2.22
     */

    private Integer dkxxxh;
    private String dkxxfl;
    private String dkpjlx;
    private String dkxxdm;
    private String dkxxhm;
    private String dkxxkprq;
    private Double dkxxpmje;

    @XmlElement(name = "DKXXXH")
    public Integer getDkxxxh() {
        return dkxxxh;
    }

    public void setDkxxxh(Integer dkxxxh) {
        this.dkxxxh = dkxxxh;
    }

    @XmlElement(name = "DKXXFL")
    public String getDkxxfl() {
        return dkxxfl;
    }

    public void setDkxxfl(String dkxxfl) {
        this.dkxxfl = dkxxfl;
    }

    @XmlElement(name = "DKPJLX")
    public String getDkpjlx() {
        return dkpjlx;
    }

    public void setDkpjlx(String dkpjlx) {
        this.dkpjlx = dkpjlx;
    }

    @XmlElement(name = "DKXXDM")
    public String getDkxxdm() {
        return dkxxdm;
    }

    public void setDkxxdm(String dkxxdm) {
        this.dkxxdm = dkxxdm;
    }

    @XmlElement(name = "DKXXHM")
    public String getDkxxhm() {
        return dkxxhm;
    }

    public void setDkxxhm(String dkxxhm) {
        this.dkxxhm = dkxxhm;
    }

    @XmlElement(name = "DKXXKPRQ")
    public String getDkxxkprq() {
        return dkxxkprq;
    }

    public void setDkxxkprq(String dkxxkprq) {
        this.dkxxkprq = dkxxkprq;
    }

    @XmlElement(name = "DKXXPMJE")
    public Double getDkxxpmje() {
        return dkxxpmje;
    }

    public void setDkxxpmje(Double dkxxpmje) {
        this.dkxxpmje = dkxxpmje;
    }

    @Override
    public String toString() {
        return "YrbZlfCedkxxRequest{" +
                "dkxxxh=" + dkxxxh +
                ", dkxxfl='" + dkxxfl + '\'' +
                ", dkpjlx='" + dkpjlx + '\'' +
                ", dkxxdm='" + dkxxdm + '\'' +
                ", dkxxhm='" + dkxxhm + '\'' +
                ", dkxxkprq=" + dkxxkprq +
                ", dkxxpmje=" + dkxxpmje +
                '}';
    }
}
