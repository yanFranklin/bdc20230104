<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <title>登记页面</title>
    <link rel="stylesheet" href="../../font-awesome-4.7.0/css/font-awesome.css">
    <link rel="stylesheet" href="../../layui/css/layui.css">
    <link rel="stylesheet" href="../../bdcui/css/application.css">
    <link rel="stylesheet" href="../css/index.css">
</head>
<body>
    <div class="bdc-container-box">
        <div class="bdc-container">
            <!--收缩左侧列表-->
            <div class="bdc-aside-zoom">
                <a class="bdc-aside-close" href="javascript:;"><i class="fa fa-chevron-left" aria-hidden="true"></i></a>
                <a class="bdc-aside-open bdc-hide" href="javascript:;"><i class="fa fa-chevron-right" aria-hidden="true"></i></a>
            </div>

            <!--由最大化变为正常-->
            <div class="bdc-normal-btn bdc-hide">
                <button class="layui-btn layui-btn-primary bdc-btn-img bdc-btn-up"><img src="../image/left-arrow.png" alt=""></button>
                <button class="layui-btn layui-btn-primary bdc-btn-img bdc-btn-down"><img src="../image/right-arrow.png" alt=""></button>
                <button class="layui-btn layui-btn-primary bdc-btn-img bdc-btn-normal"><img src="../image/zoom.png" alt=""></button>
            </div>

            <!--左侧菜单栏开始-->
            <div class="bdc-menu-aside" id="asideBox">
                <div class="aside" id="asideCon">
                    <h4>目录</h4>
                    <div id="aside">
                        <div class="bdc-search-box">
                            <input type="text" name="title" placeholder="请输入" autocomplete="off" class="layui-input bdc-search">
                            <i class="layui-icon layui-icon-search"></i>
                            <i class="layui-icon layui-icon-close"></i>
                        </div>
                        <ul id="accordion" class="accordion">

                        </ul>
                    </div>
                </div>
            </div>
            <div id="asideLine"></div>
            <!--左侧菜单栏结束-->

            <!--右侧主内容开始-->
            <div class="bdc-right-content">
                <div class="bdc-title">
                    <p>宗地宗海号： 230103001006GB00007</p>
                    <div class="bdc-tools">
                        <button class="layui-btn layui-btn-primary bdc-btn-img bdc-btn-up"><img src="../image/left-arrow.png" alt=""></button>
                        <button class="layui-btn layui-btn-primary bdc-btn-img bdc-btn-down"><img src="../image/right-arrow.png" alt=""></button>
                        <button class="layui-btn layui-btn-primary bdc-btn-img bdc-btn-zoom"><img src="../image/zoom.png" alt=""></button>
                        <button class="layui-btn layui-btn-primary bdc-export">导出</button>
                    </div>
                </div>
                <!--内容存放开始-->
                <div class="content-div" id="contentView">
                    <!--<iframe id="childFrame" src="" scrolling="no"></iframe>-->
                    <iframe id="childFrame" src=""></iframe>
                </div>
                <!--内容存放结束-->
            </div>
            <!--右侧主内容结束-->
        </div>
    </div>

    <script type="text/html" id="djbQlTpl">
        <li class="bdc-outer-li bdc-invented-li" data-num="zdzhh00001">
            <a class="link" data-src="{{d.djbfmUrl}}">
                <img src="../image/file.png" alt="图标" class="docu-icon">
                登记簿封面
            </a>
        </li>
        <li class="bdc-outer-li bdc-invented-li" data-num="zdzhjbxx00001">
            <a class="link" data-src="{{d.zdzhxxUrl}}">
                <img src="../image/file.png" alt="图标" class="docu-icon">
                宗地宗海基本信息
            </a>
        </li>
        <li class="bdc-outer-li bdc-invented-li" data-num="0000000001">
            <a class="link" data-src="{{d.qlmlUrl}}">
                <img src="../image/file.png" alt="图标" class="docu-icon">
                不动产权利登记目录
            </a>
        </li>
        {{# layui.each(d.bdcDjbQlMlList, function(index, item){ }}
        <li class="bdc-outer-li bdc-invented-li" data-num="{{item.bdcdyh}}">
            {{#  if(item.childList.length != 0){ }}
            <a class="link" data-src="{{item.bdcdyUrl}}">
                <i class="fa fa-caret-right"></i>
                <img src="../image/file.png" alt="图标" class="docu-icon">
                {{item.bdcdyh}}
            </a>
            <ul class="submenu">
                {{# layui.each(item.childList, function(i, value){ }}
                <li class="bdc-inner-li bdc-invented-li" data-src="{{value.src}}">
                    <a href="javascript:;">
                        <img src="../image/log.png" alt="图标" class="docu-icon">{{value.name}}
                    </a>
                </li>
                {{# }); }}
            </ul>
            {{#  }else{ }}
            <a class="link" data-src="{{item.bdcdyUrl}}">
                <img src="../image/file.png" alt="图标" class="docu-icon">
                {{item.bdcdyh}}
            </a>
            {{#  } }}
        </li>
        {{# }); }}

    </script>
</body>
<script src="../../jquery/jquery.min.js"></script>
<script src="../../jquery-mousewheel/jquery.mousewheel.min.js"></script>
<script src="../../layui/layui.js"></script>
<script src="../js/index.js"></script>
</html>