<!DOCTYPE html>
<html lang="zn-cn">
<head>
    <meta charset="UTF-8">
    <title>第三方日志颜色配置页面</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../static/lib/bdcui/css/config-color.css" />
    <link rel="stylesheet" href="../static/css/dsfrzYspz.css">
</head>
<body>
<div class="bdc-container">
    <div class="bdc-top">
        <!--标题  开始-->
        <div class="bdc-title">
            <span>颜色配置</span>
            <div class="bdc-btn-box">
                <a href="javascript:;" class="layui-btn bdc-major-btn" id="saveColor">保存</a>
            </div>
        </div>
        <!--标题  结束-->

        <!--颜色选择区  开始-->
        <div class="bdc-color-module">
            <p class="bdc-config-title">系统名称</p>
            <div class="bdc-config-color clear">

            </div>
        </div>
        <!--颜色选择区  结束-->
    </div>

    <!--样例展示区  开始-->
    <div class="bdc-demo clear">
        <p class="bdc-demo-title">预览</p>
        <div class="bdc-demo-content">
            <div class="bdc-left">
                <!--饼图-->
                <div id="qqslYh"></div>
            </div>
            <div class="bdc-right">
                <!--折线图-->
                <div id="dyqsZxt"></div>
            </div>
        </div>
    </div>
    <!--样例展示区  结束-->
</div>
</body>
<script type="text/html" id="colorTpl">
    {{#  layui.each(d, function(index, item){ }}
    <div class="bdc-color-list">
        <div class="bdc-one-color {{item.name}}"></div>
        <p class="bdc-color-name">{{item.mc}}</p>
    </div>
    {{#  }); }}
</script>
<script src="../static/lib/layui/layui.js"></script>
<script src="../static/lib/echarts.js"></script>
<script src="../static/lib/jquery/jquery.min.js"></script>
<script src="../static/js/common.js"></script>
<script src="../static/js/mask.js"></script>
<script src="../static/js/dsfrzYspz.js"></script>
</html>