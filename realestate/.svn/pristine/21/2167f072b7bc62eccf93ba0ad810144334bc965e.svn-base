*/
@import "common.less";
body{
    background-color: @systemBg;
}
html,body{
    width: 100%;
    height: 100%;
}
.content-div{
    width: 100%;
    height: 100%;
    padding: 68px @paddingNum @paddingNum @paddingNum;
    box-sizing: border-box;
    .bdc-content-box{
        width: 100%;
        //height: 100%;
        min-height: 100%;
        overflow-x: auto;
        background-color: #fff;
    }
    form{
        //padding-top: 71px;
        background-color: #fff;
        width: 935px;
        padding: 0 @paddingNum;
        margin: 0 auto;
        box-sizing: border-box;
    }
}

.bdc-content-fix{
    width: 100%;
    height: 58px;
    background-color: #eaedf1;
    padding: 0 10px;
    box-sizing: border-box;
    position: fixed;
    top: 10px;
    left: 0;
    z-index: 999;
}
.content-title{
    padding: @paddingNum 0;
    border-bottom: 1px solid @borderBg;
    background-color: #fff;
    height: 58px;
    clear: both;
    top: @paddingNum;
    left: @paddingNum;
    z-index: 9;
    box-sizing: border-box;

    p{
      font-size: 16px;
      color: #333;
      float: left;
      margin-left: @paddingNum;
      line-height: 38px;
    }
    .title-btn-area{
      float: right;
      margin-right: @paddingNum;
    }
}

.content-main{
    width: 100%;
    //overflow: auto;
}
.table-name{
    width: 100%;
    height: 22px;
    margin: 0 auto;
    padding: 15px 0;
    text-align: center;
    font-size: 16px;
    color: #333;
    background-color: #fff;
}

.layui-table td{
    overflow: hidden;
}
table{
    position: relative !important;

}

.content-main table input[type='text']{
    display: block;
    border: none;
    outline: none;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
}
.content-main table .bdc-not-null input[type='text']{
    background-color: @notNullBg;
}
.layui-input, .layui-textarea{
    padding-left: 5px;
}
.content-main .layui-form-danger+.layui-form-select .layui-input,
.content-main .layui-form-danger:focus{
 border: 1px solid @warnColor!important;
}
.layui-form-danger+.layui-form-select,
.layui-form-danger.layui-input,
.layui-form-danger:focus,
.layui-form-danger{
    border: 1px solid @warnColor!important;
}

.bdc-table-box{
    width: 100%;
    //overflow: auto;
}
.layui-table{
    //max-width: 1200px;
    width: 100%;
    margin: 0 auto;
    margin-bottom: 20px;
}

.content-main .layui-table tbody tr:hover,
.content-main .layui-table thead tr,
.content-main .layui-table-click,
.content-main .layui-table-header,
.content-main .layui-table-hover,
.content-main .layui-table-mend,
.content-main .layui-table-patch,
.content-main .layui-table-tool,
.content-main .layui-table[lay-even] tr:nth-child(even) {
    background-color: transparent;
}
table.layui-table .quarantine{
    border-left-color:transparent !important;
    border-right-color:transparent !important;
}
table{
    border: 0;
}
tr,
td{
    color: @mainTextColor;
    textarea{
        color: @mainTextColor;
    }
}
.layui-table td,
.layui-table th,
.layui-table-col-set,
.layui-table-fixed-r,
.layui-table-grid-down,
.layui-table-header,
.layui-table-page,
.layui-table-tips-main,
.layui-table-tool,
.layui-table-total,
.layui-table-view,
.layui-table[lay-skin=line],
.layui-table[lay-skin=row]{
    border-color: @borderBg;
}
.layui-table td.set-back-color{
    background-color: @subColor;
    padding: 0 6px;
}
.layui-table td, .layui-table th{
    //padding: 0 10px;
    padding: 0;
    height: 39px;
    overflow: visible;
    position: static;
}
.layui-table-edit{
    box-shadow:none;
    position: relative;
    color: @mainTextColor;
    padding: 0 5px;
}
.required-span {
    color: @warnColor;
    margin-right: 6px;
}

td{
    text-align: right;
    textarea{
        display: block;
        /*position: absolute;
        left: 0;
        top: 0;*/
        width: 100%;
        height: 100%;
        border: none;
        outline: none;
        resize: none;
        padding-left: 5px;
        box-sizing: border-box;
        overflow: auto;
    }
    p{
        text-align: left;
    }
}
.set-center{
    text-align: center;
}
.set-tr-height{
    height:60px;
}
.set-tr-height158{
    height: 158px;
}

.statement{
    font-size: 14px;
    color: #333;
    line-height: 23px;
    text-indent: 2em;
    margin: 20px 10px;
    text-align: left;
}
.content-main table .gnature{
    width: 800px;
    text-align: left;
}

.content-main table .gnature input[type='text']{
    display: block;
    width: 182px;
    padding-left: 10px;
    height: 38px;
    line-height: 38px;
    background-color: #fff;
    border-radius: 2px;
    border:none;
}
.content-main table .setTimes .setTimes-float{
    float: left;
    margin-left: 90px;
}
.content-main table .setTimes .setTimes-float label{
    width: 18px !important;
    padding: 9px 0;
    margin: 0;
    text-align: left;
}
.content-main table .setTimes .setTimes-float input,
.content-main table .setTimes .setTimes-float label{
    float: left;
}
.content-main table  .setTimes input[type='text']{
    max-width: 38px;
    border: none;
    text-align: center;
}
.change-label-width {
    width: 138px !important;
    padding: 9px 8px;
    font-size: 14px;
}
input::-webkit-input-placeholder {
    color: @placeholderTextColor;
}

.layui-form-select dl dd.layui-this{
    background-color: @themeColor;
}
.layui-select-title{
    height: 38px;
}
.layui-form-select{
    text-align: left;
}.layui-form-select .layui-input{
    color: @mainTextColor;
 }
 .bdc-set-width{
     width: 80px;
     box-sizing: border-box;
 }
.layui-input:hover, .layui-textarea:hover {
    border-color: #D2D2D2!important;
}
input:disabled {
    //color: #666;
    background-color: #fff;
}
.bdc-remark{
    text-indent: 2em;
}
.bdc-td-box{
    position: relative;
    height: 100%;
}
td .bdc-td-box>img {
    position: absolute;
    top: 50%;
    right: 10px;
    transform: translateY(-50%);
    color: @warnColor;
    font-size: 18px;
}
.layui-disabled, .layui-disabled:hover {
    color: #333!important;
    cursor: text!important;
}
.layui-laydate .layui-laydate-content .layui-this{
    background-color: @themeColor !important;
}

//不可使用右侧留白
.bdc-td-box.bdc-one-icon input,
.bdc-td-box.bdc-date-disabled input{
    padding-right: 26px;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
}
.bdc-td-box.bdc-one-icon textarea{
    padding-right: 26px;
}
.bdc-td-box.bdc-two-icon input{
    padding-right: 46px;
}
//单位
.bdc-td-box>span{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 6px;
}
.bdc-two-icon>span{
    right: 26px;
}
textarea:disabled {
    background-color: #fff;
}


//可查询单选下拉
.xm-select-parent .xm-select{
    min-height: 38px;
    //line-height: 34px;
}
.xm-select-parent .xm-select-title{
    min-height: 38px;
    height: 38px;
}
.bdc-radio .xm-select-parent .xm-select .xm-select-label{
    width: 100%;
    box-sizing: border-box;
}
.bdc-radio div[xm-select-skin] .xm-select-title div.xm-select-label>span{
    border-color: transparent;
    width: 100%;
    box-sizing: border-box;
    padding-left: 0;
}
.bdc-radio .xm-select-parent .xm-select-title div.xm-select-label>span{
    background-color: #fff;
    color: #333;
}
.bdc-radio .xm-select-parent .xm-select-title div.xm-select-label>span i{
    top: 0;
    right: 30px;
}
.bdc-radio .xm-form-checkbox > i,
div[xm-select-skin=default] dl dd:not(.xm-dis-disabled) i{
    border-color: @themeColor;
}
.bdc-radio .xm-select-parent dl dd.xm-select-this div i,
div[xm-select-skin=default] dl dd.xm-select-this:not(.xm-dis-disabled) i{
    color: @themeColor;
}
.bdc-radio .xm-form-checkbox:hover > i{
    border-color: @themeColor;
}

.xm-select-parent dd > .xm-cz-group{
    margin-right: 0;
    border-right: none;
}
.xm-select-parent dd > .xm-cz{
    display: none;
}
div[xm-select-skin=default] .xm-select-title div.xm-select-label>span{
    color: #333;
    background-color: #eaedf1;
}
.xm-form-selected .xm-select, .xm-form-selected .xm-select:hover{
    border-color: #1d87d1!important;
}
.xm-select-parent .xm-input{
    border-color: transparent;
}
.xm-select-parent .xm-select:hover{
    border-color: transparent;
}
div[xm-select-skin=default].xm-form-selected .xm-select, div[xm-select-skin=default].xm-form-selected .xm-select:hover{
    border-color: transparent!important;
}
.bdc-not-null .xm-select-parent .xm-select{
    background-color: @notNullBg;
    border-color: @notNullBg;
}
.bdc-radio.bdc-not-null .xm-select-parent .xm-select-title div.xm-select-label > span{
    background-color: @notNullBg;
}
.content-main table .bdc-not-null .xm-select-tips .xm-input{
    background-color: #fff;
}
.xm-select--suffix{
    height: 38px;
}

.xm-select-dis .xm-select-sj{
    display: none;
}
.xm-select-dis .xm-select--suffix+div{
    cursor: text!important;
}

//单选的清空隐藏
.xm-select-parent .xm-form-select dl.xm-select-radio .xm-select-tips:first-child{
    display: none;
}
.xm-select-radio div.xm-form-checkbox > i{
    display: none;
}
.bdc-radio .xm-form-checkbox > span{
    padding: 0;
}

/*IE下输入文字后的×*/
::-ms-clear{display: none;}
::-ms-reveal{display: none;}

//清空附记按钮
td .bdc-fj-box{
    width: 100%;
    height: 100%;
    position: relative;
    box-sizing: border-box;
}
td .bdc-fj-box textarea{
    padding-right: 55px;
}
.bdc-fj-box #clearFjBtn{
    position: absolute;
    top: 10px;
    right: 18px;
}
.bdc-fj-box .layui-btn-xs{
    line-height: 20px;
}

//重新生成 按钮
.set-tr-height100{
    height: 100px;
}
td .bdc-fj-reset {
    width: 100%;
    height: 100%;
    position: relative;
    padding-right: 28px;
    box-sizing: border-box;
}
.bdc-fj-reset .bdc-pj-popup {
    width: 28px;
    height: 100%;
    line-height: 17px;
    padding-top: 5px;
    position: absolute;
    top: 0;
    right: 0;
    border-left: 1px solid #d0d5da;
    background-color: #f3f4f6;
    box-sizing: border-box;
    text-align: center;
    color: #1d87d1;
    cursor: pointer;
}