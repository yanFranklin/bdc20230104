.org_select_show,.org_select_show2{
    display: inline-block;
    vertical-align:middle;
    margin-left: 20px;
    text-align: center;
    color: #333;
    margin-top: 0px;
    float: right;
}
.org_select_show:hover,.org_select_show2:hover{
    cursor: pointer;
}
.org_select_tree,.org_select_tree2{
    display: none;
    position: absolute;
    border: 1px solid #e6e6e6;
    z-index: 999;
    background-color: #ffffff;
    overflow: auto;
}
.org_select_name{
    margin-right: 5px;
    padding: 2px 5px;
    background-color: #f0f2f5;
    color: #909399;
}
.org_select_delete{
    background-color: #c0c4cc;
    margin-left: 8px;
    border-radius: 20px;
    color: #ffffff;
    font-size: 12px;
}
