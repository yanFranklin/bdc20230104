package cn.gtmap.realestate.portal.ui.core.dto;

import cn.gtmap.gtc.msg.domain.dto.MessageDto;

/**
 * @author <a href="mailto:huanghui@gtmap.cn">huanghui</a>
 * @version 1.0  2022/10/31
 * @description 消息返回
 */
public class MessageBackDto extends MessageDto {
    /**
     *消息生产者姓名
     */
    private String producerName;
    /**
     *是否有附件标识 0：无 1有
     */
    private int fjbs;

    public String getProducerName() {
        return producerName;
    }

    public void setProducerName(String producerName) {
        this.producerName = producerName;
    }

    public int getFjbs() {
        return fjbs;
    }

    public void setFjbs(int fjbs) {
        this.fjbs = fjbs;
    }
}
