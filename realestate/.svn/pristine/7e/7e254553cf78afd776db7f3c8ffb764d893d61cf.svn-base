        <sql>
            create table ZRZY_XM
            (
                XMID      VARCHAR2(32) not null,
                SLBH      VARCHAR2(50) not null,
                GZLSLID   VARCHAR2(32),
                ZRZYDJDYH VARCHAR2(15),
                DJDYMC    VARCHAR2(1000),
