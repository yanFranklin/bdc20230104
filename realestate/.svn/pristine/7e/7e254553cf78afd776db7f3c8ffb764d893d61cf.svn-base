                QSZT      NUMBER(1) default '0',
                QXDM      VARCHAR2(6),
                SLSJ      DATE,
                SLRID     VARCHAR2(32),
                SLR       VARCHAR2(50),
                JSSJ      DATE,
                DJJG      VARCHAR2(100),
                DJSJ      DATE,
                DBR       VARCHAR2(200),
                YCQZH     VARCHAR2(4000),
                GZLDYID   VARCHAR2(50),
                GZLDYMC   VARCHAR2(200),
