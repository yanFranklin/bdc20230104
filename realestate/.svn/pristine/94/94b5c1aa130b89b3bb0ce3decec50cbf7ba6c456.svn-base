        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.EmptyStringCustomConvert">
            <a>slbh</a>
            <b>head.orgid</b>
        </field>
        <field>
            <a>ddbh</a>
            <b>data.ddbh</b>
        </field>
        <field>
            <a>nwslbh</a>
            <b>data.nwslbh</b>
        </field>

    </mapping>
</mappings>
