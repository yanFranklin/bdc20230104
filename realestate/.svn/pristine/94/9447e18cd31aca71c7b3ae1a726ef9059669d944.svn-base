<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:ex="http://www.gtmap.cn/schema/exchange-bean"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.gtmap.cn/schema/exchange-bean http://www.gtmap.cn/schema/exchange-bean.xsd">

    <ex:bean id="yctb_delete_task">
        <ex:service-info request-type="hfyctbPostService">
            <ex:property name="logEventName" value="一窗通办 驳回网厅申请件"/>
            <ex:property name="requester" value="BDC"/>
            <ex:property name="responser" value="YCTB"/>
            <ex:property name="dsfFlag" value="YCTB"/>
            <ex:property name="url" value="${yctb.delete.task.url}"/>
            <ex:property name="method" value="${yctb.delete.task.method}"/>
        </ex:service-info>
        <ex:log dsfFlag="YCTB" logEventName="一窗通办 驳回网厅申请件" requester="YCTB" responser="BDC"/>
    </ex:bean>

    <ex:bean id="yctb_bjxnjg">
        <ex:service-info request-type="hfyctbPostService">
            <ex:property name="logEventName" value="一窗通办 办件效能监管"/>
            <ex:property name="requester" value="BDC"/>
            <ex:property name="responser" value="YCTB"/>
            <ex:property name="dsfFlag" value="YCTB"/>
            <ex:property name="url" value="${yctb.bjxnjg.url}"/>
            <ex:property name="method" value="${yctb.bjxnjg.method}"/>
        </ex:service-info>
        <ex:log dsfFlag="YCTB" logEventName="一窗通办 办件效能监管" requester="YCTB" responser="BDC"/>
    </ex:bean>

    <ex:bean id="yctb_addTaxInfo">
        <ex:service-info request-type="hfyctbPostService">
            <ex:property name="logEventName" value="一窗通办 添加税务明细和缴费明细"/>
            <ex:property name="requester" value="BDC"/>
            <ex:property name="responser" value="YCTB"/>
            <ex:property name="dsfFlag" value="YCTB"/>
            <ex:property name="url" value="${yctb.add.tax.info.url}"/>
            <ex:property name="method" value="${yctb.add.tax.info.method}"/>
        </ex:service-info>
        <ex:log dsfFlag="YCTB" logEventName="一窗通办 添加税务明细和缴费明细" requester="BDC" responser="YCTB"/>
    </ex:bean>


</beans>