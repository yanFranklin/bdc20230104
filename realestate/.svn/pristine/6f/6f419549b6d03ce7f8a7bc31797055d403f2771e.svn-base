/*2019/4/4  购物车新增以及修改样式*/
.third-all-content-container-condition .layui-form-item>input[type='text']{
    height: 32px;
    color: #333;
    position: relative;
    border: 1px solid #d0d5da;
    top: -1px;
    width: 40px;
    text-align: center;
}

#confirm{
    background: #1d87d1;
    color: #fff;
    border-radius: 2px;
    border: none;
    margin-left: 10px;
}

#create{
    background: #f24b43;
    color: #fff;
    border-radius: 2px;
    border: none;
    margin-left: 10px;
}
.new-main-content-container{
    width: 295px;
    /* box-shadow: #ddd 0px 0px 2px 1px; */
    /* border: 1px solid #ddd; */
    border-right: 1px solid #ddd;
}

.new-main-content-item-title-div{
    width: 285px!important;
    padding-right: 0px!important;
    margin-left: 0px!important;
    margin-bottom: 10px;
}

.new-main-content-item-content{
    height: 100px;
    line-height: 25px;
}

.new-main-content-item-content-tanchu{
    top: 23px;
    left: 15px;
}

.new-qlrsjly{
    padding-left: 0px!important;
    width: 100%;
}

.new-qllx, .new-ywrsjly ,.new-sffz{
    width: 100%;
}

.main-content-container-right{
    position: absolute;
    padding: 15px;
    top: 58px;
    right: 0px;
    width: 540px;
    height: 450px;
    /*max-height: 450px;*/
    overflow: auto;
    background: #F3F4F6;
    display: none;
}

.main-content-container-right-item{
    height: 175px;
    width: 100%;
    text-align: left;
    /* margin-bottom: 14px; */
    border: 1px solid #d0d5da;
    background: #fff;
    /* padding-bottom: 10px; */
    margin-bottom: 10px;
    /*padding: 5px;*/
    box-sizing: border-box;
    max-height: 250px;
    overflow: auto;
}

.main-content-container-right-item:last-child{
    margin-bottom: 0px!important;
}

.drag-disabled{
    display: none;
    /*background: #f3f4f6;*/
}

.reverse-drag{
    font-size: 12px;
    padding: 5px 10px;
    margin: 0px 5px;
    /* box-shadow: #ddd 0px 0px 2px 1px; */
    border-bottom: 1px solid #ddd;
    border-radius: 2px;
    color: #333;
    /* margin-bottom: 0px; */
}

.reverse-drag:hover{
    box-shadow: #ddd 0px 0px 1px 1px;
    border: 1px solid #ddd;
}

.drag-disabled  .main-content-item-title .todrag>div{
    color: #999999;
}

.drag-disabled  .main-content-item-title>div:nth-child(2)>input:nth-child(2){
    color: #999999;
}

.drag-disabled  .main-content-item-title>div:nth-child(2)>input:nth-child(1){
    color: rgba(255,255,255,.7);
    border: none;
    background: rgba(242,75,67,.7);
}

.drag-disabled .main-content-item-title>div:nth-child(4) input:nth-child(2) {
    padding: 2px 4px;
    font-size: 12px;
    color: #999999;
}

#addZhengshu{
    background: #1d87d1;
    color: #fff;
    border-radius: 2px;
    border: none;
    padding: 7px;
    margin-top: 10px;
}

.main-content-container-right-item-title{
    background: #eaedf1;
    height: 35px;
    line-height: 35px;
    padding: 0px 15px;
    margin-bottom: 5px;
}

.deleteZhengshu{
    background: #f24b43;
    color: #fff;
    border-radius: 2px;
    border: none;
    padding: 3px 7px;
    font-size: 12px;
    float: right;
    position: relative;
    top: 6.3px;
}

.layui-form-item .layui-inline{
    float: none;
    display: inline-block;
}


/*用于调整下拉列表宽度*/
.layui-form-select dl {
    max-height: 200px;
}

.layui-colla-item {
    border: 0px;
}

.layui-collapse {
    border: 0px;
    margin-top: 10px;
}

.layui-colla-content{
    border-bottom: 1px;
}

.layui-colla-content {
    padding: 0px;
}
.layui-colla-title{
    top:0px !important;
    left:0px !important;
}

