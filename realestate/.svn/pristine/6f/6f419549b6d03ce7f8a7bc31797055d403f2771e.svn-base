.main-content-container{
    text-align: left;
    padding: 15px;
    height: 450px;
    overflow: auto;
}

.layui-form .layui-form-item .layui-inline{
    margin-top: -5px;
    width: 160px;
    margin-left: 10px;
}

.layui-form .layui-form-item .layui-inline .layui-select-title input{
    height: 34px;
}

.main-content-container>div.main-content-container-toptitle span{
    display: inline-block;
    box-sizing: border-box;
    padding-right: 15px;
    border-right: 1px solid #8c8c8c;
    color: #666666;
    margin-left: 15px;
    height: 8px;
    line-height: 8px;
}

.main-content-container>div.main-content-container-toptitle span:first-child{
    width: 300px;
}

.main-content-container>div.main-content-container-toptitle span:nth-child(2){
    width: 210px;
}

.main-content-container>div.main-content-container-toptitle span:nth-child(3){
    width: 210px;
}

.main-content-container>div.main-content-container-toptitle span:nth-child(4){
    border-right: none;
}

.main-content-item{
    margin-top: 15px;
    border-bottom: 1px dotted #d0d5da;
    padding-bottom: 10px;
}

.main-content-item .main-content-item-title>div{
    display: inline-block;
    box-sizing: border-box;
    padding-right: 15px;
    /*border-right: 1px solid #8c8c8c;*/
    color: #333;
    margin-left: 15px;
    vertical-align: text-top;
    font-size: 12px;
    cursor: pointer;
}

.main-content-item .main-content-item-title>div.todrag{
    padding-right: 0px;
    margin-left: 0px;
}

.main-content-item .main-content-item-title .todrag>div{
    display: inline-block;
    box-sizing: border-box;
    padding-right: 15px;
    /*border-right: 1px solid #8c8c8c;*/
    color: #333;
    margin-left: 15px;
    vertical-align: text-top;
    font-size: 12px;
    cursor: pointer;
}

.main-content-item .main-content-item-title .todrag>div:first-child{
    width: 285px;
}

.main-content-item .main-content-item-title .todrag>div:nth-child(2){
    width: 210px;
}

.main-content-item .main-content-item-title .todrag>div:nth-child(3){
    width: 210px;
}

.main-content-item .main-content-item-title>div:nth-child(2){
    padding-right: 0;
}

.main-content-item .main-content-item-title>div:nth-child(2) input{
    padding: 2px 4px;
    font-size: 12px;
}

.main-content-delete{
    border: 1px solid #f24b43;
    color: #fff;
    border-radius: 2px;
    background: #f24b43;
}

.main-content-open{
    background: #f3f4f6;
    border-radius: 2px;
    border: 1px solid #d0d5da;
}

.main-content-close{
    background: #f3f4f6;
    border-radius: 2px;
    border: 1px solid #d0d5da;
}

.main-content-item-content{
    width: 100%;
    background: #f3f4f6;
    height: 70px;
    line-height: 70px;
    color: #1d87d1;
    font-size: 12px;
    margin-top: 15px;
    display: none;
}

.main-content-item-content>div{
    display: inline-block;
}

.qlrsjly,.ywrsjly{
    width: 200px;
    cursor: pointer;
    position: relative;
}

.qlrsjly{
    padding-left: 6px;
}

.qllx{
    width: 330px;
    cursor: pointer;
    position: relative;
}

.sffz{
    width: 100px;
    cursor: pointer;
    position: relative;
}

.main-content-item-content img{
    width: 13px;
    height: 13px;
}

.main-content-item-content-tanchu{
    position: absolute;
    background: #fff;
    color: #333;
    padding: 10px 15px;
    border: 1px solid #d0d5da;
    left: -15px;
    top: 55px;
    min-width: 170px;
    display: none;
    z-index: 9999;
}

/*.sffz .main-content-item-content-tanchu{*/
    /*left: -65px;*/
/*}*/

.main-content-item-content-tanchu-title{
    height: 40px;
    line-height: 40px;
    font-size: 14px;
}

.main-content-item-content-tanchu form div.layui-form-item:nth-child(2){
    text-align: center;
    line-height: 50px;
}

.main-content-item-content-tanchu .layui-input{
    height: 30px;
}

.main-content-item-content-tanchu input[type='button']{
    padding: 3px 8px;
}

.main-content-item-content-tanchu input[type='button']:first-child{
    background: #1d87d1;
    color: #fff;
    border-radius: 2px;
    border: none;
    /*margin-left: 10px;*/
}

.main-content-item-content-tanchu input[type='button']:nth-child(2){
    background: #f3f4f6;
    border-radius: 2px;
    border: 1px solid #d0d5da;
}

.modal-tanchu{
    width: 840px;
    position: absolute;
    left: 60px;
    top:180px;
    background: #fff;
    display: none;
    border: 1px solid #d0d5da;
}

.modal-tanchu-title{
    width: 100%;
    height: 45px;
    /*line-height: 60px;*/
    font-size: 16px;
    padding: 12px;
    box-sizing: border-box;
    background: #f3f4f6;
    border-bottom: 1px solid #d0d5da;
}

.modal-tanchu-title img{
    float: right;
    background: #f3f4f6;
    height: 18px;
}

.modal-tanchu-content span{
    /* width: 32%; */
    font-size: 12px;
    padding: 27px 20px;
    /* margin: 5px 0px; */
    display: inline-block;
}

.modal-tanchu-content span input{
    width: 12px;
}

.modal-tanchu-content-button{
    width: 100%;
    text-align: center;
}

.modal-tanchu-content-button input{
    padding: 10px 15px;
}

.modal-tanchu-content-button input:first-child{
    background: #1d87d1;
    color: #fff;
    border-radius: 2px;
    border: none;
    margin-right: 5px;
}

.modal-tanchu-content-button input:nth-child(2){
    background: #f3f4f6;
    border-radius: 2px;
    border: 1px solid #d0d5da;
}

.modal-tanchu-beizhu{
    padding: 15px 50px;
}

.modal-tanchu-beizhu-content{
    height: 50px;
    padding-top: 10px;
    padding-left: 20px;
    background: #eaf4fe;
    border-left: 8px solid #0087ca;
}



.layui-form-label {
    float: left;
    display: block;
    padding: 9px 5px;
    width: 90px;
    font-weight: 400;
    line-height: 20px;
    text-align: right;
}

.layui-form-item .layui-input-inline {
    width: 180px;
}
.search{
    margin-bottom: 15px;
    border-bottom: 1px dotted #d0d5da;
    overflow: hidden;
    display: none;
}

.search-jt{
    cursor: pointer;
}

.search form{
    margin-bottom: 15px;
}

/*.search form{*/
    /*display: none;*/
/*}*/

.search input{
    padding: 7px 15px;
}

.search input[type='button']:first-child{
    background: #1d87d1;
    color: #fff;
    border-radius: 2px;
    border: none;
    margin-right: 5px;
}

.search input[type='button']:nth-child(2){
    background: #f3f4f6;
    border-radius: 2px;
    border: 1px solid #d0d5da;
}

.search .layui-form-item:nth-child(2){
    margin-top: 15px;
}
