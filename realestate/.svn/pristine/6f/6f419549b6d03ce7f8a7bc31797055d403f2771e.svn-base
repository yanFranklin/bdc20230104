body{
    width: 960px;
}

input[type='button']{
    cursor: pointer;
}

.all-container{
    position: relative;
}

.all-title{
    height: 60px;
    width: 100%;
    background: #fff;
    line-height: 60px;
    font-size: 18px;
    padding-left: 15px;
    box-sizing: border-box;
}

.all-content-container{
    padding: 15px;
    background: #eaedf1;
    text-align: center;
    box-sizing: border-box;
}

.second-all-content-container{
    background: #fff;
    padding: 15px;
    /*padding-bottom: 120px;*/
}

