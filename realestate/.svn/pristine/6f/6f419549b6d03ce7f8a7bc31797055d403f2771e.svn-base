.third-all-content-container{
    border: 1px solid #d0d5da;
    position: relative;
    /*margin-bottom: 15px;*/
}

.third-all-content-container-condition{
    height: 57px;
    background: #f3f4f6;
    border-bottom: 1px solid #d0d5da;
    text-align: left;
    line-height: 57px;
    padding: 0px 15px;
}

.third-all-content-container-condition input[type="button"]{
    padding: 7px;
    border-radius: 2px;
}

#expandAll{
    border: 1px solid #d0d5da;
    background: transparent;
    margin-right: 10px;
}

#initialization{
    background: #1d87d1;
    color: #fff;
    border-radius: 2px;
    border: none;
    margin-left: 10px;
}

#openSearch{
    background: #1d87d1;
    color: #fff;
    border-radius: 2px;
    border: none;
    margin-left: 10px;
}


