@charset "utf-8";

/* CSS Document */
* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

*,
body {
  padding: 0px;
  margin: 0px;
  color: #222;
  font-family: '微软雅黑';
}

@font-face {
  font-family: electronicFont;
  src: url(../../fonts/DS-DIGIT.TTF);
}

body {
  background: #000d4a url(../../image/jsc/bg.png) center top;
  background-size: cover;
  color: #666;
  font-size: 0.1rem;
}

li {
  list-style-type: none;
}

table {
}

i {
  margin: 0px;
  padding: 0px;
  text-indent: 0px;
}

img {
  border: none;
  max-width: 100%;
}

a {
  text-decoration: none;
  color: #399bff;
}

a.active,
a:focus {
  outline: none !important;
  text-decoration: none;
}

ol,
ul,
p,
h1,
h2,
h3,
h4,
h5,
h6 {
  padding: 0;
  margin: 0;
}

a:hover {
  color: #06c;
  text-decoration: none !important;
}

.pulll_left {
  float: left;
}

.pulll_right {
  float: right;
}

/*谷哥滚动条样式*/

::-webkit-scrollbar {
  width: 5px;
  height: 5px;
  position: absolute;
}

::-webkit-scrollbar-thumb {
  background-color: #5bc0de;
}

::-webkit-scrollbar-track {
  background-color: #ddd;
}

/***/

.allnav {
  height: calc(100% - 0.34375rem);
  padding: 0 0.2rem;
  background-color: rgba(5, 49, 129, 0.4);
  border-top: 1px solid rgba(79, 169, 211, 0.4);
}

.statistic-financ {
  height: 35%;
  width: 100%;
  padding-top: 0.15625rem;
}

.statistic-items {
  display: inline-block;
  width: 49%;
  padding: 0.1rem 0.1rem;
  position: relative;
}

.statistic-item {
  position: absolute;
  color: white;
  top: 0;
  left: 0.6rem;
  font-size: 0.140625rem;
}

.statistic-item span {
  color: #43f9fb;
  font-size: 0.171875rem;
  font-weight: bold;
}

.statistic-trend {
  width: 90%;
  margin: 0 auto;
  height: 0.234375rem;
  line-height: 0.234375rem;
  bottom: 0;
  color: white;
  font-size: 0.140625rem;
  padding-left: 0.15625rem;
  background-image: url(../../image/jsc/itemTitle-bg.png);
}

.chart-wrapper {
  height: 65%;
  width: 100%;
}

.provice {
  position: relative;
  height: 100%;
  width: 100%;
  font-size: 0;
  padding: 0.1rem 0;
}

.provice-items {
  display: inline-block;
  width: 33%;
  text-align: center;
  font-size: 0.2rem;
  position: relative;
  top: 50%;
  transform: translate(0, -50%);
}

.provice::before {
  content: '';
  width: 100%;
  height: 2px;
  display: inline-block;
  position: absolute;
  vertical-align: middle;
  /* background: linear-gradient(
    to right,
    rgba(255, 255, 255, 0) 0% 5%,
    #3cdfe7 10% 90%,
    rgba(255, 255, 255, 0) 95% 100%
  ); */
  /* filter: progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr=red,endColorstr=red);
  -ms-filter: 'progid:DXImageTransform.Microsoft.gradient (GradientType=1, startColorstr=red, endColorstr=red)';
  background-image: linear-gradient(to right, red, red); */
  background-image: url(../../image/jsc/line2.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  left: 0;
  top: 0.5rem;
}

.provice-items img {
  width: 0.3rem;
  height: 0.3rem;
}

.provice-items div:nth-child(2) {
  color: #3cdfe7;
}

.provice-items div:nth-child(3) {
  color: white;
  font-size: 0.3rem;
}

.today-survey {
  /* font-size: 0; */
  position: relative;
  height: 100%;
  /* padding: 0.1rem 0; */
}

.survey-items {
  display: inline-block;
  width: calc(33% - 1em);
  height: 100%;
  top: -0.1rem;
  position: relative;
}

.survey-item {
  font-size: 0.25rem;
  display: inline-block;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
  width: calc(70% - 1em);
}

.survey-items img {
  top: 35%;
  position: relative;
}

.survey-item div:nth-child(1) {
  color: #3cdfe7;
  font-size: 0.2rem;
  text-align: center;
  line-height: 0.4rem;
}

.survey-item div:nth-child(3) {
  color: red;
  font-size: 0.2rem;
  text-align: center;
  line-height: 0.4rem;
}

.survey-item div:nth-child(2),
.survey-item div:nth-child(4) {
  color: white;
  font-size: 0.15rem;
  text-align: center;
}

.province-score {
  font-size: 0;
  height: 100%;
  padding: 0.3rem 0;
  position: relative;
}

.access-quality,
.registration-quality {
  display: inline-block;
  width: 47%;
  height: 100%;
  font-size: 0.18rem;
}

.access-quality > div,
.registration-quality > div {
  color: white;
  margin-left: 0.4rem;
  line-height: 0.35rem;
}

.score-item {
  height: calc(98% - 0.35rem);
  overflow: hidden;
  margin: 0 !important;
  font-size: 0;
}

.score-item > div {
  display: inline-block;
  vertical-align: top;
  height: 100%;
}

.score-item > div:nth-child(1) {
  width: 30%;
  color: white;
  font-size: 0.35rem;
  text-align: center;
  padding: 20px 0px;
  left: 0.15625rem;
  position: relative;
}

.score-item > div:nth-child(2) {
  width: 65%;
  font-size: 0.15rem;
  text-align: center;
  padding: 0.039062rem 0;
}

.score-item > div div {
  color: #43f9fb;
  height: 33%;
  vertical-align: middle;
  line-height: 0.2rem;
}

.access-quality {
  background-image: url(../../image/jsc/jieruzhiliang.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.registration-quality {
  background-image: url(../../image/jsc/dengbozhiliang.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  right: -0.3rem;
  position: relative;
}

.loading {
  position: fixed;
  left: 0;
  top: 0;
  font-size: 18px;
  z-index: 100000000;
  width: 100%;
  height: 100%;
  background: #1a1a1c;
  text-align: center;
}

.loadbox {
  position: absolute;
  width: 160px;
  height: 150px;
  color: #aaa;
  left: 50%;
  top: 50%;
  margin-top: -100px;
  margin-left: -75px;
}

.loadbox img {
  margin: 10px auto;
  display: block;
  width: 40px;
}

.head {
  height: 1.05rem;
  background: url(../../image/jsc/title.png) no-repeat center center;
  background-size: 100% 100%;
  position: relative;
  z-index: 100;
}

.head h1 {
  color: #fff;
  text-align: center;
  font-size: 0.4rem;
  line-height: 1.1rem;
}

.mainbox {
  padding: 0.1rem 0.1rem 0rem 0.1rem;
}

.mainbox > ul {
}

.mainbox > ul > li {
  float: left;
  padding: 0 0.1rem;
}

.mainbox > ul > li {
  width: 30%;
}

.mainbox > ul > li:nth-child(2) {
  width: 40%;
  padding: 0;
}

.boxall {
  border: 1px solid rgba(79, 169, 211, 0.4);
  background: rgba(255, 255, 255, 0.04) url(../../image/jsc/line.png);
  background-size: 100% auto;
  position: relative;
  margin-bottom: 0.15rem;
  z-index: 10;
}

.alltitle {
  font-size: 0.2rem;
  color: #fff;
  background-color: #053181;
  line-height: 0.34rem;
  padding: 0 0.1rem;
  height: 0.34375rem;
}

.alltitle .more {
  font-size: 0.1rem;
  line-height: 0.34rem;
  text-align: right;
  position: absolute;
  right: 10px;
  color: white;
  cursor: pointer;
}
.alltitle .more:hover {
  color: #06c;
}

.alltitle img {
  margin-right: 0.1rem;
}

.boxfoot {
  position: absolute;
  bottom: 0;
  width: 100%;
  left: 0;
}

.map {
  width: 100%;
  height: 6.55rem;
  margin-top: 0.75rem;
  /* transform: rotate3d(2, -1, 0, -29deg); */
}

.m3-item {
  display: inline-block;
  width: 48%;
  height: 45%;
  box-shadow: 0px 0px 15px #888888 inset;
}
.m3-item > div {
  display: inline-block;
  top: 45%;
  transform: translateY(-50%);
  position: relative;
}
.m3-item img {
  position: relative;
  top: 50%;
  transform: translateY(-50%);
  left: 0.3rem;
}
.m3-item > div div:nth-child(1) {
  font-size: 0.25rem;
  color: #3cdfe7;
  left: 0.6rem;
  position: relative;
}
.m3-item > div div:nth-child(2) {
  font-size: 0.15rem;
  color: white;
  left: 0.6rem;
  position: relative;
}
.m3-item:nth-child(2),
.m3-item:nth-child(4) {
  left: 0.25rem;
  position: relative;
}
.m3-item:nth-child(3),
.m3-item:nth-child(4) {
  top: 0.2rem;
  position: relative;
}

.bar {
  width: 39%;
  height: 1.25rem;
  position: absolute;
  padding: 0 0.25rem;
  z-index: 9;
  /* font-size: 0; */
}

.bar-items {
  display: inline-block;
  width: calc(25% - 1em);
  height: 100%;
  background-image: url(../../image/jsc/tuoyuandi.png);
  background-size: 50% 65%;
  background-repeat: no-repeat;
  background-position: 50% 70%;
}
.bar-items div {
  text-align: center;
}
.bar-items div:nth-child(1) {
  font-size: 0.15rem;
  color: white;
  line-height: 0.25rem;
  padding-top: 0.25rem;
}
.bar-items div:nth-child(2) {
  font-size: 0.1rem;
  color: white;
}

.goBar{
  position: absolute;
  line-height: 1.1rem;
  cursor: pointer;
  color: white;
  font-size: 0.36rem !important;
  right: 1rem;
}