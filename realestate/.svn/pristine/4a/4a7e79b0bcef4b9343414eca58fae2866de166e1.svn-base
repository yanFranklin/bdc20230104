<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>外业查看</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="../../static/lib/font-awesome-4.7.0/css/font-awesome.min.css" media="all">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../static/lib/slick-master/slick/slick.css" media="all">
    <!--下拉框需要-->
    <link rel="stylesheet" href="../../static/lib/form-select/formSelects-v4.css">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/form.css"/>
    <style>
        .bdc-form-div {
            padding: 10px;
            padding-top: 68px;
            height: 100%;
        }
    </style>
</head>
<body>
<div class="bdc-form-div bdc-not-full-bg layui-form" id="jsztck">
    <!--全局 标题和操作按钮 开始-->
</div>
</body>
<script type="text/html" id="jsztTpl">
    <div class="bdc-content-fix">
        <div class="content-title layui-clear">
            <p>外业查看</p>
            <div class="title-btn-area">
                <button class="layui-btn bdc-major-btn print-btn" lay-filter="saveFwjsztCkxx" type="button"
                        lay-submit="" id="saveFwjsztCkxx">保存
                </button>
            </div>
        </div>
    </div>
    <form class="layui-form setOverflow" action="">
        <div class="form-margin-area">
            <div class="basic-info">
                {{# var fwjsztck = d.fwjsztck.bdcFwjsztckDO}}
                <div class="title-sign"><p><a id="" href="javascript:;">外业查看</a></p></div>
                <input type="text" name="wyckid" value="{{fwjsztck.wyckid || ''}}" class="fwjszt bdc-hide">
                <div class="layui-form-item">
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">申请人</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input" id="sqr"
                                   value="{{d.fwjsztck.qlr || ''}}" title="{{d.fwjsztck.qlr || ''}}" disabled="off">
                        </div>
                    </div>
                </div>
                <div class="layui-form-item change-textarea-margin">
                    <label class="layui-form-label change-label-width">房屋坐落</label>
                    <div class="layui-input-inline">
                        <textarea placeholder="请输入内容" class="layui-textarea change-textarea-width fwjszt" id="fwzl"
                                  name="fwzl" disabled="off" title="{{d.fwjsztck.zl || ''}}">{{d.fwjsztck.zl || ''}}</textarea>
                    </div>
                </div>
                <div class="layui-form-item">
                    <div class="layui-inline xzq-sele">
                        <label class="layui-form-label change-label-width">房屋现状</label>
                        <div class="layui-input-inline select-height">
                            <select lay-search="" lay-filter="fwjszt" id="fwjszt" class="fwjszt" name="fwjszt" title="{{fwjsztck.fwjszt || ''}}">
                                <option value="">请选择</option>
                                {{# layui.each(d.zd.fwjszt, function(index, item){ }}
                                <option value="{{item.DM || '0'}}" {{# if(fwjsztck.fwjszt==item.DM ){ }}
                                        selected="selected" {{# } }}>{{item.MC || ''}}
                                </option>
                                {{# }); }}
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline xzq-sele">
                        <label class="layui-form-label change-label-width">结构</label>
                        <div class="layui-input-inline select-height">
                            <select lay-search="" lay-filter="fwjg" id="fwjg" class="fwjszt" name="fwjg" title="{{fwjsztck.fwjg || ''}}">
                                <option value="">请选择</option>
                                {{# layui.each(d.zd.fwjg, function(index, item){ }}
                                <option value="{{item.DM || '0'}}" {{# if(fwjsztck.fwjg==item.DM ){ }}
                                        selected="selected" {{# } }}>{{item.MC || ''}}
                                </option>
                                {{# }); }}
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline xzq-sele bdc-two-column">
                        <label class="layui-form-label change-label-width">资料核实</label>
                        <div class="layui-input-inline select-height">
                            <select lay-search="" lay-filter="zlhs" id="zlhs" class="fwjszt" name="zlhs" title="{{fwjsztck.zlhs || ''}}">
                                <option value="">请选择</option>
                                {{# layui.each(d.zd.zlhs, function(index, item){ }}
                                <option value="{{item.DM || '0'}}" {{# if(fwjsztck.zlhs==item.DM ){ }}
                                        selected="selected" {{# } }}>{{item.MC || ''}}
                                </option>
                                {{# }); }}
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">四至情况(东)</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input fwjszt" id="szqkd" name="d"
                                   value="{{fwjsztck.d || ''}}" title="{{fwjsztck.d || ''}}">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">四至情况(西)</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input fwjszt" id="szqkx" name="x"
                                   value="{{fwjsztck.x || ''}}" title="{{fwjsztck.x || ''}}">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">四至情况(南)</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input fwjszt" id="szqkn" name="n"
                                   value="{{fwjsztck.n || ''}}" title="{{fwjsztck.n || ''}}">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">四至情况(北)</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input fwjszt" id="szqkb" name="b"
                                   value="{{fwjsztck.b || ''}}" title="{{fwjsztck.b || ''}}">
                        </div>
                    </div>
                    {{#if(fwjsztck.dcr){ }}
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">外业调查人</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input fwjszt" id="wydcr" name="dcr"
                                   value="{{fwjsztck.dcr || ''}}" title="{{fwjsztck.dcr || ''}}">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">外业调查日期</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input render-date test-item fwjszt" id="wydcrq" name="dcsj"
                                   value="{{fwjsztck.dcsj || ''}}" title="{{fwjsztck.dcsj || ''}}">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">权籍审核人</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input fwjszt" id="qjshr" name="qjshr"
                                   value="{{fwjsztck.qjshr || ''}}" title="{{fwjsztck.qjshr || ''}}">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">权籍审核日期</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input render-date test-item fwjszt" id="qjshrq"
                                   name="qjshsj" value="{{fwjsztck.qjshsj || ''}}" title="{{fwjsztck.qjshsj || ''}}">
                        </div>
                    </div>
                    {{# } }}
                </div>
                <div class="layui-form-item change-textarea-margin">
                    <label class="layui-form-label change-label-width">备注</label>
                    <div class="layui-input-inline">
                        <textarea placeholder="请输入内容" class="layui-textarea change-textarea-width fwjszt" id="bz"
                                  name="bz" title="{{fwjsztck.bz || ''}}">{{fwjsztck.bz || ''}}</textarea>
                    </div>
                </div>
            </div>
        </div>
    </form>
</script>
<script src="../../static/lib/layui/layui.js"></script>
<script src="../../static/lib/jquery/jquery.min.js"></script>
<script src="../../static/lib/bdcui/js/common.js"></script>
<script src="../../static/js/table.js"></script>
<script src="../../static/lib/bdcui/js/form-tab.js"></script>
<script src="../../static/js/common.js"></script>
<script src="../../static/js/slymCommon.js"></script>
<script src="../../static/js/formCenter.js"></script>
<script src="../../static/js/changzhou/slym/wyck.js"></script>
</html>