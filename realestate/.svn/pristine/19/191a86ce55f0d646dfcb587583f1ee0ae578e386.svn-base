body,
html {
    font-family: "Microsoft YaHei", Helvetica, Arial, sans-serif !important;
    font-size: 14px;
    background: #eaedf1;
}

.clear:after {
    content: '';
    display: block;
    clear: both;
}

.bdc-hide {
    display: none;
}

.bdc-major-btn {
    background-color: #1d87d1;
}

.bdc-major-btn:hover {
    background-color: #1675b8;
}

.bdc-secondary-btn {
    background-color: #f3f4f6;
    color: #666;
    border: 1px solid #d0d5da;
}

.bdc-secondary-btn:hover {
    color: #666;
}

.bdc-delete-btn {
    background-color: #f54743;
}

.bdc-table-second-btn {
    background-color: #fff;
    border: 1px solid #d0d5da;
    color: #333;
}

.bdc-table-second-btn:hover {
    color: #333;
}

.bdc-prohibit-btn {
    background-color: #fff;
    color: #999;
    border: 1px solid #d0d5da;
}

.bdc-prohibit-btn:hover {
    cursor: not-allowed;
    opacity: 1;
    color: #999;
}

.bdc-search-box {
    background-color: #fff;
}

.bdc-search-box .layui-form {
    width: 100%;
    padding-bottom: 10px;
    border-bottom: 1px dashed #d0d5da;
}

.layui-form-item {
    margin-bottom: 0;
}

.layui-form-item .layui-inline {
    width: 33%;
    margin-bottom: 0;
    margin-top: 10px;
    margin-right: 0;
}

.layui-inline .layui-form-label {
    width: 90px;
}

.layui-form-item .layui-input-inline {
    width: 60%;
}

.layui-form-select dl dd.layui-this {
    background-color: #1d87d1;
}

.layui-laydate .layui-laydate-content .layui-this {
    background-color: #1d87d1 !important;
}

.layui-form-item .layui-inline.bdc-button-box {
    width: 100%;
    text-align: center;
}

.layui-badge-rim,
.layui-inline .layui-colla-content,
.layui-inline .layui-colla-item,
.layui-inline .layui-collapse,
.layui-inline .layui-elem-field,
.layui-inline .layui-form-pane .layui-form-item[pane],
.layui-inline .layui-form-pane .layui-form-label,
.layui-inline .layui-input,
.layui-inline .layui-layedit,
.layui-inline .layui-layedit-tool,
.layui-inline .layui-quote-nm,
.layui-inline .layui-select,
.layui-inline .layui-tab-bar,
.layui-inline .layui-tab-card,
.layui-inline .layui-tab-title,
.layui-inline .layui-tab-title .layui-this:after,
.layui-inline .layui-textarea,
select {
    border-color: #d0d5da;
    color: #333;
}

/*placeholder 的字体颜色*/
input::-webkit-input-placeholder {
    color: #999;
}

.layui-input-inline .layui-select-disabled .layui-disabled {
    border-color: #d0d5da !important;
}

.easyui-combotree {
    display: none !important;
}

.tree-node-selected {
    background: #1d87d1;
    color: #fff;
}

.panel-body {
    color: #333;
}

.textbox {
    width: 190px;
    height: 38px;
    border-color: #d0d5da;
    border-radius: 0;
}

.textbox .textbox-text {
    font-size: 14px;
    outline: none;
    color: #333;
}

.combo-arrow {
    background-color: transparent;
}

.tree-node {
    height: 36px;
    position: relative;
    padding-left: 24px;
    box-sizing: border-box;
}

.tree-title {
    height: 36px;
    line-height: 36px;
    font-size: 14px;
}

.tree-collapsed,
.tree-checkbox {
    position: absolute;
    top: 50%;
    left: 4px;
    transform: translateY(-50%);
}

html,
body {
    height: 100%;
}

body {
    background-color: #eaedf1;
}

.layui-form-item .layui-inline.bdc-two-column {
    width: 64%;
}

.layui-form-item .bdc-two-column .layui-input-inline {
    width: 77%;
}

.bdc-form-div {
    width: 100%;
    height: 100%;
    padding: 68px 10px 10px;
    box-sizing: border-box;
    background-color: #eaedf1;
}

.bdc-form-not-full {
    height: 100%;
    padding-top: 68px;
}

.bdc-form-div .layui-form {
    height: 100%;
    box-sizing: border-box;
    background-color: #fff;
}

.basic-info {
    padding: 15px 10px;
    border-bottom: 1px dotted #d0d5da;
}

.basic-info:last-child {
    border-bottom: none;
}

.bdc-content-fix {
    width: 100%;
    height: 58px;
    background-color: #eaedf1;
    padding: 0 10px;
    box-sizing: border-box;
    position: fixed;
    top: 10px;
    left: 0;
    z-index: 999;
}

.content-title {
    padding: 10px 0;
    border-bottom: 1px solid #eaedf1;
    background-color: #fff;
    height: 58px;
    clear: both;
    top: 10px;
    left: 10px;
    z-index: 9;
    box-sizing: border-box;
}

.content-title p {
    font-size: 16px;
    color: #333;
    float: left;
    margin-left: 10px;
    line-height: 38px;
}

.content-title .title-btn-area {
    float: right;
    margin-right: 10px;
}

.layui-form-item .layui-inline {
    width: 32%;
}

.layui-form-item .layui-input-inline {
    width: 53%;
}

.form-margin-area {
    background-color: #fff;
}

.change-input-width90 {
    width: 25.5% !important;
}

.title-sign {
    width: 100%;
    height: 20px;
}

.title-sign p {
    font-size: 14px;
    color: #333;
    height: 16px;
    border-left: 4px solid #1d87d1;
    margin-left: 5px;
    padding-left: 10px;
    line-height: 16px;
}

.title-checkbox p {
    border-left-color: transparent;
}

.layui-form-item {
    margin-bottom: 0;
}

.change-textarea-margin .layui-input-inline {
    width: 81.5%;
}

.change-textarea-margin {
    margin-top: 10px;
}

.required-span {
    color: #f54743;
    margin-right: 6px;
}

.layui-form-checked[lay-skin=primary] i {
    border-color: #1d87d1;
    background-color: #1d87d1;
    color: #fff;
}

.layui-form-checkbox[lay-skin=primary]:hover i {
    border-color: #1d87d1;
    color: #fff;
}

.layui-input:focus,
.layui-textarea:focus {
    border-color: #1d87d1 !important;
}

.layui-disabled:focus {
    border-color: #d0d5da !important;
}

.layui-disabled,
.layui-disabled:hover {
    cursor: text !important;
    color: #333 !important;
}

.layui-form-radio > i:hover,
.layui-form-radioed > i {
    color: #1d87d1;
}

.layui-form-danger + .layui-form-select .layui-input,
.layui-form-danger:focus {
    border-color: #f54743 !important;
}

input:disabled {
    color: #666 !important;
}

.layui-inline .layui-form-label {
    height: 20px;
}

.layui-form-label {
    width: 90px;
}

.change-label-width {
    font-size: 14px;
}

.basic-info .bdc-label-newline {
    padding: 0 15px;
    height: 28px;
    line-height: 18px;
}

.bdc-nav-container {
    width: 100%;
    height: 50px;
    padding-right: 20px;
    overflow: hidden;
    position: fixed;
    top: 68px;
    left: 10px;
    box-sizing: border-box;
    z-index: 9;
}

.bdc-nav-container .bdc-nav-box {
    width: 100%;
    height: 50px;
    background-color: #eaf4fe;
    position: relative;
}

.bdc-nav-container .bdc-nav-box .layui-icon {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    font-size: 20px;
    color: #999;
}

.bdc-nav-container .bdc-nav-box .layui-icon-prev {
    left: 8px;
}

.bdc-nav-container .bdc-nav-box .layui-icon-next {
    right: 8px;
}

.bdc-nav-container .slick-slider {
    width: 100%;
    height: 50px;
    margin-bottom: 0;
    padding: 0 35px;
    box-sizing: border-box;
}

.bdc-nav-container .slick-prev {
    left: 8px;
}

.bdc-nav-container .slick-next {
    right: 8px;
}

.bdc-nav-container .slick a {
    display: inline-block;
    width: 100%;
    height: 50px;
    line-height: 50px;
    padding: 0 5px;
    border-bottom: 2px solid #eaf4fe;
    box-sizing: border-box;
    text-align: center;
}

.bdc-nav-container .slick a:hover {
    color: #1d87d1;
}

.bdc-nav-container .slick .bdc-this a {
    border-bottom-color: #1d87d1;
    color: #1d87d1;
}

.slick-prev,
.slick-next {
    opacity: 0;
    z-index: 999;
}

.layui-input-inline .fa-ban {
    position: absolute;
    top: 50%;
    right: 10px;
    transform: translateY(-50%);
    color: #f54743;
    font-size: 18px;
}
.bdc-checkbox-item{
    padding-left: 20px;
}
.bdc-checkbox-item .layui-form-checkbox[lay-skin=primary]{
    margin-bottom: 10px;
}
.bdc-textarea-item .layui-textarea{
    resize: none;
}

.layui-form-checkbox[lay-skin=primary] span{
    color: #333;
}
.basic-info-checkbox{
    border-bottom: none;
    padding-bottom: 0;
}