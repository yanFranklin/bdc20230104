<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>共享接口流程配置</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="../../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../../static/lib/form-select/formSelects-v4.css">
    <link rel="stylesheet" href="../../../static/lib/bdcui/css/table-hb.css"/>
    <link rel="stylesheet" href="../../../static/lib/bdcui/css/popup.css" />
    <link rel="stylesheet" href="../../../static/lib/bdcui/css/mask.css" />

    <link rel="stylesheet" href="../../../static/css/config/yml/pzym.css">
</head>
<body>
<div class="bdc-percentage-container bdc-line-search-container">
    <!--查询条件开始-->
    <div class="bdc-search-content">
        <div class="bdc-search-box">
            <form class="layui-form" lay-filter="searchFilter">
                <div class="layui-form-item pf-form-item">
                    <div class="layui-inline">
                        <label class="layui-form-label">流程名称</label>
                        <div class="layui-input-inline bdc-radio">
                            <select class="bdc-jglc bdc-lcmc" lay-filter="lcmcFilter" name="lcmc" xm-select="selectLcmc" xm-select-radio xm-select-search="" xm-select-height="34px">
                            </select>
                        </div>
                    </div>

                    <div class="layui-inline bdc-button-box">
                        <a href="javascript:;" class="layui-btn layui-btn-normal bdc-major-btn" id="search">查询</a>
                        <button type="reset" class="layui-btn layui-btn-normal bdc-secondary-btn" id="reset">重置</button>

                    </div>
                </div>
            </form>
        </div>
    </div>
    <!--查询条件结束-->
    <!--表格开始-->
    <div class="bdc-table-box">
        <table id="pzTable" lay-filter="pzFilter"></table>
    </div>
    <!--表格结束-->
</div>

<!--表格上面功能按钮-->
<script type="text/html" id="toolbarDemo">
    <div class="layui-btn-container">

    </div>
</script>

<!--表格内部操作按钮-->
<script type="text/html" id="barDemo">
    <a class="layui-btn layui-btn-xs bdc-major-btn" lay-event="edit">配置</a>
</script>
<div id="bdc-popup-small" class="bdc-hide">
    <form class="layui-form" action="">
        <div class="layui-form-item pf-form-item">
            <div class="layui-inline">
                <label class="layui-form-label"><span class="required-span"><sub>*</sub></span>流程名称</label>
                <div class="layui-input-inline">
                    <select class="bdc-lcmc" lay-filter="lcmcFilter" name="lcmc" xm-select="selectLcmcAdd" xm-select-skin="default" xm-select-radio xm-select-search="" xm-select-height="34px" id="lcadd">
                    </select>
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label"><span class="required-span"><sub>*</sub></span>配置说明</label>
                <div class="layui-input-inline bdc-end-time-box">
                    <textarea name="pzsm" id="pzsm" placeholder="请输入内容" class="layui-textarea"></textarea>
                </div>
            </div>
        </div>
    </form>
</div>

<script src="../../../static/lib/jquery/jquery.min.js"></script>
<script src="../../../static/lib/layui/layui.js"></script>
<!--添加水印-->
<script src="../../../static/lib/bdcui/js/common.js"></script>
<script src="../../../static/js/config/yml/jquery-form.js"></script>
<script src="../../../static/js/config/common.js"></script>
<script src="../../../static/js/config/gxjkpz/gxjklcpz.js"></script>
</body>
</html>