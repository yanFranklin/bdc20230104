package cn.gtmap.realestate.common.core.dto.exchange.sdqgh.dian.sqgh.response;


/**
 * @author <a href="mailto:caolu@gtmap.cn">caolu</a>
 * @version 1.0  2022/8/12
 * @description 水电气过户前核验结果data
 */
public class SdqHyDataDTO {
    /**
     * 户号
     */
    private String consno;

    /**
     * 是否限制过户的标志，0允许过户，其它值表示不允许过户
     */
    private String limitFlag;

    /**
     * 户主名称
     */
    private String hzmc;

    /**
     * 不可过户原因
     */
    private String reason;


    public String getConsno() {
        return consno;
    }

    public void setConsno(String consno) {
        this.consno = consno;
    }

    public String getLimitFlag() {
        return limitFlag;
    }

    public void setLimitFlag(String limitFlag) {
        this.limitFlag = limitFlag;
    }

    public String getHzmc() {
        return hzmc;
    }

    public void setHzmc(String hzmc) {
        this.hzmc = hzmc;
    }

    public String getReason() {
        return reason;
    }

    public void setReason(String reason) {
        this.reason = reason;
    }

    @Override
    public String toString() {
        return "SdqHyDataDTO{" +
                "consno='" + consno + '\'' +
                ", limitFlag='" + limitFlag + '\'' +
                ", hzmc='" + hzmc + '\'' +
                ", reason='" + reason + '\'' +
                '}';
    }
}
