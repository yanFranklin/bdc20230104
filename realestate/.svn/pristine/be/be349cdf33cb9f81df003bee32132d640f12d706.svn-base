html {
    background-color: #eaedf1;
}

.main-container {
    padding: 0 10px;
    min-width: 1000px;
    overflow: auto;
    position: relative;
    margin-top: 61px;
}

.data-row {
    padding: 20px 0;
    border-bottom: 1px dotted #eaedf1;
    background: #fff;
    clear: both;
    position: relative;
}

.data-row .row-title {
    height: 30px;
    position: relative;
    width: 100%;
    clear: both;
}

.data-row .row-title p {
    font-size: 16px;
    color: #333;
    height: 16px;
    border-left: 4px solid #1d87d1;
    margin-left: 15px;
    padding-left: 10px;
    line-height: 16px;
}

.data-row .row-title .row-operate {
    position: absolute;
    right: 15px;
    top: -13px;
}

.data-row .row-info {
    float: left;
    margin: 5px 0;
}

.data-row .table-row {
    margin: 0 15px;
    padding-bottom: 15px;
    position: relative;
}

.pannel-container {
    margin: 30px 0px 0;
}

.pannel-container .pannel-form-btns {
    margin: 30px 0;
    text-align: center;
}

.pannel-padding {
    padding: 0 15px;
}

