        FROM JBZK
        INNER JOIN QSZK on QSZK.ZRZYDJDYH = JBZK.ZRZYDJDYH
        INNER JOIN ZRZK on ZRZK.ZRZYDJDYH = JBZK.ZRZYDJDYH
