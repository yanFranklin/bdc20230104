    <!-- 分页查询子户室信息 -->
    <select id="listJbzkByPage" parameterType="map"
            resultType="cn.gtmap.realestate.common.core.domain.naturalresource.JbzkDO"
    >
        SELECT DCJG,DCSJ,ZRZYDJDYH,DJDYLX,MC,DYXXB,YSDM,ZL,DYSZB,DYSZD,DYSZN,
        DYSZX,ZRZYDJDCB,ZRZYDJDCBBH,ZRZYDJDYHZHSL,DJDYZMJ,GYMJ,JTMJ,ZYQMJ
        FROM JBZK
        <where>
            <if test="ysdm != null and ysdm != ''">
                AND JBZK.ysdm = #{ysdm}
            </if>
            <if test="zl != null and zl != ''">
                AND JBZK.zl = #{zl}
            </if>
            <if test="mc != null and mc != ''">
                AND JBZK.mc LIKE '%'|| #{mc}||'%'
            </if>
        </where>
    </select>

