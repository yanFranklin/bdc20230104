<body>
<div class="bdc-percentage-container">
    <div class="bdc-search-content">
        <div class="bdc-search-box">
            <form class="layui-form" lay-filter="searchForm">
                <div class="layui-form-item pf-form-item">
<!--                    <div class="layui-inline">-->
<!--                        <label class="layui-form-label"></label>-->
<!--                        <div class="layui-input-inline">-->
<!--                            <input type="text" autocomplete="off" id="slbhOrbdcdyh" name="slbhOrbdcdyh" placeholder="受理号或不动产单元号"-->
<!--                                   class="layui-input search">-->
<!--                            <i class="layui-icon layui-icon-close bdc-hide"></i>-->
<!--                        </div>-->
<!--                    </div>-->
                    <div class="layui-inline">
                        <label class="layui-form-label">受理号</label>
                        <div class="layui-input-inline">
                            <input class="layui-input cxtj search required" name="slbh" placeholder="请输入" type="text"/>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label">不动产单元号</label>
                        <div class="layui-input-inline">
                            <input class="layui-input cxtj search required" name="bdcdyh" placeholder="请输入" type="text"/>
                        </div>
                    </div>

                    <!--受理号、不动产单元号、登记类型、申请类型、办结时间、推送状态-->
                    <div class="layui-inline">
                        <label class="layui-form-label">登记类型</label>
                        <div class="layui-input-inline">
                            <select name="djlx" lay-search="" lay-filter="djlx" class="djlx cxtj required">
                                <option value="">请选择</option>
                            </select>
                            <i class="layui-icon layui-icon-close bdc-hide"></i>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label">申请类型</label>
                        <div class="layui-input-inline">
                            <select name="djxl" lay-search="" lay-filter="djxl" class="djxl cxtj required">
                                <option value="">请选择</option>
                            </select>
                            <i class="layui-icon layui-icon-close bdc-hide"></i>
                        </div>
                    </div>

                    <div class="layui-inline">
                        <label class="layui-form-label">推送状态</label>
                        <div class="layui-input-inline">
                            <select name="czzt" lay-search="" lay-filter="czzt" class="search required">
                                <option value="">请选择</option>
                                <option value="0">推送失败</option>
                                <option value="1">推送成功</option>
                            </select>
                        </div>
                    </div>
                    <div class="pf-senior-show bdc-hide">
                        <div class="layui-inline">
                            <label class="layui-form-label">登记起始时间</label>
                            <div class="layui-input-inline">
                                <input type="text" placeholder="请选择" id="kssj" name="kssj" autocomplete="off" class="layui-input search required">
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label">登记结束时间</label>
                            <div class="layui-input-inline">
                                <input type="text" placeholder="请选择" id="jssj" name="jssj" autocomplete="off" class="layui-input search required">
                            </div>
                        </div>
                    </div>
                    <div class="layui-inline bdc-button-box">
                        <button class="layui-btn bdc-major-btn" id="query" lay-filter="query" lay-submit="query">查询
                        </button>
                        <button class="layui-btn bdc-secondary-btn reset-btn" type="reset">重置</button>
                        <a href="javascript:;" class="layui-btn layui-btn-normal bdc-secondary-btn" id="seniorSearch">高级查询</a>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="bdc-table-box">
        <table id="gxtscxTable" lay-filter="dataTable"></table>
    </div>
</div>
<script type="text/html" id="DmMcTpl">
    {{# layui.each(d, function(index, zdItem){ }}
    <option value="{{zdItem.DM}}">{{zdItem.MC}}</option>
    {{# }); }}
</script>
<!--表格上面功能按钮-->
<script type="text/html" id="toolbarSqxx">
    <div class="layui-btn-container">
        <button class="layui-btn layui-btn-sm bdc-major-btn" lay-event="batchSharedb">推送</button>
    </div>
</script>
<script type="text/html" id="gxtslogListToolBarTmpl">
    <a class="layui-btn layui-btn-danger layui-btn-xs bdc-secondary-btn" lay-event="sharedb">推送</a>
</script>

<script src="../static/js/gxts/gxtscx.js"></script>
</body>
</html>


