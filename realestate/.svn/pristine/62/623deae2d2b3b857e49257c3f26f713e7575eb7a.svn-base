<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping type="one-way" wildcard="false">
        <class-a>com.alibaba.fastjson.JSONObject</class-a>
        <class-b>cn.gtmap.realestate.exchange.core.dto.nantong.gs.yzyyzz.GsYyzz</class-b>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="gs.syscode">
            <a>qrid</a>
            <b>message_header.syscode</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="gs.authcode">
            <a>qrid</a>
            <b>message_header.authcode</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.DefaultValueCustomConvert"
               custom-converter-param="005">
            <a>qrid</a>
            <b>message_header.businesstype</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="gs.sign">
            <a>qrid</a>
            <b>message_header.sign</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="gs.version">
            <a>qrid</a>
            <b>message_header.version</b>
        </field>

        <field>
            <a>qrid</a>
            <b>message_content.qrid</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="gs.rettype">
            <a>qrid</a>
            <b>message_content.rettype</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.NewDateCustomConverter"
               custom-converter-param="yyyyMMddHHmmss">
            <a>qrid</a>
            <b>message_content.opertime</b>
        </field>
    </mapping>

</mappings>
