@import "./common.less";
//复选框需要
.easyui-combotree{
    display: none !important;
}
.tree-node-selected{
    background: @themeColor;
    color: #fff;
}
.panel-body{
    color: @mainTextColor;
}
.textbox{
    width: 190px;
    height: 38px;
    border: none;
}
.textbox-focused{
    box-shadow: 0 0 0 0;
}
.textbox .textbox-text{
    font-size: 14px;
    outline: none;
    color: @mainTextColor;
}
.combo-arrow{
    background-color: transparent;
}
.tree-node{
    height: 36px;
    position: relative;
    padding-left: 24px;
    box-sizing: border-box;
}
.tree-title{
    height: 36px;
    line-height: 36px;
    font-size: 14px;
}
.tree-collapsed, .tree-checkbox{
    position: absolute;
    top: 50%;
    left: 4px;
    transform: translateY(-50%);
}