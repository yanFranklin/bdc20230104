<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>最高法-司法判决信息查询申请接口</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="../../static/lib/font-awesome-4.7.0/css/font-awesome.min.css" media="all">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/application.css">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css"/>
    <link rel="stylesheet" href="../../static/css/common.css"/>
</head>
<body>

<div class="content-div gxjklc-div">
    <div class="bdc-content-box">
        <form class="layui-form setOverflow" action="">
            <div class="bdc-content-fix">
                <div class="content-title">
                    <p>司法判决信息查询(最高法)</p>
                    <div class="title-btn-area .layui-layout-right">
                        <button type="reset" class="layui-btn bdc-secondary-btn upload-pdf">上传PDF</button>
                    </div>
                </div>
            </div>

            <div class="content-main">
                <div class="table-name"></div>

                <div class="bdc-table-box">
                    <table class="layui-table" border="1">
                        <colgroup>
                            <col width="50">
                            <col width="100">
                            <col width="150">
                            <col width="100">
                            <col width="150">
                        </colgroup>
                        <tbody>
                        <tr>
                            <td rowspan="1" class="set-back-color set-center">申请<br>信息</td>
                            <td class="set-back-color">案件编号</td>
                            <td colspan="2"><input type="text" class="layui-table-edit" name="ajbh" value=""></td>
                            <td class="set-center"><a href="javascript:;" class="layui-btn layui-btn-xs bdc-major-btn get-cxsq">查询申请</a></td>
                        </tr>
                        <tr>
                            <td colspan="5" class="quarantine"></td>
                        </tr>
                        </tbody>
                        <tbody id="jgxx">

                        </tbody>
                    </table>
                </div>
            </div>
        </form>
    </div>
</div>

<!--结果反馈表格-->
<script type="text/html" id="jgxxTpl">
    {{# if(d && d.length !==0){ }}
    <tr>
        <td rowspan="{{d.rowLength}}" class="set-back-color set-center">结<br>果<br>反<br>馈</td>
        <td class="set-back-color">案号</td>
        <td colspan="2"><input type="text" class="layui-table-edit" value="{{ d.ah || ''}}" disabled="off"></td>
        <td rowspan="3" class="set-center"><a href="javascript:;" class="layui-btn layui-btn-xs bdc-major-btn" id="wsnr">文书内容</a></td>
    </tr>
    <tr>
        <td class="set-back-color">格式化案号</td>
        <td colspan="2"><input type="text" class="layui-table-edit" value="{{ d.gshah || ''}}" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">实体码</td>
        <td colspan="2"><input type="text" class="layui-table-edit" value="{{ d.stm || ''}}" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color set-center">序号</td>
        <td class="set-back-color set-center">当事人姓名/企业名称</td>
        <td colspan="2" class="set-back-color set-center">证件号码/组织机构代码</td>
    </tr>
    {{# if(d.ajDsrDTOList && d.ajDsrDTOList.length>0){ }}
    {{# layui.each(d.ajDsrDTOList,function(i,v){ }}
    <tr>
        <td class="set-center">{{i+1}}</td>
        <td class="set-center">{{ v.mc || ''}}</td>
        <td colspan="2" class="set-center">{{ v.zjhm || ''}}</td>
    </tr>
    {{# }); }}
    {{# }else{ }}
    <tr>
        <td colspan="5" class="set-center">
            <div class="layui-none"><img src="../../static/lib/bdcui/images/table-none.png" alt="">无数据</div>
        </td>
    </tr>
    {{# } }}

    {{# }else{ }}
    <tr>
        <td colspan="2" class="set-back-color">结果反馈</td>
        <td colspan="3" class="set-center">
            <div class="layui-none"><img src="../../static/lib/bdcui/images/table-none.png" alt="">无数据</div>
        </td>
    </tr>
    {{# } }}
</script>

<script src="../../static/lib/jquery/jquery.min.js"></script>
<script src="../../static/lib/layui/layui.js"></script>
<script src="../../static/lib/bdcui/js/mask.js"></script>
<!--添加水印-->
<script src="../../static/lib/bdcui/js/common.js"></script>
<script src="../../static/js/common.js"></script>
<script src="../../static/js/gxjklc/gxjkCommon.js"></script>
<script src="../../static/js/gxjklc/lcbjSfpjxxsq.js"></script>
</body>
</html>