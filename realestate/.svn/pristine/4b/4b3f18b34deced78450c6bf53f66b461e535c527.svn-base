<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>宗海基本信息</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/application.css"/>
    <link rel="stylesheet" href="../../static/css/djbxx/bdcDjbZdjbxx.css"/>
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css">
    <script src="../../static/lib/jquery/jquery.min.js"></script>
    <script src="../../static/lib/layui/layui.all.js"></script>
    <!--添加水印-->
    <script src="../../static/lib/bdcui/js/common.js"></script>
    <script src="../../static/js/common.js"></script>
</head>
<body class="">
<div class="content-div" style="padding-top:0;">
    <div class="bdc-content-box">
        <form class="layui-form setOverflow" action="" lay-filter="form">
            <!--主体开始部分-->
            <div class="content-main common-form">
                <p class="bsc-table-tips">第<span>1</span>页</p>
                <div class="table-name">宗海基本信息</div>
                <table class="layui-table new-table" border="1">
                    <colgroup>
                        <col width="60">
                        <col width="60">
                        <col width="120">
                        <col width="195">
                        <col width="60">
                        <col width="135">
                        <col width="60">
                        <col width="210">
                    </colgroup>
                    <tbody id="mainBody">
                    <tr class="td-noborder">
                        <td colspan="6" class="">
                        <td class="" colspan="1"></td>
                        <td colspan="1" class="" id="dw">

                        </td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="2">不动产类型</td>
                        <td colspan="6" class="text-center">
                            <span><input type="checkbox" name="bdclx" value="5" lay-skin="primary" disabled="true" title="海域"></span>
                            <span><input type="checkbox" name="bdclx" value="6" lay-skin="primary" disabled="true" title="海域和构筑物"></span>
                            <span><input type="checkbox" name="bdclx" value="7" lay-skin="primary" disabled="true" title="其他"></span>
                            <span><input type="checkbox" name="bdclx" value="10" lay-skin="primary" disabled="true" title="海域和房屋"></span>
                            <span><input type="checkbox" name="bdclx" value="11" lay-skin="primary" disabled="true" title="海域和森林林木"></span>
                            <span><input type="checkbox" name="bdclx" value="12" lay-skin="primary" disabled="true" title="海域和其他"></span>
                            <span><input type="checkbox" name="bdclx" value="13" lay-skin="primary" disabled="true" title="无居民海岛"></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="2">项目名称</td>
                        <td colspan="3"><input type="text" class="layui-table-edit" name="xmmc" readonly></td>
                        <td class="set-back-color" colspan="1">项目性质</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="xmxz" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="2" rowspan="4" style="border-bottom: 0">
                            海&nbsp;&nbsp;&nbsp;&nbsp;域<br/><br/>状&nbsp;&nbsp;&nbsp;&nbsp;况
                        </td>
                        <td class="set-back-color" colspan="1">用海总面积</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="yhzmj" readonly></td>
                        <td class="set-back-color" colspan="1">宗海面积</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="zhmj" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="1">等&nbsp;&nbsp;&nbsp;&nbsp;别</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="db" readonly></td>
                        <td class="set-back-color" colspan="1">占用岸线</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="zhax" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="1">用海类型A</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="yhlxa" readonly></td>
                        <td class="set-back-color" colspan="1">用海类型B</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="yhlxb" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="1">用海位置说明</td>
                        <td colspan="5"><input type="text" class="layui-table-edit" name="yhwzsm" readonly></td>
                    </tr>
                    </tbody>
                    <!--用海状况动态加载-->

                    <!--无居民海岛状况-->
                    <tbody id="wjmhdqk">
                    <tr>
                        <td class="set-back-color" colspan="2" rowspan="4">无居民<br/><br/>海岛状况</td>
                        <td class="set-back-color" colspan="1">海岛名称</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="hdmc" readonly></td>
                        <td class="set-back-color" colspan="1">海岛代码</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="hddm" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="1">用岛范围</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="ydfw" readonly></td>
                        <td class="set-back-color" colspan="1">用岛面积</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="ydmj" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="1">海岛位置</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="hdwz" readonly></td>
                        <td class="set-back-color" colspan="1">用&nbsp;&nbsp;&nbsp;&nbsp;途</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="hdyt" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color text-center" colspan="8">用海、用岛坐标</td>
                    </tr>
                    <tr>
                        <td class="set-back-color text-center" colspan="1">序号</td>
                        <td class="set-back-color text-center" colspan="2">北纬</td>
                        <td class="set-back-color text-center" colspan="1">东经</td>
                        <td class="set-back-color text-center" colspan="1">序号</td>
                        <td class="set-back-color text-center" colspan="2">北纬</td>
                        <td class="set-back-color text-center" colspan="1">东经</td>
                    </tr>
                    </tbody>
                    <!--纬度，经度动态加载-->
                    <tbody id="dbxx">
                    <tr>
                        <td class="set-back-color" colspan="2">登记时间</td>
                        <td colspan="3"><input type="text" class="layui-table-edit" name="djsj" readonly></td>
                        <td class="set-back-color" colspan="1">登簿人</td>
                        <td colspan="2"><input type="text" class="layui-table-edit" name="dbr" readonly></td>
                    </tr>
                    <tr>
                        <td class="set-back-color" colspan="2">附&nbsp;&nbsp;记</td>
                        <td colspan="6"><textarea name="fj" class="text-center" style="height: 100px"></textarea></td>
                    </tr>
                    </tbody>
                    <!--变化情况，动态加载-->
                    <tbody>
                    </tbody>
                    <tbody id="imgbody">
                    <tr class="td-noborder">
                        <td colspan="8">
                        </td>
                    </tr>
                    <tr>
                        <td colspan="8" style="border-bottom: 0">
                            <p style="text-align: center"> 附 图
                                （宗海图，可附页）
                            </p>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="8" style="border-top: 0;height: 600px">
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </form>
    </div>
</div>
<!-- 插入用海、用岛坐标-->
<script type="text/html" id="yhydzb">
    <tbody>
    {{# if(d && d.count > 0){ }}
    {{# for(var i = 0, len = d.zblist.length,count = d.count; i < d.count; i++){ }}
    <tr>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit text-center" name="xh" value="{{i+1}}" readonly></td>
        <td class="text-center" colspan="2">
            <input type="text" class="layui-table-edit text-center" name="bw" value="{{d.zblist[i].bw}}" readonly>
        </td>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit text-center" name="dj" value="{{d.zblist[i].dj}}" readonly>
        </td>
        {{# if(i+count< len){ }}
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit text-center" name="xh" value="{{i+count+1}}" readonly></td>
        <td class="text-center" colspan="2">
            <input type="text" class="layui-table-edit text-center" name="bw" value="{{d.zblist[i+count].bw}}" readonly>
        </td>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit text-center" name="dj" value="{{d.zblist[i+count].dj}}" readonly>
        </td>
        {{# }else { }}
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit text-center" name="xh" readonly></td>
        <td class="text-center" colspan="2">
            <input type="text" class="layui-table-edit text-center" name="bw" readonly></td>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit text-center" name="dj" readonly></td>
        {{# } }}
    </tr>
    {{# } }}
    {{# } else { }}
    <tr>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit" name="xh" readonly></td>
        <td class="text-center" colspan="2">
            <input type="text" class="layui-table-edit" name="bw" readonly></td>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit" name="dj" readonly></td>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit" name="xh" readonly></td>
        <td class="text-center" colspan="2">
            <input type="text" class="layui-table-edit" name="bw" readonly></td>
        <td class="text-center" colspan="1">
            <input type="text" class="layui-table-edit" name="dj" readonly></td>
    </tr>
    {{# } }}
    </tbody>
</script>

<!--插入宗海变化情况模块-->
<script type="text/html" id="zhbhqk">
    <tbody>
    {{# if(d && d.length > 0){ }}
    <tr>
        <td class="set-back-color" colspan="2" rowspan="{{d.length+1 }}">变&nbsp;&nbsp;&nbsp;&nbsp;化<br/><br/>情&nbsp;&nbsp;&nbsp;&nbsp;况
        </td>
        <td class="set-back-color text-center" colspan="1">变化原因</td>
        <td class="set-back-color text-center" colspan="2">变化内容</td>
        <td class="set-back-color text-center" colspan="2">登记时间</td>
        <td class="set-back-color text-center" colspan="1">登簿人</td>
    </tr>
    {{# for(var i = 0, len = d.length; i < len; i++){ }}
    <tr>
        <td colspan="1">
            <input type="text" class="layui-table-edit text-center" name="bhyy" value="{{d[i].bhyy}}" readonly></td>
        <td colspan="2">
            <input type="text" class="layui-table-edit text-center" name="bhnr" value="{{d[i].bhnr}}" readonly></td>
        <td colspan="2">
            <input type="text" class="layui-table-edit text-center" name="djsj" value="{{d[i].djsj}}" readonly></td>
        <td colspan="1">
            <input type="text" class="layui-table-edit text-center" name="dbr" value="{{d[i].dbr}}" readonly>
        </td>
    </tr>
    {{# } }}
    {{# } else { }}
    <tr>
        <td class="set-back-color" colspan="2" rowspan="2">变&nbsp;&nbsp;&nbsp;&nbsp;化<br/><br/>情&nbsp;&nbsp;&nbsp;&nbsp;况
        </td>
        <td class="set-back-color text-center" colspan="1">变化原因</td>
        <td class="set-back-color text-center" colspan="2">变化内容</td>
        <td class="set-back-color text-center" colspan="2">登记时间</td>
        <td class="set-back-color text-center" colspan="1">登簿人</td>
    </tr>
    <tr>
        <td colspan="1">
            <input type="text" class="layui-table-edit text-center" name="bhyy" value="" readonly></td>
        <td colspan="2">
            <input type="text" class="layui-table-edit text-center" name="bhnr" value="" readonly></td>
        <td colspan="2">
            <input type="text" class="layui-table-edit text-center" name="djsj" value="" readonly></td>
        <td colspan="1">
            <input type="text" class="layui-table-edit text-center" name="dbr" value="" readonly>
        </td>
    </tr>
    {{# } }}
    </tbody>
</script>

<!--插入用海状况模块-->
<script type="text/html" id="yhzk">
    <tbody id="yhfsTpl">
    {{# if(d && d.length > 0){ }}
    <tr>
        <td class="set-back-color" colspan="2" rowspan="{{d.length+1 }}" style="border-top: 0"></td>
        <td class="set-back-color text-center" colspan="1">用海方式</td>
        <td class="set-back-color text-center" colspan="2">面积</td>
        <td class="set-back-color text-center" colspan="1">具体用途</td>
        <td class="set-back-color text-center" colspan="2">使用金数额</td>
    </tr>
    {{# for(var i = 0, len = d.length; i < len; i++){ }}
    <tr>
        <td colspan="1">
            <input type="text" class="layui-table-edit text-center" name="yhfs" value="{{d[i].yhfs}}" readonly></td>
        <td colspan="2">
            <input type="text" class="layui-table-edit text-center" name="yhmj" value="{{d[i].yhmj}}" readonly></td>
        <td colspan="1">
            <input type="text" class="layui-table-edit text-center" name="jtyt" value="{{d[i].jtyt}}" readonly></td>
        <td colspan="2">
            <input type="text" class="layui-table-edit text-center" name="syjse" value="{{d[i].syjse}}" readonly></td>
    </tr>
    {{# } }}
    {{# } else { }}
    <tr>
        <td class="set-back-color" colspan="2" rowspan="2" style="border-top: 0"></td>
        <td class="set-back-color text-center" colspan="1">用海方式</td>
        <td class="set-back-color text-center" colspan="2">面积</td>
        <td class="set-back-color text-center" colspan="1">具体用途</td>
        <td class="set-back-color text-center" colspan="2">使用金数额</td>
    </tr>
    <tr>
        <td colspan="1"><input type="text" class="layui-table-edit" name="yhfs" readonly></td>
        <td colspan="2"><input type="text" class="layui-table-edit" name="yhmj" readonly></td>
        <td colspan="1"><input type="text" class="layui-table-edit" name="jtyt" readonly></td>
        <td colspan="2"><input type="text" class="layui-table-edit" name="syjse" readonly></td>
    </tr>
    {{# } }}
    </tbody>
</script>


<script src="../../static/js/djbxx/bdcDjbZhjbxx.js"></script>
<script src="../../static/js/djbxx/djbCss.js"></script>
</body>
</html>