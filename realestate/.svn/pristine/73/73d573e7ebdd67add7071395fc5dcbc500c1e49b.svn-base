                <input type="text" class="layui-table-edit" value="{{item.zgzqe || ''}}" title="{{item.zgzqe || ''}}">
            </div>
        </td>
        <td>
            <div class="bdc-td-box">
                <input type="text" class="layui-table-edit" value="{{item.zwlxjssj || ''}}" title="{{item.zwlxjssj || ''}}">
            </div>
        </td>
        <td>
            <div class="bdc-td-box">
                <input type="text" class="layui-table-edit" value="{{item.djsj || ''}}" title="{{item.djsj || ''}}">
            </div>
        </td>
    </tr>
    {{# }) }}
</script>
<!-- 限制权利信息模板 -->
