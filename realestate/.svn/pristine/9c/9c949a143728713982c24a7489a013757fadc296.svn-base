.bdc-search-box .layui-form {
  width: 100%;
  padding-bottom: 15px;
  border-bottom: 1px dashed #d0d5da;
}
.layui-form-item {
  margin-bottom: 0;
}
.layui-form-item .layui-inline {
  width: 33%;
  margin-bottom: 0;
  margin-top: 15px;
  margin-right: 0;
}
.layui-inline .layui-form-label {
  width: 90px;
}
.layui-form-item .layui-input-inline {
  width: 60%;
}
.layui-form-select dl dd.layui-this {
  background-color: #1d87d1;
}
.layui-laydate .layui-laydate-content .layui-this {
  background-color: #1d87d1 !important;
}
.layui-form-item .layui-inline.bdc-button-box {
  width: 100%;
  text-align: center;
}
.layui-badge-rim,
.layui-colla-content,
.layui-colla-item,
.layui-collapse,
.layui-elem-field,
.layui-form-pane .layui-form-item[pane],
.layui-form-pane .layui-form-label,
.layui-input,
.layui-layedit,
.layui-layedit-tool,
.layui-quote-nm,
.layui-select,
.layui-tab-bar,
.layui-tab-card,
.layui-tab-title,
.layui-tab-title .layui-this:after,
.layui-textarea,
select {
  border-color: #d0d5da;
  color: #333;
}
/*placeholder 的字体颜色*/
input::-webkit-input-placeholder {
  color: #999;
}
.easyui-combotree {
  display: none !important;
}
.tree-node-selected {
  background: #1d87d1;
  color: #fff;
}
.panel-body {
  color: #333;
}
.textbox {
  width: 190px;
  height: 38px;
  border-color: #d0d5da;
  border-radius: 0;
}
.textbox .textbox-text {
  font-size: 14px;
  outline: none;
  color: #333;
}
.combo-arrow {
  background-color: transparent;
}
.tree-node {
  height: 36px;
  position: relative;
  padding-left: 24px;
  box-sizing: border-box;
}
.tree-title {
  height: 36px;
  line-height: 36px;
  font-size: 14px;
}
.tree-collapsed,
.tree-checkbox {
  position: absolute;
  top: 50%;
  left: 4px;
  transform: translateY(-50%);
}
#layerDemo {
  display: none;
}
.layui-layer .layui-form-item .layui-input-inline {
  width: 140px;
}
.layui-layer .textbox {
  width: 140px;
}
.layui-layer .textbox .textbox-text {
  width: 140px;
}
html,
body {
  height: 100%;
}
body {
  background-color: #eaedf1;
}
/*tab栏*/
.layui-tab {
  background-color: #fff;
  width: 100%;
  min-height: 100%;
  margin: 0;
  padding: 0 15px;
  box-sizing: border-box;
}
.layui-tab-content {
  padding: 5px 0 0 0;
  position: relative;
}
.layui-tab-title {
  height: auto;
  border-bottom-color: #d0d5da;
  color: #666;
  padding-top: 10px;
}
.layui-tab-title li {
  font-size: 16px;
  height: 38px;
  line-height: 16px;
  padding: 10px 15px;
  box-sizing: border-box;
}
.layui-tab-title .layui-this {
  color: #1d87d1;
  border-bottom: 2px solid #1d87d1;
}
.layui-tab-title .layui-this:after {
  border: none;
}
.layui-form-item .layui-inline.bdc-two-column {
  width: 64%;
}
.layui-form-item .bdc-two-column .layui-input-inline {
  width: 77%;
}
.bdc-form-div {
  width: 100%;
  height: 100%;
  padding: 15px;
  box-sizing: border-box;
  background-color: #eaedf1;
}
.bdc-form-div .layui-form {
  height: 100%;
  box-sizing: border-box;
  background-color: #fff;
}
.basic-info {
  padding: 15px 0;
  border-bottom: 1px dotted #eaedf1;
}
.basic-info:last-child {
  border-bottom: none;
}
.content-title {
  padding: 15px 0;
  border-bottom: 1px solid #eaedf1;
  background-color: #fff;
  height: 38px;
  clear: both;
  top: 15px;
  left: 15px;
  z-index: 999;
}
.content-title p {
  font-size: 20px;
  color: #333;
  float: left;
  margin-left: 20px;
  line-height: 38px;
}
.content-title .title-btn-area {
  float: right;
  margin-right: 20px;
}
.layui-form-item .layui-inline {
  width: 32%;
}
.layui-form-item .layui-input-inline {
  width: 53%;
}
.fixed-content{
  margin-top: 52px;
}
.fixed-content ul{
  background-color: #fff;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  margin-left: 15px;
  margin-right: 15px;
  margin-top: 71px;
  height: 24px;
  z-index: 4;
  padding: 15px;
  border-bottom: 1px solid #eaedf1;
}
.fixed-content .layui-tab-content{
  margin-top: 111px;
}
.fixed-title{
  background-color: #fff;
  z-index: 10;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  padding: 15px;
  border-bottom: 1px solid #eaedf1;
  height: 40px;
  margin-left: 15px;
  margin-right: 15px;
}
.fixed-title p {
  font-size: 20px;
  color: #333;
  float: left;
  margin-left: 20px;
  line-height: 38px;
}
.fixed-title .title-btn-area {
  float: right;
  margin-right: 20px;
}
.form-margin-area {
  background-color: #fff;
}
.change-input-width90 {
  width: 25.5% !important;
}
.title-sign {
  width: 100%;
  height: 20px;
}
.title-sign p {
  font-size: 16px;
  color: #333;
  height: 16px;
  border-left: 4px solid #1d87d1;
  margin-left: 30px;
  padding-left: 10px;
  line-height: 16px;
}
.layui-form-item {
  margin-bottom: 0;
}
.change-textarea-margin .layui-input-inline {
  width: 81.5%;
}
.change-textarea-margin {
  margin-top: 15px;
}
.required-span {
  color: #f54743;
  margin-right: 6px;
}
.layui-form-checked[lay-skin=primary] i {
  border-color: #1d87d1;
  background-color: #1d87d1;
  color: #fff;
}
.layui-form-checkbox[lay-skin=primary]:hover i {
  border-color: #1d87d1;
  color: #fff;
}
.layui-input:focus,
.layui-textarea:focus {
  border-color: #1d87d1 !important;
}
.layui-form-radio > i:hover,
.layui-form-radioed > i {
  color: #1d87d1;
}
.layui-form-danger + .layui-form-select .layui-input,
.layui-form-danger:focus {
  border-color: #f54743 !important;
}
input:disabled {
  /*background-color: #f3f4f6;*/
  /*cursor: not-allowed;*/
  /*color: #666;*/
}
.layui-inline .layui-form-label {
  height: 20px;
}
.layui-form-label {
  width: 90px;
}
.change-label-width {
  font-size: 14px;
}
.basic-info .bdc-label-newline {
  padding: 0 15px;
  height: 19px;
  line-height: 19px;
}
@media screen and (max-width: 1400px) {
  .layui-table {
    width: 98%;
    margin: 0 auto;
    margin-bottom: 20px;
  }
}
@media screen and (max-width: 1660px) {
  .change-textarea-margin .layui-input-inline {
    width: 81.5%;
  }
  .change-input-width90 {
    width: 25% !important;
  }
}
@media screen and (max-width: 1350px) {
  .basic-info .layui-form-item .layui-inline {
    width: 48%;
  }
  .basic-info .layui-form-item .layui-inline.bdc-two-column {
    width: 96%;
  }
  .change-textarea-margin .layui-input-inline {
    width: 74%;
  }
  .change-input-width90 {
    width: 25.5% !important;
  }
}
@media screen and (max-width: 1250px) {
  .layui-form-item .layui-inline {
    width: 100%;
  }
  .basic-info .layui-form-item .layui-inline {
    width: 100%;
  }
  .layui-form-item .layui-input-inline {
    width: 70%;
  }
  .layui-form-item .bdc-two-column .layui-input-inline {
    width: 73%;
  }
  .change-textarea-margin .layui-input-inline {
    width: 70%;
  }
  .change-input-width90 {
    width: 34.5% !important;
  }
}
.bdc-nav-container {
  width: 100%;
  height: 50px;
  padding-right: 30px;
  overflow: hidden;
  position: fixed;
  top: 84px;
  left: 15px;
  box-sizing: border-box;
  z-index: 9;
}
.bdc-nav-container .bdc-nav-box {
  width: 100%;
  height: 50px;
  background-color: #eaf4fe;
  position: relative;
}
.bdc-nav-container .bdc-nav-box .layui-icon {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  font-size: 20px;
  color: #999;
}
.bdc-nav-container .bdc-nav-box .layui-icon-prev {
  left: 8px;
}
.bdc-nav-container .bdc-nav-box .layui-icon-next {
  right: 8px;
}
.bdc-nav-container .slick-slider {
  width: 100%;
  height: 50px;
  margin-bottom: 0;
  padding: 0 35px;
  box-sizing: border-box;
}
.bdc-nav-container .slick-prev {
  left: 8px;
}
.bdc-nav-container .slick-next {
  right: 8px;
}
.bdc-nav-container .slick a {
  display: inline-block;
  width: 100%;
  height: 50px;
  line-height: 50px;
  padding: 0 5px;
  border-bottom: 2px solid #eaf4fe;
  box-sizing: border-box;
  text-align: center;
}
.bdc-nav-container .slick a:hover {
  color: #1d87d1;
}
.bdc-nav-container .slick .bdc-this a {
  border-bottom-color: #1d87d1;
  color: #1d87d1;
}
.slick-prev,
.slick-next {
  opacity: 0;
  z-index: 999;
}
.layui-tab-item .bdc-table-box {
  padding: 15px 0;
}
.bdc-tab-btn {
  position: absolute;
  top: -35px;
  right: 0;
}
.content-title .btn-tips {
  position: absolute;
  bottom: 15px;
  right: 170px;
  color: #f54743;
}
.bdc-tab-btn .btn-tips {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 400%;
  color: #f54743;
}

.title-ui{
  z-index: 20;
  margin-right: 14px;
  display: block;
  position: absolute;
  background-color: #fff;
  border: 1px solid #d0d5da;
  top: 62px;
  right: -15px;
  display: none;
}

.title-ui li {
  padding: 0 14px 0 14px;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  color: #999;
  cursor: pointer;
}

.print li:hover {
  background-color: #f3f4f6;
  color: #333;
}