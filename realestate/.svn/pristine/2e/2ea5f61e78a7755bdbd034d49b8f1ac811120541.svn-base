<!--* version 3.0.0-->
<!--* describe: form表单填写--不满一屏的情况，默认白色背景占满屏幕，超出一屏会滚动的情况，可以与本页面使用同一css，js，包含禁用样式，单选，多选，选中状态-->
<!--*/-->
<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>表单填写--不满一屏</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="../../../layui/css/layui.css" media="all">

    <!--下拉框需要-->
    <link rel="stylesheet" href="../../form-select/formSelects-v4.css">

    <link rel="stylesheet" href="../css/form.css" />
</head>
<body>
    <div class="bdc-form-div bdc-form-not-full bdc-not-full-bg">
        <form class="layui-form setOverflow" action="">
            <div class="bdc-content-fix">
                <div class="content-title layui-clear">
                    <p>表单填写</p>
                    <div class="title-btn-area">
                        <button class="layui-btn bdc-major-btn" lay-submit id="submitBtn">提交</button>
                        <button type="reset" class="layui-btn bdc-secondary-btn">保存</button>
                    </div>
                </div>
            </div>
            <div class="form-margin-area">
                <div class="basic-info">
                    <div class="title-sign"><p><a id="jbxx" href="javascript:;">基本信息</a></p></div>
                    <div class="layui-form-item">
                        <div class="layui-inline bdc-not-null">
                            <label class="layui-form-label change-label-width"><span class="required-span"><sub>*</sub></span>联动选择框</label>
                            <div class="layui-input-inline change-input-width90 bdc-change-first-select bdc-radio">
                                <select name="city" lay-verify="required" xm-select="select01" xm-select-radio xm-select-search="" xm-select-height="34px" xm-select-search-type="dl">
                                    <option value="">请选择省</option>
                                    <option value="浙江" selected="selected">浙江省</option>
                                    <option value="你的工号">江西省</option>
                                    <option value="你最喜欢的老师">福建省</option>
                                </select>
                            </div>
                            <div class="layui-input-inline change-input-width90 bdc-radio">
                                <select name="city" lay-verify="required" xm-select="select02" xm-select-radio xm-select-search="" xm-select-height="34px" xm-select-search-type="dl">
                                    <option value="">请选择</option>
                                    <option value="320001">天宁区</option>
                                    <option value="320002">钟楼区</option>
                                    <option value="320003">新北区</option>
                                    <option value="320004">武进区</option>
                                    <option value="320005">金坛区</option>
                                </select>
                            </div>
                        </div>
                        <div class="layui-inline bdc-not-null">
                            <label class="layui-form-label change-label-width"><span class="required-span"><sub>*</sub></span>可选时间</label>
                            <div class="layui-input-inline">
                                <input type="text" lay-verify="required" class="layui-input" id="endTime">
                            </div>

                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">不可选时间</label>
                            <div class="layui-input-inline bdc-date-disabled">
                                <input type="text" class="layui-input" disabled="off" id="startTime">
                                <img src="../images/lock.png" alt="">
                            </div>
                        </div>

                        <div class="layui-inline bdc-not-null">
                            <label class="layui-form-label change-label-width"><span class="required-span"><sub>*</sub></span>房屋状况</label>
                            <div class="layui-input-inline bdc-radio">
                                <select name="city" lay-verify="required" xm-select="select03" xm-select-radio xm-select-search="" xm-select-height="34px" xm-select-search-type="dl">
                                    <option value="">请选择</option>
                                    <option value="1">layer</option>
                                    <option value="2">form</option>
                                    <option value="3">layim</option>
                                    <option value="4">element</option>
                                    <option value="5">laytpl</option>
                                </select>
                            </div>
                        </div>

                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">业主姓名</label>
                            <div class="layui-input-inline">
                                <select name="modules" lay-search="" disabled="off" id="bdcName">

                                </select>
                            </div>
                        </div>

                        <div class="layui-inline bdc-not-null">
                            <label class="layui-form-label change-label-width"><span class="required-span"><sub>*</sub></span>建筑面积</label>
                            <div class="layui-input-inline bdc-one-icon">
                                <input type="text" lay-verify="required" autocomplete="off" class="layui-input" placeholder="请输入" onkeyup="value=value.replace(/[^\d{1,}\.\d{1,}|\d{1,}]/g,'')">
                                <span>m²</span>
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">房屋面积</label>
                            <div class="layui-input-inline bdc-two-icon">
                                <input type="text" disabled="off" class="layui-input" placeholder="不可使用" >
                                <span>m²</span>
                                <img src="../images/lock.png" alt="">
                            </div>

                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">不可选择单选</label>
                            <div class="layui-input-inline bdc-radio">
                                <select name="city"  disabled="disabled" xm-select="select04" xm-select-radio xm-select-search="" xm-select-height="34px" xm-select-search-type="dl">
                                    <option value="">请选择</option>
                                    <option value="1">layer</option>
                                    <option value="2">form</option>
                                    <option value="3">layim</option>
                                    <option value="4">element</option>
                                    <option value="5">laytpl</option>
                                </select>
                                <img src="../images/lock.png" alt="">
                            </div>
                        </div>

                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width bdc-label-newline">不动产<br>单元号</label>
                            <div class="layui-input-inline">
                                <input type="text" lay-verify="" autocomplete="off" class="layui-input" placeholder="请输入">
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">单选框</label>
                            <div class="layui-input-inline">
                                <input type="radio" name="sex" value="男" title="男" checked="" class="change-radio-top">
                                <input type="radio" name="sex" value="女" title="女" class="change-radio-top">
                            </div>
                        </div>
                        <div class="layui-inline bdc-two-column">
                            <label class="layui-form-label change-label-width">面积单位</label>
                            <div class="layui-input-inline">
                                <input type="checkbox" name="" title="平方米" lay-skin="primary" checked>
                                <input type="checkbox" name="" title="亩" lay-skin="primary">
                                <input type="checkbox" name="" title="公顷" lay-skin="primary" disabled>
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">测试</label>
                            <div class="layui-input-inline">
                                <select name="city" lay-verify="test">
                                    <option value=""></option>
                                    <option value="0">北京</option>
                                    <option value="1">上海</option>
                                    <option value="2">广州</option>
                                    <option value="3">深圳</option>
                                    <option value="4">杭州</option>
                                </select>
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">测试不可用</label>
                            <div class="layui-input-inline bdc-one-icon">
                                <select id="disabledSelect" name="city" lay-verify="test" disabled="off">
                                    <option value=""></option>
                                    <option value="0">北京</option>
                                    <option value="1">上海</option>
                                    <option value="2">广州</option>
                                    <option value="3">深圳</option>
                                    <option value="4">杭州</option>
                                </select>
                                <img src="../images/lock.png" alt="">
                            </div>
                        </div>
                        <div class="layui-inline">
                            <label class="layui-form-label change-label-width">不可用input</label>
                            <div class="layui-input-inline bdc-one-icon">
                                <input id="disabledInput" type="text" disabled="off" class="layui-input" placeholder="不可使用" >
                                <img src="../images/lock.png" alt="">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</body>
<script src="../../../layui/layui.js"></script>
<script src="../js/form-not-full.js"></script>
</html>