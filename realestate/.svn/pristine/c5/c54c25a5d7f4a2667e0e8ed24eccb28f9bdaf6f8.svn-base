/*表格展示*/
.layui-table thead tr,
.layui-table-click,
.layui-table-header,
.layui-table-mend,
.layui-table-patch,
.layui-table-tool,
.layui-table-total,
.layui-table-total tr,
.layui-table[lay-even] tr:nth-child(even){
    background-color: #f3f4f6;
}
.layui-table-tool{
    padding-top: 9px;
}
.layui-table-hover{
    background-color: #fff;
}
.layui-table tbody tr:hover{
    background-color: transparent;
}
.layui-table, .layui-table-view{
    margin: 0;
}


/*隐藏第二栏*/
.layui-table-header{
    height: 38px;
}
.layui-table td{
    height: 38px;
    position: relative;
    box-sizing: border-box;
}
.bdc-fh{
    height: 24px;
}
.bdc-fh span{
    display: inline-block;
    height: 24px;
    line-height: 24px;
}
.bdc-describe{
    height: 24px;
    line-height: 28px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.bdc-table-state{
    height: 16px;
    line-height: 16px;
}
.bdc-jy-state{
    margin-top: 5px;
    margin-bottom: 15px;
}
.bdc-ql-state{
    margin-bottom: 10px;
}
.bdc-table-state span{
    display: inline-block;
    width: 16px;
    height: 16px;
    margin-right: 5px;
}
.bdc-btn{
    text-align: center;
}
tbody .layui-table-cell{
    height: 100%;
    padding: 0 5px;
}


/*详情*/
.bdc-details{
    width: 424px;
    /*height: 232px;*/
    position: fixed;
    z-index: 9999;
}
.bdc-details .layui-table td{
    height: 28px;
    padding: 5px 10px;
    box-sizing: border-box;
}
.bdc-details-title{
    width: 154px;
    background-color: #f3f4f6;
    text-align: right;
    padding-right: 10px;
    box-sizing: border-box;
}

.bdc-details-name{
    height: 30px;
    line-height: 30px;
    padding-left: 10px;
    background-color: #eaedf1;
}

/*更多操作按钮*/
.bdc-more-box{
    display: inline-block;
    position: relative;
    /*margin-left: 10px;*/
}
.bdc-btn-more i{
    position: relative;
    top: 1px;
    left: 2px;
}
.bdc-more-box .bdc-table-btn-more{
    width: 112px;
    border: 1px solid #d0d5da;
    position: absolute;
    top: 32px;
    background-color: #fff;
    z-index: 999;
    display: none;
}
.bdc-more-box .bdc-table-btn-mor.bdc-left-more{
    left: 0;
}
.bdc-more-box .bdc-table-btn-more a,
.bdc-more-box .bdc-table-btn-more>span{
    display: inline-block;
    width: 100%;
    height: 30px;
    line-height: 30px;
    padding-left: 8px;
    box-sizing: border-box;
    color: #333;
    font-size: 12px;
}
.bdc-more-box .bdc-table-btn-more>span{
    position: relative;
    float: left;
}
.bdc-more-box .bdc-table-btn-more a:hover,
.bdc-more-box .bdc-table-btn-more>span:hover{
    background-color: #eaedf1;
}
.layui-table-box{
    overflow: visible;
}

.layui-aside-top{
    height: 50px;
    box-sizing: border-box;
    width: 100%;
}
.layui-aside-top span{
    line-height: 40px;
    font-size: 16px;
    margin-right: 10px;
}
.layui-aside-top button{
    line-height: 40px;
}
.layui-right-top span{
    line-height: 40px;
    font-size: 16px;
}

.accordion .area-link {
    cursor: pointer;
