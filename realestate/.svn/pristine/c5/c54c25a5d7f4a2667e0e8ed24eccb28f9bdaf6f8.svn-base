body,
html {
    font-family: "Microsoft YaHei", Helvetica, Arial, sans-serif !important;
    font-size: 14px;
    width: 100%;
    height: 100%;
}
body{
    padding: 10px;
    box-sizing: border-box;
    background-color: #eaedf1;
}
.clear:after {
    content: '';
    display: block;
    clear: both;
}
.bdc-hide {
    display: none;
}
.bdc-major-btn {
    background-color: #1d87d1;
}
.bdc-major-btn:hover {
    background-color: #1675b8;
}
.bdc-secondary-btn {
    background-color: #f3f4f6;
    color: #666;
    border: 1px solid #d0d5da;
    line-height: 20px;
}
.bdc-secondary-btn:hover {
    color: #666;
    opacity: 1;
}
.bdc-delete-btn {
    background-color: #f54743;
}
.bdc-table-second-btn {
    background-color: #fff;
    border: 1px solid #d0d5da;
    color: #333;
}
.bdc-table-second-btn:hover {
    color: #333;
}
/*外包框*/
.bdc-container{
    width: 100%;
    height: 100%;
    /*padding-left: 300px;*/
    padding-left: 280px;
    box-sizing: border-box;
    position: relative;
    background-color: #fff;
    /*top: -10px;*/
}
/*左侧菜单栏*/
.bdc-menu-aside {
    width: 280px;
    height: 100%;
    min-height: 100%;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: 0;
    background-color: #fff;
    padding: 10px;
}
.bdc-aside-zoom{
    width: 12px;
    height: 36px;
    border: 1px solid #d0d5da;
    position: absolute;
    left: -10px;
    top: 50%;
    transform: translateY(-50%);
    z-index: 99;
    text-align: center;
    background-color: #fff;
}
/*.bdc-aside-close{*/
/*left: 246px;*/
/*}*/
/*.bdc-aside-open{*/
/*left: -13px;*/
/*}*/
.bdc-aside-zoom .layui-icon{
    font-size: 12px;
    line-height: 36px;
    color: #666;
    font-weight: 700;
}
.bdc-menu-aside .aside {
    height: 100%;
    position: relative;
    padding-top: 52px;
    box-sizing: border-box;
}
.bdc-menu-aside h4 {
    width: 100%;
    padding-left: 12px;
    font-size: 16px;
    line-height: 52px;
    text-align: left;
    box-sizing: border-box;
    border: 1px solid #d0d5da;
    border-bottom: none;
    position: absolute;
    top: 0;
    left: 0;
}
.bdc-menu-aside .bdc-xlh{
    height: 32px;
    line-height: 32px;
    padding-left: 15px;
    margin-top: 10px;
}
#aside{
    height: 100%;
    box-sizing: border-box;
    border: 1px solid #d0d5da;
    margin-bottom: 10px;
    overflow: auto;

}
.accordion {
    width: 100%;
    box-sizing: border-box;
}
.accordion .link {
    cursor: pointer;
    display: block;
    padding: 15px 15px 15px 50px;
    color: #333;
    font-size: 14px;
    position: relative;
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
    white-space: nowrap;
}
.accordion .area-link {
    cursor: pointer;
    display: block;
    padding: 15px 15px 15px 46px;
    color: #333;
    font-size: 14px;
    position: relative;
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
}
.accordion li:last-child .link {
    border-bottom: 0;
}
.accordion li i {
    /*position: absolute;*/
    top: 16px;
    left: 12px;
    font-size: 18px;
    color: #333;
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
}
.accordion .area-link i{
    position: absolute;
    top: 16px;
    left: 28px;
    font-size: 18px;
    color: #b5b7b8;
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
}
.link .docu-icon {
    position: absolute;
    left: 28px;
    top: 18px;
}
.accordion li.open .link {
    color: #1d87d1;
}
.accordion li.open i {
    color: #1d87d1;
}
.accordion .open .link i.fa-caret-right {
    -webkit-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    -o-transform: rotate(90deg);
    transform: rotate(90deg);
}

.submenu {
    display: none;
    font-size: 14px;
}
.submenu li {
    /*padding: 6px 56px;*/
    box-sizing: border-box;
    padding-left: 56px;
    padding-top: 6px;
    padding-bottom: 6px;
}
.submenu a {
    display: block;
    position: relative;
    text-decoration: none;
    color: #333;
    height: 28px;
    line-height: 28px;
    padding-left: 6px;
    box-sizing: border-box;
    -webkit-transition: all 0.25s ease;
    -o-transition: all 0.25s ease;
    transition: all 0.25s ease;
    word-break: normal;
    white-space: nowrap;
}
.submenu a .docu-icon {
    position: absolute;
    top: 50%;
    left: -20px;
    transform: translateY(-50%);
}
.submenu a:hover {
    background-color: #1675b3;
    color: #fff;
}
.submenu a.active {
    background-color: #1d87d1;
    color: #fff;
}
/*右侧主内容区*/
.bdc-right-content {
    width: 100%;
    height: 100%;
    background-color: #fff;
    padding: 60px 10px 10px;
    box-sizing: border-box;
    position: relative;
}
.bdc-right-content .bdc-right-title{
    width: 100%;
    height: 60px;
    line-height: 75px;
    padding-left: 20px;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: 0;
}
/*tab栏*/
.layui-tab{
    width: 100%;
    height: 100%;
    margin: 0;
}
.layui-tab-title{
    height: 32px;
    border-bottom-color: #d0d5da;
    color: #666;
}
.layui-tab-title li{
    font-size: 16px;
    height: 32px;
    line-height: 32px;
    box-sizing: border-box;
}
.layui-tab-title .layui-this{
    color: #1d87d1;
    border-bottom: 2px solid #1d87d1;
}
.layui-tab-title .layui-this:after{
    border: none;
}
/*楼盘表主内容*/
.layui-tab-content{
    padding: 0;
}
.layui-tab-item{
    position: relative;
}
.bdc-state-zoom{
    width: 38px;
    height: 14px;
    line-height: 14px;
    border: 1px solid #d0d5da;
    background-color: #fff;
    box-sizing: border-box;
    text-align: center;
    position: absolute;
    top: 125px;
    left: 50%;
    transform: translateX(-50%);
    cursor: pointer;
}
.bdc-state-zoom .layui-icon{
    font-size: 12px;
    color: #666;
    font-weight: 700;
}
/*状态展示*/
.bdc-state-show{
    padding-top: 20px;
    border-bottom: 1px dashed #d0d5da;
    margin-bottom: 15px;
    position: relative;
    overflow: hidden;
}
.bdc-state-hide{
    border-bottom: none;
    padding-top: 0;
}
.bdc-state-show p{
    color: #333;
    height: 20px;
    line-height: 20px;
    margin-bottom: 15px;
}
.bdc-state-show p span{
    display: inline-block;
    min-width: 72px;
    padding-left: 30px;
    margin-right: 20px;
    position: relative;
    box-sizing: border-box;
}
.bdc-state-show p i{
    position: absolute;
    top: 0;
    left: 0;
    display: inline-block;
    width: 20px;
    height: 20px;
}
.bdc-fh span{
    font-weight: 700;
}
