/*收缩后系统图标*/
.set-layui-icon-shrink-right{
    background: url(../image/arrow.png) no-repeat 0 0;
    background-size:cover;
    margin-top: 16px;
    transition: all 0.1s;
    -moz-transition: all 0.1s;
    -webkit-transition: all 0.1s;
    -o-transition: all 0.1s;
}
/*收缩后设置*/
.layadmin-side-shrink .layui-layout-admin .layui-logo{
    width: 50px;
}
.layadmin-side-shrink .layui-side{
    width: 50px;
}
.layadmin-side-shrink .layadmin-pagetabs,
.layadmin-side-shrink .layui-layout-admin .layui-body,
.layadmin-side-shrink .layui-layout-admin .layui-footer,
.layadmin-side-shrink .layui-layout-admin .layui-layout-left{
    left: 50px;
}
.layui-layout-admin .layui-body .layadmin-tabsbody-item{
    overflow-y: auto;
}


/*右上方*/
.ul-margin-right{
    margin-right: 30px;
}
.layui-layout-admin .layui-header{
    /*border-bottom-color: #d0d5da;*/
    border-bottom: none;
}
.layui-header .layui-nav-bar{
    display: none;
}
.layui-layout-admin .layui-layout-left .layui-nav-item{
    margin: 0;
}
/*消息中心*/
/*提示消息数量*/
.layui-layout-right .layui-nav-item a{
    padding: 0 30px 0 0;
}
.ul-margin-right.layui-nav .layui-nav-item{
    margin-left: 30px;
}
.ul-margin-right.layui-nav .bdc-person-nav dl a{
    padding: 0 15px;
}
.layui-nav .layui-nav-more{
    right: 12px;
}
.bdc-num-tips{
    background-color: #f54743;
    border-radius: 10px;
    color: #fff;
    width: 26px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    position: absolute;
    top: 4px;
    right: 8px;
    box-sizing: border-box;
}
.layui-nav .layui-nav-item img{
    padding-right: 10px;
}
.bdc-msg-nav .layui-nav-more{
    display: none;
}
.layui-layout-admin .layui-header .bdc-msg-child{
    top: 49px;
    width: 380px;
    color: #333;
    box-shadow: 0 20px 50px #e9e9e9 inset;
    padding: 0 20px;
    box-sizing: border-box;
}
.bdc-msg-child .bdc-msg-title{
    height: 60px;
    line-height: 60px;
    font-size: 18px;
    text-align: center;
    position: relative;
}
.bdc-msg-child .bdc-msg-title .layui-icon-close{
    position: absolute;
    top: 0;
    right: 18px;
    font-size: 22px;
    color: #999;
    cursor: pointer;
}
.layui-tab-brief>.layui-tab-title .layui-this{
    color: #1d87d1;
}
.bdc-msg-nav .layui-tab-brief>.layui-tab-more li.layui-this:after,
.bdc-msg-nav .layui-tab-brief>.layui-tab-title .layui-this:after{
    border-bottom-color: #1d87d1;
    bottom: 0!important;
    top: auto!important;
}
.bdc-msg-child .layui-tab-title li{
    width: 120px;
    box-sizing: border-box;
    color: #666;
}
.bdc-msg-child .layui-tab-content{
    padding: 0;
}
.layui-layout-right .layui-nav-item .bdc-msg-child a{
    padding: 0 0 0 20px;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px dashed #d0d5da;
    box-sizing: border-box;
    position: relative;
}
.layui-nav .bdc-msg-child a:hover{
    background-color: #fff;
}
.layui-layout-admin .layui-header .layui-nav .bdc-msg-child .bdc-read a{
    color: #666;
}
.layui-layout-right .layui-nav-item .bdc-msg-child a img{
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
}
.layui-tab{
    margin: 0 0 10px 0;
}
.bdc-msg-child .layui-tab-item{
    overflow-y: auto;
}
.bdc-btn-more{
    width: 72px;
    height: 36px;
    padding: 0;
    text-align: center;
    border-color: #d0d5da;
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
}
.layui-btn-primary:hover{
    border-color: #d0d5da;
    opacity: .8;
}

/*管理员*/
.bdc-person-nav .bdc-person-child{
    padding: 0;
    background-color: #f3f4f6;
}
.bdc-person-nav .bdc-person-child dd{
    height: 40px;
    line-height: 40px;
}
.layui-nav .bdc-person-nav .bdc-person-child a:hover {
    background-color: #eaedf1;
}

/*系统切换*/
.bdc-system-child{
    width: 218px;
    background-color: #f3f4f6;
}
.layui-layout-right .bdc-system-child a{
    padding: 0 12px;
    border-left: 4px solid transparent;
}
.layui-layout-right .bdc-system-child a:hover{
    border-left-color: #1d87d1;
    background-color: transparent;
}
.layui-nav .bdc-system-child img{
    width: 24px;
    height: 24px;
    padding-right: 8px;
}
.bdc-system-child ul li{
    height: 28px;
    line-height: 28px;
    padding: 7px 0;
}
.layui-layout-admin .layui-header .layui-nav .layui-this:after,
.layui-layout-admin .layui-header .layui-nav-bar{
    height: 0;
}

/*tab切换*/
.layadmin-pagetabs .layui-tab{
    position: relative;
    top: -1px;
}
.layadmin-pagetabs .layui-tab-title li,
.layadmin-pagetabs .layui-icon-prev{
    border-right-color: #d0d5da;
}
.layadmin-pagetabs .layadmin-tabs-control{
    border-left-color: #d0d5da;
}
.layadmin-pagetabs .layui-tab-title li.layui-this{
    background-color: #045085;
    color: #fff;
}
.layadmin-pagetabs .layui-tab-title li:hover{
    background-color: #f3f4f6;
    border-top: 1px solid #d0d5da;
}
.layadmin-pagetabs .layui-tab-title li.layui-this:hover{
    background-color: #045085;
}
.layadmin-pagetabs .layui-tab-title li:after{
    height: 0;
}
.layadmin-pagetabs .layui-tab-title li.layui-this:after{
    height: 0;
}
.layadmin-pagetabs{
    border-bottom: 1px solid #045085;
    border-top: 1px solid #d0d5da;
}
.layadmin-pagetabs .layui-tab-title li{
    line-height: 39px;
    border-top: 1px solid transparent;
}
.layadmin-pagetabs .layui-icon-set-sm{
    right: 0;
    font-size: 0;
}
.layadmin-pagetabs .layui-icon-set-sm>img{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}
.layadmin-pagetabs .layui-tab-title{
    height: 39px;
}
/*新增 固定home*/
.layadmin-pagetabs .layui-tab-title li:first-child{
    display: none;
}
.layadmin-pagetabs .layui-tab-title li:nth-child(2){
    width: 46px;
    padding: 0;
    position: fixed;
    top: 50px;
    left: 260px;
}
.layadmin-side-shrink .layadmin-pagetabs .layui-tab-title li:nth-child(2){
    left: 90px;
}
.layadmin-pagetabs .layui-tab-title li:nth-child(2) .layui-tab-close{
    display: none;
}


/*主内容区*/
/*左侧*/
.bdc-container{
    width: 100%;
    height: 100%;
    position: relative;
    padding: 10px 340px 0 10px;
    box-sizing: border-box;
}
.bdc-left{
    width: 100%;
    min-height: 100%;
    float: left;
    /*padding-right: 10px;*/
    box-sizing: border-box;
}
/*办理情况*/
.bdc-handle-info{
    width: 100%;
    background-color: #fff;
    box-sizing: border-box;
    margin-bottom: 7px;
}
.bdc-merge.bdc-handle-info{
    display: inline-block;
    width: 50%;
}
.bdc-common-title{
    /*width: 100%;*/
    height: 38px;
    border-bottom: 1px solid #eaedf1;
    box-sizing: border-box;
    padding: 10px;
    position: relative;
    /*cursor: move;*/
    /*margin-right: 200px;*/
}
.bdc-common-title>span{
    display: block;
    height: 18px;
    font-size: 16px;
    line-height: 18px;
    border-left: 4px solid #1d87d1;
    padding-left: 10px;
}
.bdc-module .bdc-others{
    height: 36px;
    line-height: 36px;
    position: absolute;
    top: -38px;
    right: 10px;
    font-size: 14px;
    color: #999;
    cursor: pointer;
}
.bdc-others .bdc-others-more{
    display: inline-block;
    margin-left: 5px;
}
.bdc-others-more>img{
    padding: 15px 0;
}
.bdc-others>span{
    margin-right: 5px;
}
.bdc-common-title>div>img{
    padding: 15px 0;
}
.bdc-common-title>div>span{
    padding-right: 10px;
}
.bdc-common-more{
    min-width: 72px;
    line-height: 30px;
    text-align: left;
    position: absolute;
    top: 30px;
    right: -10px;
    color: #333;
    z-index: 9;
    box-shadow: 0 0 5px #d0d5da;
    background-color: #fff;
}
.bdc-common-more>span{
    display: inline-block;
    width: 100%;
    height: 30px;
    line-height: 30px;
    padding-left: 10px;
    box-sizing: border-box;
}
.bdc-common-more>span:hover{
    background-color: #eaedf1;
}
.bdc-handle-container{
    width: 100%;
    height: 138px;
    padding: 10px 0 10px 10px;
    box-sizing: border-box;
    position: relative;
}
.bdc-handle-msg-box{
    width: 33.33%;
    height: 100%;
    padding-right: 10px;
    box-sizing: border-box;
    float: left;
}
.bdc-handle-msg{
    width: 100%;
    height: 100%;
    padding: 10px 10px 10px 15px;
    box-sizing: border-box;
    background-color: #eaedf1;
    position: relative;
}
.bdc-handle-name{
    line-height: 24px;
}
.bdc-handle-num{
    font-size: 24px;
    color: #1d87d1;
}
.bdc-handle-done .bdc-handle-num{
    color: #26ac26;
}
.bdc-handle-warn .bdc-handle-num{
    color: #f24b43;
}
.bdc-handle-msg>img{
    position: absolute;
    right: 10px;
    bottom: 10px;
}
/*错误提示*/
.bdc-error-tips{
    width: 100%;
    height: 100%;
    background-color: #fff;
    /*position: relative;*/
    margin-bottom: 7px;
}
.bdc-merge.bdc-error-tips{
    display: inline-block;
    width:calc(50% - 12px);
    width:-webkit-calc(50% - 12px);
    width:-moz-calc(50% - 12px);
    margin-left: 7px;
    height: 100%;
    box-sizing: border-box;
}

.bdc-error-tips .bdc-operate-more{
    line-height: 38px;
    height: 38px;
    /*cursor: pointer;*/
    z-index: 9;
    text-align: right;
    padding: 0 10px;
    position: relative;
}
.bdc-operate-more>img{
    padding: 15px 0;
    cursor: pointer;
}
.bdc-operate-more>span{
    display: inline-block;
    font-size: 14px;
    color: #999;
