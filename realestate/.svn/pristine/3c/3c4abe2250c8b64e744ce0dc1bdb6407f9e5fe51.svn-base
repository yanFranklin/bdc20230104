body,html{
    font-family: "Microsoft YaHei", Helvetica, Arial, sans-serif !important;
    font-size: 14px;
    width: 100%;
    height: 100%;
    background-color: #eaedf1;
}
.bdc-hide{
    display: none;
}
.bdc-container .bdc-left .bdc-hide{
    display: none;
}
.bdc-visible{
    visibility: hidden;
}
.clear:after {
    content: '';
    display: block;
    clear: both;
}

/*侧边栏*/
.layadmin-setTheme-side,
.layui-side-menu{
    background-color: #045085;
}
.layui-layout-admin .layui-logo{
    background-color: #045085;
    box-shadow:none;
    font-weight: 400;
    padding: 0 0 0 34px;
    text-align: left;
}
.layui-nav-tree .layui-nav-child dd.layui-this,
.layui-nav-tree .layui-nav-child dd.layui-this a,
.layui-nav-tree .layui-this,
.layui-nav-tree .layui-this>a,
.layui-nav-tree .layui-this>a:hover{
    background-color: #1d87d1;
}
.layui-layout-admin .layui-logo,
.layui-layout-admin .layui-logo a{
    color: rgba(255,255,255,1);
}
.layui-layout-admin .layui-logo .bdc-logo-img{
    position: absolute;
    top: 50%;
    left: 6px;
    transform: translateY(-50%);
}
.layadmin-side-shrink .layui-layout-admin .layui-logo .bdc-logo-img{
    display: none;
}
.layadmin-side-shrink .layui-layout-admin .layui-logo{
    background-position-x: 12px;
}
.layui-side-menu .layui-nav .layui-nav-itemed>.layui-nav-child{
    padding: 0;
    background-color: #07436e;
}
.layui-header .layui-icon {
    display: block;
    width: 16px;
    height: 16px;
    background-size: cover;
    font-weight: 700;
}
/*一级菜单*/
.layui-side-menu .layui-nav .layui-nav-item>a{
    height: 50px;
    line-height: 50px;
    padding-left: 50px;
    position: relative;
    padding-top: 0;
    padding-bottom: 0;
}
.layui-side-menu .layui-nav .layui-nav-item .layui-icon{
    left: 12px;
    transform: translateY(-50%);
    margin-top: 0;
}

/*二级菜单*/
.layui-side-menu .layui-nav .layui-nav-child dd a{
    height: 40px;
    line-height: 40px;
    font-size: 12px;
    padding-left: 60px;
}
/*三级菜单*/
.layui-side-menu .layui-nav .layui-nav-child .layui-nav-child a{
    padding-left: 70px;
}

/*四级菜单*/
.slide-level-four{
    position: absolute;
    left: 230px;
    top: 10px;
    background-color: #fff;
    z-index: 999999;
    display: none;
    box-shadow:#eaedf1 0 4px 30px 0;
}

.slide-level-four dd{
    padding: 0 30px 0 20px;
    height: 30px;
    line-height: 30px;
    color: #666;
    font-size: 12px;
}
.slide-level-four dd:hover{
    background-color: #eaedf1;
    color: #333;
}
/*侧边栏小图标*/
.layui-nav-tree .layui-nav-bar{
    background-color: #1d87d1;
}
.layui-side-menu .layui-icon{
    height: 24px;
    width: 24px;
    background-size:cover ;
}
.layui-side-menu .layui-nav .layui-icon>img{
    padding-right: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}
