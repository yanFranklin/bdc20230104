package cn.gtmap.realestate.exchange.core.dto.hefei.fcjy.response;
/*
 * @author <a href="mailto:huangzijian@gtmap.cn">huangzijian</a>
 * @version 1.0, 2018/11/23
 * @description  交易双方
 */
public class ResponseJysf {
    // 姓名
    private String xm;
    // 证件类型
    private String zjlx;
    // 证件号码
    private String zjhm;
    // 联系电话
    private String dh;
    // 地址
    private String dj;
    // 国籍中文名称
    private String gj;
    // 所占份额 %
    private String szfe;
    // 买卖双方标记 0为卖方，1为买方
    private String bj;
    // 共有人标记 0不是共有人，1是共有人
    private String gyrbj;

    private String gyfs;

    public String getXm() {
        return xm;
    }

    public void setXm(String xm) {
        this.xm = xm;
    }

    public String getZjlx() {
        return zjlx;
    }

    public void setZjlx(String zjlx) {
        this.zjlx = zjlx;
    }

    public String getZjhm() {
        return zjhm;
    }

    public void setZjhm(String zjhm) {
        this.zjhm = zjhm;
    }

    public String getDh() {
        return dh;
    }

    public void setDh(String dh) {
        this.dh = dh;
    }

    public String getDj() {
        return dj;
    }

    public void setDj(String dj) {
        this.dj = dj;
    }

    public String getGj() {
        return gj;
    }

    public void setGj(String gj) {
        this.gj = gj;
    }

    public String getSzfe() {
        return szfe;
    }

    public void setSzfe(String szfe) {
        this.szfe = szfe;
    }

    public String getBj() {
        return bj;
    }

    public void setBj(String bj) {
        this.bj = bj;
    }

    public String getGyrbj() {
        return gyrbj;
    }

    public void setGyrbj(String gyrbj) {
        this.gyrbj = gyrbj;
    }

    public String getGyfs() {
        return gyfs;
    }

    public void setGyfs(String gyfs) {
        this.gyfs = gyfs;
    }
}
