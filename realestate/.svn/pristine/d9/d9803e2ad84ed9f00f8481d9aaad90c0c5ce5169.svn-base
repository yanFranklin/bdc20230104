<body>
<div class="bdc-upload-dragdiv" style="position: relative;left: 50%;top: 50%;transform: translate(-50%,-50%);width:100%;height:calc(100% - 40px);overflow:scroll;" id="hstdiv">
    <div class="layui-upload-drag img-drag" id="hstImage" style="padding: 0;border: none;width:100%;height:100%;">
    </div>
</div>
</body>

<script type="text/html" id="uploadTpl">
    {{# if(d.url){ }}
    <img id="img" class="upload-img" style="max-width: 100%;overflow: scroll;" src="{{d.url}}" alt="">
    {{# }else{ }}
    <div class="upload-icon">
        <i class="layui-icon">&#xe654;</i>
        <span>无分层分户图</span>
    </div>
    <img id="img" class="upload-img" style="max-width: 900px" src="" alt="">
    <div class="video-icon"></div>
    {{# } }}
</script>

