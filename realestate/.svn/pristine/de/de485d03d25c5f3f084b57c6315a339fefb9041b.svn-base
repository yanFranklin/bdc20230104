            <div class="title-sign">
                <p>基本信息</p>
            </div>
            <div class="layui-form-item">

                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">收件编号</label>
                    <div class="layui-input-inline">
                        <input type="text" log="slbh" name="slbh" id="slbh" disabled="off" class="layui-input"
                               value="{{bdcxmxx.slbh || ''}}">
                    </div>
                </div>

                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">受理人</label>
                    <div class="layui-input-inline">
                        <input type="text" log="slr" name="slr" id="slr" disabled="off" class="layui-input"
                               value="{{bdcxmxx.slr || ''}}">
                    </div>
                </div>

                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">受理时间</label>
                    <div class="layui-input-inline">
                        <input type="text" log="slsj" name="slsj" id="slsj" disabled="off"
                               class="layui-input date test-item"
                               value="{{bdcxmxx.slsj || ''}}">
                    </div>
                </div>

                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">是否分别持证</label>
                    <div class="layui-input-inline">
                        <input type="radio" log="sqfbcz" name="sqfbcz" id="sqfbcz" value="1" title="是" {{#
                               if(bdcxmxx.sqfbcz=='1' || bdcxmxx.sqfbcz== null){ }} checked {{# } }}>
                        <input type="radio" log="sqfbcz" name="sqfbcz" value="0" title="否" {{# if(bdcxmxx.sqfbcz=='0'){
                               }} checked {{# } }}>
                    </div>
                </div>

                <div class="layui-inline bdc-two-column">
                    <label class="layui-form-label change-label-width">登记原因</label>
                    <div class="layui-input-inline bdc-radio">
                        <select lay-search="" log="djyy" name="djyy" id="djyy" xm-select="djyy{{d.index || ''}}"
                                xm-select-radio
                                xm-select-search="" xm-select-height="34px"
                                xm-select-search-type="dl" xm-select-create="" lay-filter="djyy{{d.index || ''}}">
                            <option value="{{bdcxmxx.djyy ||''}}" {{# if(bdcxmxx.djyy){ }} selected="selected" {{# } }}>
                                {{bdcxmxx.djyy || '请选择'}}
                            </option>
                            {{# layui.each(d.djxldjyy, function(index, item){ }}
                            {{# if(bdcxmxx.djyy !=item.djyy ){ }}
                            <option value="{{item.djyy}}">{{item.djyy || ''}}</option>
                            {{# } }}
                            {{# }); }}
                        </select>
                    </div>
                </div>
                <div class="layui-inline bdc-two-column">
                    <label class="layui-form-label change-label-width">面积单位</label>
                    <div class="layui-input-inline" style="margin-top: 7px">
                        {{# layui.each(d.zd.mjdw, function(index, item){ }}
                        <input type="checkbox" lay-filter="mjdw" id="mjdw" log="mjdw" name="mjdw"
                               value="{{item.DM || '0'}}" {{#
                               if(bdcxmxx.mjdw==item.DM ){ }} checked="checked" {{# } }} lay-skin="primary">{{item.MC ||
                        ''}}
                        {{# }); }}
                    </div>
                </div>

                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">
                        <span class="required-span"><sub>*</sub></span>登记时间</label>
                    <div class="layui-input-inline">
                        <input type="text" log="djsj" name="djsj" id="djsj" class="layui-input date test-item"
                               value="{{bdcxmxx.djsj || ''}}">
                    </div>
                </div>

                <div class="layui-inline">
                    <label class="layui-form-label change-label-width">
                        <span class="required-span"><sub>*</sub></span>登簿人</label>
                    <div class="layui-input-inline">
                        <input type="text" log="dbr" name="dbr" id="dbr" class="layui-input"
                               value="{{bdcxmxx.dbr || ''}}">
                    </div>
                </div>
            </div>
            <div class="layui-form-item change-textarea-margin">
                <label class="layui-form-label change-label-width">备注</label>
                <div class="layui-input-inline">
                    <textarea class="layui-textarea change-textarea-width " name="bz"
                              id="bz">{{bdcxmxx.bz || ''}}</textarea>
                </div>
            </div>
        </div>
    </form>
    {{#  } }}

</script>

