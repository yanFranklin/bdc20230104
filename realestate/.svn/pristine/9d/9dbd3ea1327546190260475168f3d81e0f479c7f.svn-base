body, html {
    font-family: "Microsoft YaHei", Helvetica, Arial, sans-serif !important;
    font-size: 14px;
}

html {
    background-color: #eaedf1;
}

.bdc-hide {
    display: none;
}

.clear:after {
    content: '';
    display: block;
    clear: both;
}

/*侧边栏*/
.layadmin-setTheme-side,
.layui-side-menu {
    background-color: #045085;
}

.layui-layout-admin .layui-logo {
    background-color: #045085;
    box-shadow: none;
    font-weight: 400;
    padding: 0 0 0 50px;
    text-align: left;
}

.layui-nav-tree .layui-nav-child dd.layui-this,
.layui-nav-tree .layui-nav-child dd.layui-this a,
.layui-nav-tree .layui-this,
.layui-nav-tree .layui-this > a,
.layui-nav-tree .layui-this > a:hover {
    background-color: #1d87d1;
}

.layui-layout-admin .layui-logo,
.layui-layout-admin .layui-logo a {
    color: rgba(255, 255, 255, 1);
}

.layui-layout-admin .layui-logo .bdc-logo-img {
    position: absolute;
    top: 50%;
    left: 12px;
    transform: translateY(-50%);
}

.layadmin-side-shrink .layui-layout-admin .layui-logo .bdc-logo-img {
    display: none;
}

.layadmin-side-shrink .layui-layout-admin .layui-logo {
    background-position-x: 12px;
}

.layui-side-menu .layui-nav .layui-nav-itemed > .layui-nav-child {
    padding: 0;
    background-color: #07436e;
}

.layui-header .layui-icon {
    display: block;
    width: 16px;
    height: 16px;
    background-size: cover;
}

/*一级菜单*/
.layui-side-menu .layui-nav .layui-nav-item > a {
    height: 50px;
    line-height: 50px;
    padding-left: 50px;
    position: relative;
    padding-top: 0;
    padding-bottom: 0;
}

.layui-side-menu .layui-nav .layui-nav-item .layui-icon {
    left: 12px;
    transform: translateY(-50%);
    margin-top: 0;
}

/*二级菜单*/
.layui-side-menu .layui-nav .layui-nav-child dd a {
    height: 40px;
    line-height: 40px;
    font-size: 12px;
    padding-left: 60px;
}

/*三级菜单*/
.layui-side-menu .layui-nav .layui-nav-child .layui-nav-child a {
    padding-left: 70px;
}

/*四级菜单*/
.slide-level-four {
    position: absolute;
    left: 230px;
    top: 10px;
    background-color: #fff;
    z-index: 999999;
    display: none;
    box-shadow: #eaedf1 0 4px 30px 0;
}

.slide-level-four dd {
    padding: 0 30px 0 20px;
    height: 30px;
    line-height: 30px;
    color: #666;
    font-size: 12px;
}

.slide-level-four dd:hover {
    background-color: #eaedf1;
    color: #333;
}

/*侧边栏小图标*/
.layui-nav-tree .layui-nav-bar {
    background-color: #1d87d1;
}

.layui-side-menu .layui-icon {
    height: 24px;
    width: 24px;
    background-size: cover;
}

.bdc-icon-mine {
    background: url(../image/icon-1.png) no-repeat 0 0;
}

.layui-this .bdc-icon-mine,
.layui-nav-itemed .bdc-icon-mine,
.layui-nav-item:hover .bdc-icon-mine {
    background: url(../image/icon-2.png) no-repeat 0 0;
}


.bdc-icon-task {
    background: url(../image/icon-1.png) no-repeat 0 -24px;
}

.layui-this .bdc-icon-task,
.layui-nav-itemed .bdc-icon-task,
.layui-nav-item:hover .bdc-icon-task {
    background: url(../image/icon-2.png) no-repeat 0 -24px;
}

.bdc-icon-comprehensive {
    background: url(../image/icon-1.png) no-repeat 0 -48px;
}

.layui-this .bdc-icon-comprehensive,
.layui-nav-itemed .bdc-icon-comprehensive,
.layui-nav-item:hover .bdc-icon-comprehensive {
    background: url(../image/icon-2.png) no-repeat 0 -48px;
}

.bdc-icon-build {
    background: url(../image/icon-1.png) no-repeat 0 -72px;
}

.layui-this .bdc-icon-build,
.layui-nav-itemed .bdc-icon-build,
.layui-nav-item:hover .bdc-icon-build {
    background: url(../image/icon-2.png) no-repeat 0 -72px;
}

.bdc-icon-supply {
    background: url(../image/icon-1.png) no-repeat 0 -96px;
}

.layui-this .bdc-icon-supply,
.layui-nav-itemed .bdc-icon-supply,
.layui-nav-item:hover .bdc-icon-supply {
    background: url(../image/icon-2.png) no-repeat 0 -96px;
}

.bdc-icon-supervise {
    background: url(../image/icon-1.png) no-repeat 0 -120px;
}

.layui-this .bdc-icon-supervise,
.layui-nav-itemed .bdc-icon-supervise,
.layui-nav-item:hover .bdc-icon-supervise {
    background: url(../image/icon-2.png) no-repeat 0 -120px;
}

.bdc-icon-save {
    background: url(../image/icon-1.png) no-repeat 0 -144px;
}

.layui-this .bdc-icon-save,
.layui-nav-itemed .bdc-icon-save,
.layui-nav-item:hover .bdc-icon-save {
    background: url(../image/icon-2.png) no-repeat 0 -144px;
}

/*收缩后系统图标*/
.set-layui-icon-shrink-right {
    background: url(../image/arrow.png) no-repeat 0 0;
    background-size: cover;
    margin-top: 16px;
    transition: all 0.1s;
    -moz-transition: all 0.1s;
    -webkit-transition: all 0.1s;
    -o-transition: all 0.1s;
}

/*收缩后设置*/
.layadmin-side-shrink .layui-layout-admin .layui-logo {
    width: 50px;
}

.layadmin-side-shrink .layui-side {
    width: 50px;
}

.layadmin-side-shrink .layadmin-pagetabs,
.layadmin-side-shrink .layui-layout-admin .layui-body,
.layadmin-side-shrink .layui-layout-admin .layui-footer,
.layadmin-side-shrink .layui-layout-admin .layui-layout-left {
    left: 50px;
}


/*右上方*/
.ul-margin-right {
    margin-right: 30px;
}

.layui-layout-admin .layui-header {
    border-bottom-color: #d0d5da;
}

.layui-header .layui-nav-bar {
    display: none;
}

.layui-layout-admin .layui-layout-left .layui-nav-item {
    margin: 0;
}

/*消息中心*/
/*提示消息数量*/
.layui-layout-right .layui-nav-item a {
    padding: 0 30px;
}

.layui-nav .layui-nav-more {
    right: 12px;
}

.bdc-num-tips {
    background-color: #f54743;
    border-radius: 10px;
    color: #fff;
    width: 26px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    position: absolute;
    top: 4px;
    right: 8px;
    box-sizing: border-box;
}

.layui-nav .layui-nav-item img {
    padding-right: 10px;
}

.bdc-msg-nav .layui-nav-more {
    display: none;
}

.layui-layout-admin .layui-header .bdc-msg-child {
    top: 49px;
    width: 380px;
    color: #333;
    box-shadow: 0 20px 50px #e9e9e9 inset;
    padding: 0 20px;
    box-sizing: border-box;
}

.bdc-msg-child .bdc-msg-title {
    height: 60px;
    line-height: 60px;
    font-size: 18px;
    text-align: center;
    position: relative;
}

.bdc-msg-child .bdc-msg-title .layui-icon-close {
    position: absolute;
    top: 0;
    right: 18px;
    font-size: 22px;
    color: #999;
    cursor: pointer;
}

.layui-tab-brief > .layui-tab-title .layui-this {
    color: #1d87d1;
}

.bdc-msg-nav .layui-tab-brief > .layui-tab-more li.layui-this:after,
.bdc-msg-nav .layui-tab-brief > .layui-tab-title .layui-this:after {
    border-bottom-color: #1d87d1;
    bottom: 0 !important;
    top: auto !important;
}

.bdc-msg-child .layui-tab-title li {
    width: 120px;
    box-sizing: border-box;
    color: #666;
}

.bdc-msg-child .layui-tab-content {
    padding: 0;
}

.layui-layout-right .layui-nav-item .bdc-msg-child a {
    padding: 0 0 0 20px;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px dashed #d0d5da;
    box-sizing: border-box;
    position: relative;
}

.layui-nav .bdc-msg-child a:hover {
    background-color: #fff;
}

.layui-layout-admin .layui-header .layui-nav .bdc-msg-child .bdc-read a {
    color: #666;
}

.layui-layout-right .layui-nav-item .bdc-msg-child a img {
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
}

.layui-tab {
    margin: 0 0 10px 0;
}

.bdc-msg-child .layui-tab-item {
    overflow-y: auto;
}

.bdc-btn-more {
    width: 72px;
    height: 36px;
    padding: 0;
    text-align: center;
    border-color: #d0d5da;
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
}

.layui-btn-primary:hover {
    border-color: #d0d5da;
    opacity: .8;
}

/*管理员*/
.bdc-person-nav .bdc-person-child {
    padding: 0;
    background-color: #f3f4f6;
}

.bdc-person-nav .bdc-person-child dd {
    height: 40px;
    line-height: 40px;
}

.layui-nav .bdc-person-nav .bdc-person-child a:hover {
    background-color: #eaedf1;
}

/*系统切换*/
.bdc-system-child {
    width: 228px;
    background-color: #f3f4f6;
}

.layui-layout-right .bdc-system-child a {
    padding: 0 12px;
    border-left: 4px solid transparent;
}

.layui-layout-right .bdc-system-child a:hover {
    border-left-color: #1d87d1;
    background-color: transparent;
}

.layui-nav .bdc-system-child img {
    width: 24px;
    height: 24px;
    padding-right: 8px;
}

.bdc-system-child ul li {
    height: 28px;
    line-height: 28px;
    padding: 7px 0;
}

.layui-layout-admin .layui-header .layui-nav .layui-this:after,
.layui-layout-admin .layui-header .layui-nav-bar {
    height: 0;
}

/*面包屑导航*/
.breadcrumb-div {
    width: 100%;
    height: 50px;
    margin-top: 50px;
    background-color: #fff;
    line-height: 50px;
}

.layui-layout-admin .layui-layout-left {
    padding: 0 15px;
    box-sizing: border-box;
}

/*主内容区*/
.layui-layout-admin .layui-body {
    top: 100px;
}

.bdc-container {
    width: 100%;
    height: 100%;
    padding: 15px;
    box-sizing: border-box;
}

.bdc-content-box {
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    overflow-y: auto;
}

/*任务tab切*/
.bdc-task-tab {
    margin: 0;
    background-color: #fff;
    padding: 6px 15px 0;
    box-sizing: border-box;
    position: relative;
}

.bdc-container-tab > .layui-tab-more li.layui-this:after,
.bdc-container-tab > .layui-tab-title .layui-this:after {
    border-bottom-color: #1d87d1;
}

.bdc-container-tab .layui-tab-title li {
    width: 118px;
    box-sizing: border-box;
    font-size: 16px;
    color: #666;
}

.bdc-task-tab .layui-tab-title {
    border-bottom-color: #eaedf1;
}

.bdc-task-tab .bdc-task-tools {
    position: absolute;
    top: 8px;
    right: 20px;
}

.bdc-task-search-box {
    /*display: inline-block;*/
    width: 240px;
    height: 30px;
    position: relative;
    float: left;
}

.bdc-task-search-box .layui-icon-search {
    position: absolute;
    top: 50%;
    left: 14px;
    transform: translateY(-50%);
    color: #999;
    border-color: #d0d5da;
}

.bdc-task-search-box .layui-input {
    width: 100%;
    height: 100%;
    border-radius: 15px;
    padding-left: 38px;
}

.bdc-operate-show {
    display: inline-block;
    width: 70px;
    height: 30px;
    line-height: 30px;
    text-align: right;
    padding-right: 6px;
    box-sizing: border-box;
    font-weight: 700;
    color: #999;
}

.bdc-operate-show > .layui-icon {
    font-size: 24px;
    cursor: pointer;
}

/*轮播图*/
.bdc-task-tab .layui-tab-content {
    padding: 0;
}

.layui-carousel[lay-arrow=always] .layui-carousel-arrow {
    left: -26px;
    opacity: 0;
    z-index: 999;
}

.layui-carousel:hover .layui-carousel-arrow[lay-type=add],
.layui-carousel[lay-arrow=always] .layui-carousel-arrow[lay-type=add] {
    right: -26px;
    opacity: 0;
    z-index: 999;
}

.bdc-build-task {
    position: relative;
    padding: 15px 25px;
}

.bdc-build-task .layui-icon-triangle-r {
    position: absolute;
    top: 42px;
    font-size: 24px;
    color: #999;
}

.bdc-build-task .bdc-carousel-left {
    left: 4px;
    transform: rotate(180deg) translateY(12px);
}

.bdc-build-task .bdc-carousel-right {
    right: 4px;
    transform: translateY(-12px);
}

.layui-carousel[lay-indicator=outside] {
    margin-bottom: 20px;
}

.layui-carousel[lay-indicator=outside] .layui-carousel-ind ul {
    background-color: #fff;
}

.layui-carousel-ind li,
.layui-carousel-ind li:hover {
    background-color: #d0d5da;
}

.layui-carousel-ind li.layui-this {
    background-color: #1d87d1;
}

.bdc-carousel-father .bdc-carousel-son {
    display: block;
    float: left;
    width: 25%;
    height: 100%;
    padding: 0 10px 10px;
    box-sizing: border-box;
    cursor: pointer;
    position: relative;
}

.bdc-carousel-father .bdc-carousel-son:hover .bdc-carousel-title {
    color: #fff;
}

.bdc-carousel-father .bdc-carousel-son > .bdc-carousel-title {
    position: absolute;
    top: 10px;
    left: 20px;
}

.bdc-carousel-father .bdc-carousel-son img {
    width: 100%;
    height: 100%;
}

.layui-carousel > [carousel-item] > * {
    background-color: #fff;
}

/*轮播图点击内容显示*/
.layui-carousel > [carousel-item] {
    overflow: visible;
}

.bdc-carousel-details {
    width: 400%;
    padding: 24px 20px;
    position: absolute;
    top: 60px;
    left: -20px;
    border: 1px solid #d0d5da;
    background-color: #fff;
    z-index: 999;
}

.bdc-carousel-details .bdc-carousel-close {
    position: absolute;
    top: 5px;
    right: 5px;
    font-size: 18px;
    font-weight: 700;
    padding: 10px;
    color: #666;
    z-index: 99;
}

.bdc-details-type {
    width: 100%;
    box-sizing: border-box;
    padding: 10px 300px 10px 110px;
    position: relative;
    line-height: 30px;
    border-bottom: 1px dashed #d0d5da;
}

.bdc-details-type:last-child {
    border-bottom: none;
}

.bdc-details-type-content {
    overflow-y: hidden;
}

.bdc-details-type-content a {
    display: block;
    float: left;
    width: 25%;
    height: 30px;
}

.bdc-details-type-content a:hover {
    color: #1d87d1;
}

.bdc-details-type-content a > .layui-icon {
    padding-left: 8px;
    color: #1d87d1;
}

.bdc-details-type-name {
    position: absolute;
    top: 10px;
    left: 0;
    font-weight: 700;
}

.bdc-details-type-tool {
    position: absolute;
    top: 10px;
    right: 224px;
    color: #1d87d1;
}

.bdc-details-type-tool:hover {
    color: #1d87d1;
}

/*常用收藏*/
.bdc-collection-tab {
    padding: 15px 0;
}

.bdc-collection-content {
    line-height: 42px;
    height: 84px;
    overflow: hidden;
}

.bdc-collection-content > a {
    width: 20%;
    height: 42px;
    float: left;
}

.bdc-collection-content > a:hover {
    color: #1d87d1;
}

.bdc-collection-content > a > .layui-icon {
    color: #1d87d1;
    padding-left: 8px;
}

/*暂无收藏内容*/
.bdc-no-collection {
    position: relative;
}

.bdc-no-collection > img {
    display: block;
    margin: 0 auto;
}

.bdc-no-collection > span {
    line-height: 20px;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
}

/*任务列表*/
.bdc-table-box {
    padding: 15px 0;
}

.bdc-list-tab {
    padding: 5px 15px 0;
    margin: 15px 0 0 0;
    background-color: #fff;
    box-sizing: border-box;
}

.bdc-list-num-tips {
    display: inline-block;
    background-color: #f54743;
    border-radius: 10px;
    color: #fff;
    width: 26px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    box-sizing: border-box;
    margin-left: 12px;
}

.bdc-list-tab .layui-tab-title li {
    padding: 0;
}

.bdc-list-tab .layui-tab-content {
    padding: 0;
}

.bdc-tab-search {
    padding: 15px 0;
    height: 68px;
    border-bottom: 1px dashed #d0d5da;
    box-sizing: border-box;
}

.bdc-tab-search .layui-btn {
    margin-left: 7px;
}

.bdc-table-name {
    text-align: left;
}

.bdc-table-name-pd {
    padding-left: 10px;
    text-align: left;
}

.bdc-table-name > span {
    color: #f54743;
}

.bdc-table-lc {
    color: #1d87d1;
}

.bdc-table-lc > img {
    margin-right: 10px;
}

.bdc-table-state-blz {
    color: #1d87d1;
}

.bdc-table-state-th {
    color: #f54743;
}

.bdc-table-state-zt {
    color: #f49c67;
}