.bdc-percentage-container{
    width: 100%;
    height: 100%;
    padding: 103px 0 0;
    box-sizing: border-box;
    position: relative;
    background-color:transparent;
}
.bdc-line-search-container{
    padding-top: 61px;
}
.layui-form-item .layui-inline {
    margin-top: 8px;
}

.layui-tab-item .bdc-table-box {
    padding: 0 0 10px 0;
}
.dataPic .layui-tab{
    padding: 8px;
    position: relative;
    height:100%;
}
.bdc-search-content{
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    padding: 0;
    box-sizing: border-box;
}

/*百分比布局修改*/
.bdc-form-div .layui-form{
    min-height: auto;
}

.bdc-table-box .layui-form{
    height: auto;
}

.bdc-tab-box{
    background-color: #fff;
    width: 100%;
    min-height: 100%;
    margin: 0;
    padding: 0 10px;
    height: 100%;
    box-sizing: border-box;
}
.layui-tab{
    padding: 47px 0 0 0;
    position: relative;
    height:100%;
}
.layui-tab-title{
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
}
.layui-tab-content{
    height: 100%;
}
.layui-tab-item{
    height: 100%;
}

.nosearch {
    width: 100%;
    height: 100%;
    padding: 0 0 0 !important;
    box-sizing: border-box;
    position: relative;
    background-color: #eaedf1;
}

/*下拉框删除图标样式*/
.layui-form-item .xzq-sele .layui-input-inline .layui-icon-close {
    right: 23px;

}

/*购物车样式调整*/
@media screen and (max-width: 1400px) {
    .layui-layer.bdc-gwc-layer{
        top: 60px!important;
    }
}

/*整行显色时字体颜色修改*/
tr .bdc-special-bdcdyh-color span{
    color: #333!important;
}

.change-htjgxx-padding{
    padding-top: 60px;
}

