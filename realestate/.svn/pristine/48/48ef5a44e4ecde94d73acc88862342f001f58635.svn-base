body,html{
    font-family: "Microsoft YaHei", Helvetica, Arial, sans-serif !important;
    font-size: 14px;
    width: 100%;
    height: 100%;
}
html{
    background-color: #eaedf1;
}
.bdc-hide{
    display: none;
}
.bdc-visible{
    visibility: hidden;
}
.clear:after {
    content: '';
    display: block;
    clear: both;
}

/*侧边栏*/
.layadmin-setTheme-side,
.layui-side-menu{
    background-color: #045085;
}
.layui-layout-admin .layui-logo{
    background-color: #045085;
    box-shadow:none;
    font-weight: 400;
    padding: 0 0 0 34px;
    text-align: left;
}
.layui-nav-tree .layui-nav-child dd.layui-this,
.layui-nav-tree .layui-nav-child dd.layui-this a,
.layui-nav-tree .layui-this,
.layui-nav-tree .layui-this>a,
.layui-nav-tree .layui-this>a:hover{
    background-color: #1d87d1;
}
.layui-layout-admin .layui-logo,
.layui-layout-admin .layui-logo a{
    color: rgba(255,255,255,1);
}
.layui-layout-admin .layui-logo .bdc-logo-img{
    position: absolute;
    top: 50%;
    left: 6px;
    transform: translateY(-50%);
}
.layadmin-side-shrink .layui-layout-admin .layui-logo .bdc-logo-img{
    display: none;
}
.layadmin-side-shrink .layui-layout-admin .layui-logo{
    background-position-x: 12px;
}
.layui-side-menu .layui-nav .layui-nav-itemed>.layui-nav-child{
    padding: 0;
    background-color: #07436e;
}
.layui-header .layui-icon {
    display: block;
    width: 16px;
    height: 16px;
    background-size: cover;
    font-weight: 700;
}
/*一级菜单*/
.layui-side-menu .layui-nav .layui-nav-item>a{
    height: 50px;
    line-height: 50px;
    padding-left: 50px;
    position: relative;
    padding-top: 0;
    padding-bottom: 0;
}
.layui-side-menu .layui-nav .layui-nav-item .layui-icon{
    left: 12px;
    transform: translateY(-50%);
    margin-top: 0;
}

/*二级菜单*/
.layui-side-menu .layui-nav .layui-nav-child dd a{
    height: 40px;
    line-height: 40px;
    font-size: 12px;
    padding-left: 60px;
}
/*三级菜单*/
.layui-side-menu .layui-nav .layui-nav-child .layui-nav-child a{
    padding-left: 70px;
}

/*四级菜单*/
.slide-level-four{
    position: absolute;
    left: 230px;
    top: 10px;
    background-color: #fff;
    z-index: 999999;
    display: none;
    box-shadow:#eaedf1 0 4px 30px 0;
}

.slide-level-four dd{
    padding: 0 30px 0 20px;
    height: 30px;
    line-height: 30px;
    color: #666;
    font-size: 12px;
}
.slide-level-four dd:hover{
    background-color: #eaedf1;
    color: #333;
}
/*侧边栏小图标*/
.layui-nav-tree .layui-nav-bar{
    background-color: #1d87d1;
}
.layui-side-menu .layui-icon{
    height: 24px;
    width: 24px;
    background-size:cover ;
}
.layui-side-menu .layui-nav .layui-icon>img{
    padding-right: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}
.bdc-icon-mine{
    background:url(../image/icon-1.png) no-repeat 0 0 ;
}
.layui-this .bdc-icon-mine,
.layui-nav-itemed .bdc-icon-mine,
.layui-nav-item:hover .bdc-icon-mine{
    background:url(../image/icon-2.png) no-repeat 0 0 ;
}

.bdc-icon-task{
    background:url(../image/icon-1.png) no-repeat  0 -24px;
}
.layui-this .bdc-icon-task,
.layui-nav-itemed .bdc-icon-task,
.layui-nav-item:hover .bdc-icon-task{
    background:url(../image/icon-2.png) no-repeat  0 -24px;
}

.bdc-icon-comprehensive{
    background:url(../image/icon-1.png) no-repeat 0 -48px;
}
.layui-this .bdc-icon-comprehensive,
.layui-nav-itemed .bdc-icon-comprehensive,
.layui-nav-item:hover .bdc-icon-comprehensive{
    background:url(../image/icon-2.png) no-repeat 0 -48px;
}

.bdc-icon-build{
    background:url(../image/icon-1.png) no-repeat 0 -72px;
}
.layui-this .bdc-icon-build,
.layui-nav-itemed .bdc-icon-build,
.layui-nav-item:hover .bdc-icon-build{
    background:url(../image/icon-2.png) no-repeat 0 -72px;
}

.bdc-icon-supply{
    background:url(../image/icon-1.png) no-repeat 0 -96px;
}
.layui-this .bdc-icon-supply,
.layui-nav-itemed .bdc-icon-supply,
.layui-nav-item:hover .bdc-icon-supply{
    background:url(../image/icon-2.png) no-repeat 0 -96px;
}

.bdc-icon-supervise{
    background:url(../image/icon-1.png) no-repeat 0 -120px;
}
.layui-this .bdc-icon-supervise,
.layui-nav-itemed .bdc-icon-supervise,
.layui-nav-item:hover .bdc-icon-supervise{
    background:url(../image/icon-2.png) no-repeat 0 -120px;
}

.bdc-icon-save{
    background:url(../image/icon-1.png) no-repeat 0 -144px;
}
.layui-this .bdc-icon-save,
.layui-nav-itemed .bdc-icon-save,
.layui-nav-item:hover .bdc-icon-save{
    background:url(../image/icon-2.png) no-repeat 0 -144px;
}

/*动态渲染图标*/
.bdc-icon1{
    background:url(../image/icon-1.png) no-repeat 0 0;
}
.layui-this .bdc-icon1,
.layui-nav-itemed .bdc-icon1,
.layui-nav-item:hover .bdc-icon1{
    background:url(../image/icon-2.png) no-repeat 0 0;
}
.bdc-icon2{
    background:url(../image/icon-1.png) no-repeat 0 -24px;
}
.layui-this .bdc-icon2,
.layui-nav-itemed .bdc-icon2,
.layui-nav-item:hover .bdc-icon2{
    background:url(../image/icon-2.png) no-repeat 0 -24px;
}
.bdc-icon3{
    background:url(../image/icon-1.png) no-repeat 0 -48px;
}
.layui-this .bdc-icon3,
.layui-nav-itemed .bdc-icon3,
.layui-nav-item:hover .bdc-icon3{
    background:url(../image/icon-2.png) no-repeat 0 -48px;
}
.bdc-icon4{
    background:url(../image/icon-1.png) no-repeat 0 -72px;
}
.layui-this .bdc-icon4,
.layui-nav-itemed .bdc-icon4,
.layui-nav-item:hover .bdc-icon4{
    background:url(../image/icon-2.png) no-repeat 0 -72px;
}
.bdc-icon5{
    background:url(../image/icon-1.png) no-repeat 0 -96px;
}
.layui-this .bdc-icon5,
.layui-nav-itemed .bdc-icon5,
.layui-nav-item:hover .bdc-icon5{
    background:url(../image/icon-2.png) no-repeat 0 -96px;
}
.bdc-icon6{
    background:url(../image/icon-1.png) no-repeat 0 -120px;
}
.layui-this .bdc-icon6,
.layui-nav-itemed .bdc-icon6,
.layui-nav-item:hover .bdc-icon6{
    background:url(../image/icon-2.png) no-repeat 0 -120px;
}
.bdc-icon7{
    background:url(../image/icon-1.png) no-repeat 0 -144px;
}
.layui-this .bdc-icon7,
.layui-nav-itemed .bdc-icon7,
.layui-nav-item:hover .bdc-icon7{
    background:url(../image/icon-2.png) no-repeat 0 -144px;
}
.bdc-icon8{
    background:url(../image/icon-1.png) no-repeat 0 -168px;
}
.layui-this .bdc-icon8,
.layui-nav-itemed .bdc-icon8,
.layui-nav-item:hover .bdc-icon8{
    background:url(../image/icon-2.png) no-repeat 0 -168px;
}
.bdc-icon9{
    background:url(../image/icon-1.png) no-repeat 0 -192px;
}
.layui-this .bdc-icon9,
.layui-nav-itemed .bdc-icon9,
.layui-nav-item:hover .bdc-icon9{
    background:url(../image/icon-2.png) no-repeat 0 -192px;
}



/*收缩后系统图标*/
.set-layui-icon-shrink-right{
    background: url(../image/arrow.png) no-repeat 0 0;
    background-size:cover;
    margin-top: 16px;
    transition: all 0.1s;
    -moz-transition: all 0.1s;
    -webkit-transition: all 0.1s;
    -o-transition: all 0.1s;
}
/*收缩后设置*/
.layadmin-side-shrink .layui-layout-admin .layui-logo{
    width: 50px;
}
.layadmin-side-shrink .layui-side{
    width: 50px;
}
.layadmin-side-shrink .layadmin-pagetabs,
.layadmin-side-shrink .layui-layout-admin .layui-body,
.layadmin-side-shrink .layui-layout-admin .layui-footer,
.layadmin-side-shrink .layui-layout-admin .layui-layout-left{
    left: 50px;
}


/*右上方*/
.ul-margin-right{
    margin-right: 30px;
}
.layui-layout-admin .layui-header{
    /*border-bottom-color: #d0d5da;*/
    border-bottom: none;
}
.layui-header .layui-nav-bar{
    display: none;
}
.layui-layout-admin .layui-layout-left .layui-nav-item{
    margin: 0;
}
/*消息中心*/
/*提示消息数量*/
.layui-layout-right .layui-nav-item a{
    padding: 0 30px 0 0;
}
.ul-margin-right.layui-nav .layui-nav-item{
    margin-left: 30px;
}
.ul-margin-right.layui-nav .bdc-person-nav dl a{
    padding: 0 15px;
}
.layui-nav .layui-nav-more{
    right: 12px;
}
.bdc-num-tips{
    background-color: #f54743;
    border-radius: 10px;
    color: #fff;
    width: 26px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    position: absolute;
    top: 4px;
    right: 8px;
    box-sizing: border-box;
}
.layui-nav .layui-nav-item img{
    padding-right: 10px;
}
.bdc-msg-nav .layui-nav-more{
    display: none;
}
.bdc-msg-nav .layui-tab-title{
    height: 34px;
}
.layui-layout-admin .layui-header .bdc-msg-child{
    top: 49px;
    width: 380px;
    color: #333;
    box-shadow: 0 20px 50px #e9e9e9 inset;
    padding: 0 20px;
    box-sizing: border-box;
}
.bdc-msg-child .bdc-msg-title{
    height: 60px;
    line-height: 60px;
    font-size: 18px;
    text-align: center;
    position: relative;
}
.bdc-msg-child .bdc-msg-title .layui-icon-close{
    position: absolute;
    top: 0;
    right: 18px;
    font-size: 22px;
    color: #999;
    cursor: pointer;
}
.layui-tab-brief>.layui-tab-title .layui-this{
    color: #1d87d1;
}
.bdc-msg-nav .layui-tab-brief>.layui-tab-more li.layui-this:after,
.bdc-msg-nav .layui-tab-brief>.layui-tab-title .layui-this:after{
    border-bottom-color: #1d87d1;
    bottom: 0!important;
    top: auto!important;
}
.bdc-msg-child .layui-tab-title li{
    width: 120px;
    box-sizing: border-box;
    color: #666;
}
.bdc-msg-child .layui-tab-content{
    padding: 0;
}
.layui-layout-right .layui-nav-item .bdc-msg-child a{
    padding: 0;
    height: 50px;
    line-height: 50px;
    box-sizing: border-box;
    position: relative;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
}
.layui-nav .bdc-msg-child a:hover{
    background-color: #fff;
}
.layui-layout-admin .layui-header .layui-nav .bdc-msg-child .bdc-read a{
    color: #666;
}
.layui-layout-right .layui-nav-item .bdc-msg-child a img{
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
}
.layui-tab{
    margin: 0 0 10px 0;
}
.bdc-msg-child .layui-tab-item{
    overflow-y: auto;
}
.bdc-msg-child .layui-tab-item li{
    position: relative;
    padding: 0 20px 0 10px;
    border-bottom: 1px dashed #d0d5da;
}
.bdc-msg-child .layui-tab-item li .layui-icon-close{
    position: absolute;
    top: 10px;
    right: 5px;
    cursor: pointer;
}
.bdc-btn-more{
    width: 72px;
    height: 36px;
    padding: 0;
    text-align: center;
    border-color: #d0d5da;
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
}
.layui-btn-primary:hover{
    border-color: #d0d5da;
    opacity: .8;
}

/*管理员*/
.bdc-person-nav .bdc-person-child{
    padding: 0;
    background-color: #f3f4f6;
}
.bdc-person-nav .bdc-person-child dd{
    height: 40px;
    line-height: 40px;
}
.layui-nav .bdc-person-nav .bdc-person-child a:hover {
    background-color: #eaedf1;
}

/*系统切换*/
.bdc-system-child{
    width: 218px;
    background-color: #f3f4f6;
}
.layui-layout-right .bdc-system-child a{
    padding: 0 12px;
    border-left: 4px solid transparent;
}
.layui-layout-right .bdc-system-child a:hover{
    border-left-color: #1d87d1;
    background-color: transparent;
}
.layui-nav .bdc-system-child img{
    width: 24px;
    height: 24px;
    padding-right: 8px;
}
.bdc-system-child ul li{
    height: 28px;
    line-height: 28px;
    padding: 7px 0;
}
.layui-layout-admin .layui-header .layui-nav .layui-this:after,
.layui-layout-admin .layui-header .layui-nav-bar{
    height: 0;
}

/*tab切换*/
.layadmin-pagetabs .layui-tab{
    position: relative;
    top: -1px;
}
.layadmin-pagetabs .layui-tab-title li,
.layadmin-pagetabs .layui-icon-prev{
    border-right-color: #d0d5da;
}
.layadmin-pagetabs .layadmin-tabs-control{
    border-left-color: #d0d5da;
}
.layadmin-pagetabs .layui-tab-title li.layui-this{
    background-color: #045085;
    color: #fff;
}
.layadmin-pagetabs .layui-tab-title li:hover{
    background-color: #f3f4f6;
    border-top: 1px solid #d0d5da;
}
.layadmin-pagetabs .layui-tab-title li.layui-this:hover{
    background-color: #045085;
}
.layadmin-pagetabs .layui-tab-title li:after{
    height: 0;
}
.layadmin-pagetabs .layui-tab-title li.layui-this:after{
    height: 0;
}
.layadmin-pagetabs{
    border-bottom: 1px solid #045085;
    border-top: 1px solid #d0d5da;
}
.layadmin-pagetabs .layui-tab-title li{
    line-height: 39px;
    border-top: 1px solid transparent;
}
.layadmin-pagetabs .layui-icon-set-sm{
    right: 0;
    font-size: 0;
}
.layadmin-pagetabs .layui-icon-set-sm>img{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}
.layadmin-pagetabs .layui-tab-title{
    height: 39px;
}


/*主内容区*/
.bdc-container{
    width: 100%;
    height: calc( 100% - 20px );
    padding: 10px;
    box-sizing: border-box;
    overflow-y: auto;
    background-color: #eaedf1;
}
.bdc-content-box{
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    /*overflow-y: auto;*/
}
/*任务tab切*/
.bdc-task-tab{
    margin: 0;
    background-color: #fff;
    padding: 0 10px;
    box-sizing: border-box;
    position: relative;
}
.bdc-container-tab>.layui-tab-more li.layui-this:after,
.bdc-container-tab>.layui-tab-title .layui-this:after{
    border-bottom-color: #1d87d1;
}
.bdc-container-tab .layui-tab-title li{
    width: 140px;
    box-sizing: border-box;
    font-size: 16px;
    color: #666;
}
.bdc-container-tab .layui-tab-title li.bdc-word-li{
    width: 180px;
}
.bdc-task-tab .layui-tab-title{
    border-bottom-color: #eaedf1;
    height: 34px;
    padding-top: 10px;
}
.bdc-task-tab .bdc-task-tools{
    position: absolute;
    top: 7px;
    right: 20px;
    width: 310px;
    padding-right: 70px;
    box-sizing: border-box;
}
.bdc-task-search-box{
    width: 100%;
    height: 30px;
    position: relative;
}
.bdc-task-search-box .layui-icon-search{
    position: absolute;
    top: 50%;
    left: 14px;
    transform: translateY(-50%);
    color: #999;
    border-color: #d0d5da;
}
.bdc-task-search-box .layui-icon-close{
    position: absolute;
    top: 50%;
    right: 14px;
    transform: translateY(-50%);
    color: #f54743;
    border-color: #d0d5da;
    cursor: pointer;
    font-weight: 700;
}
.bdc-task-search-box .layui-input{
    width: 100%;
    height: 100%;
    border-radius: 15px;
    padding: 0 38px;
}
.bdc-operate-show{
    position: absolute;
    top: 0;
    right: 0;
    width: 70px;
    height: 30px;
    line-height: 30px;
    text-align: right;
    padding-right: 6px;
    box-sizing: border-box;
    font-weight: 700;
    color: #999;
}
.bdc-operate-show>.layui-icon{
    font-size: 24px;
    cursor: pointer;
}
/*轮播图*/
.bdc-task-tab .layui-tab-content{
    width: 100%;
    padding: 0;
}
.layui-carousel[lay-arrow=always] .layui-carousel-arrow{
    left: -26px;
    opacity: 0;
    z-index: 999;
}
.layui-carousel:hover .layui-carousel-arrow[lay-type=add],
.layui-carousel[lay-arrow=always] .layui-carousel-arrow[lay-type=add]{
    right: -26px;
    opacity: 0;
    z-index: 999;
}
.bdc-build-task{
    position: relative;
    padding: 5px 5px;
}
.bdc-build-task .layui-icon-triangle-r{
    position: absolute;
    top: 52px;
    font-size: 24px;
    color: #999;
}
.bdc-build-task .bdc-carousel-left{
    left: -15px;
    transform: rotate(180deg) translateY(12px);
}
.bdc-build-task .bdc-carousel-right{
    right: -15px;
    transform: translateY(-12px);
}
.layui-carousel[lay-indicator=outside]{
    margin-bottom: 6px;
}
.layui-carousel[lay-indicator=outside] .layui-carousel-ind{
    top: -6px;
}
.layui-carousel[lay-indicator=outside] .layui-carousel-ind ul{
    background-color: #fff;
}
.layui-carousel-ind li,
.layui-carousel-ind li:hover{
    background-color: #d0d5da;
}
.layui-carousel-ind li.layui-this{
    background-color: #1d87d1;
}
.bdc-carousel-father .bdc-carousel-son{
    display: block;
    float: left;
    width: 16.66%;
    height: 50px;
    padding: 5px;
    box-sizing: border-box;
    cursor: pointer;
    position: relative;
}
.bdc-carousel-father .bdc-carousel-son.bdc-carousel-son-this .bdc-carousel-title{
    color: #fff;
    background-color: #1d87d1;
}
.bdc-carousel-father .bdc-carousel-son.bdc-carousel-son-this .bdc-carousel-default-img{
    display: none;
}
.bdc-carousel-father .bdc-carousel-son.bdc-carousel-son-this .bdc-carousel-select-img{
    display: block;
}
.bdc-carousel-father .bdc-carousel-son>.bdc-carousel-title{
    width: 100%;
    height: 100%;
    color: #333;
    line-height: 40px;
    background-color: #eaedf1;
    padding-left: 50px;
    box-sizing: border-box;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.bdc-carousel-father .bdc-carousel-son .bdc-sprite-img{
    position: absolute;
    top: 15px;
    left: 25px;
    width: 20px;
    height: 20px;
    background: url(../image/task.png) no-repeat;
}
.bdc-carousel-father .bdc-carousel-son>.bdc-carousel-title:hover{
    background-color: #1d87d1;
    color: #fff;
}
/*新建任务图标*/
.bdc-carousel-father .bdc-carousel-son:first-child .bdc-sprite-img{
    background-position: 0 0;
}
.bdc-carousel-father .bdc-carousel-son:first-child>.bdc-carousel-title:hover .bdc-sprite-img{
    background-position: -20px 0;
}
.bdc-carousel-father .bdc-carousel-son.bdc-carousel-son-this:first-child>.bdc-carousel-title .bdc-sprite-img{
    background-position: -20px 0;
}
.bdc-carousel-father .bdc-carousel-son:nth-child(2) .bdc-sprite-img{
    background-position: 0 -20px;
}
.bdc-carousel-father .bdc-carousel-son:nth-child(2)>.bdc-carousel-title:hover .bdc-sprite-img{
    background-position: -20px -20px;
}
.bdc-carousel-father .bdc-carousel-son.bdc-carousel-son-this:nth-child(2)>.bdc-carousel-title .bdc-sprite-img{
    background-position: -20px -20px;
}
.bdc-carousel-father .bdc-carousel-son:nth-child(3) .bdc-sprite-img{
