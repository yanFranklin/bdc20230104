/*轮播图点击内容显示*/
.layui-carousel>[carousel-item]{
    overflow: visible;
}
.bdc-carousel-details{
    width: 600%;
    padding: 10px 30px 10px 10px;
    position: absolute;
    top: 58px;
    left: -20px;
    border: 1px solid #d0d5da;
    background-color: #fff;
    z-index: 999;
    box-shadow: 0 0 15px #999;
    box-sizing: border-box;
}
.bdc-carousel-details .bdc-carousel-close{
    position: absolute;
    top: 5px;
    right: 5px;
    font-size: 18px;
    font-weight: 700;
    padding: 10px;
    color: #666;
    z-index: 99;
}
.bdc-details-type{
    width: 100%;
    box-sizing: border-box;
    /*padding: 10px 300px 10px 110px;*/
    padding: 10px 70px 10px 10px;
    position: relative;
    line-height: 30px;
    border-bottom: 1px dashed #d0d5da;
}
.bdc-details-type:last-child{
    border-bottom: none;
}
.bdc-details-type-content{
    overflow-y: hidden;
}
.bdc-details-type-content a{
    display: block;
    float: left;
    width: 25%;
    height: 30px;
    padding: 0 10px 0 0;
    box-sizing: border-box;
    font-size: 14px;
}
.bdc-details-type-content a>span{
    max-width: 88%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    float: left;
}
.bdc-details-type-content a:hover{
    color: #1d87d1;
}
.bdc-details-type-content a>.layui-icon{
    padding-left: 8px;
    color: #1d87d1;
    width: 14px;
    height: 14px;
}
.bdc-details-type-name{
    position: absolute;
    top: 10px;
    left: 0;
    font-weight: 700;
}
.bdc-details-type-tool{
    position: absolute;
    top: 10px;
    right: 20px;
    color: #1d87d1;
}
.bdc-details-type-tool:hover{
    color: #1d87d1;
}
/*常用收藏*/
.bdc-collection-tab{
    width: 100%;
    box-sizing: border-box;
    padding: 0 50px 5px 10px;
    position: relative;
    overflow: hidden;
}
.bdc-show-more-collection{
    position: absolute;
    top: 15px;
    right: 0;
    color: #1d87d1;
    cursor: pointer;
}
.bdc-collection-content{
    width: 100%;
    line-height: 36px;
    min-height: 72px;
    height: 72px;
    overflow: hidden;
    position: relative;
}
.bdc-collection-content>li{
    width: 25%;
    height: 36px;
    float: left;
    font-size: 14px;
    box-sizing: border-box;
    cursor: move;
    padding: 0 10px;
}
.bdc-collection-content>li a{
    color: #333;
    padding: 0 10px 0 5px;
}
.bdc-collection-content>li:hover a{
    color: #1d87d1;
}
.bdc-collection-content .active a{
    background: #eaf4fe;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
    filter: alpha(opacity=80);
}
.bdc-collection-content>li .layui-icon{
    color: #1d87d1;
    /*padding-left: 8px;*/
    cursor: pointer;
    padding: 5px 8px;
    margin-left: 5px;
}
/*暂无收藏内容*/
