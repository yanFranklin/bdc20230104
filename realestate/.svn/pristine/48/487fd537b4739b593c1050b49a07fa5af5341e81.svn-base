<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <title>收件量统计查询</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/search.css" />
    <link rel="stylesheet" href="../../static/lib/bdcui/css/table.css" />
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css" />
    <link rel="stylesheet" href="../../static/lib/registerui/css/percentage-layout.css"/>
    <link rel="stylesheet" href="../../static/lib/form-select/formSelects-v4.css">
    <link rel="stylesheet" href="../../static/css/common.css">
</head>
<body>
<div class="bdc-percentage-container tssfxx">
    <!--查询条件-->
    <div class="bdc-search-content">
        <div class="bdc-search-box">
            <!--默认两行的-->
            <form class="layui-form" action="" lay-filter="searchform">
                <div class="layui-form-item pf-form-item">
                    <div class="layui-inline">
                        <label id="slbhLabel"  class="layui-form-label">受理编号</label>
                        <div id="slbhDiv"  class="layui-input-inline">
                            <input id="slbh" name="slbh" type="text" autocomplete="off" placeholder="请输入" class="layui-input search">
                            <i class="layui-icon layui-icon-close bdc-hide"></i>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label id="kssjLabel"  class="layui-form-label">开始日期</label>
                        <div id="kssjDiv"  class="layui-input-inline">
                            <input type="text" id="beginTime" name="kssj" class="layui-input search" placeholder="请选择">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label id="jssjLabel"  class="layui-form-label">结束时间</label>
                        <div id="jssjDiv"  class="layui-input-inline">
                            <input type="text" id="endTime" name="jzsj" class="layui-input search" placeholder="请选择">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label">部门</label>
                        <div class="layui-input-inline bdc-radio">
                            <select class="bdc-jgry bdc-bmmc" name="djjg" id = "djjg" xm-select="selectBmmc" xm-select-radio xm-select-height="34px">
                            </select>
                        </div>
                    </div>
                    <div class="layui-inline bdc-button-box">
                        <a href="javascript:;" class="layui-btn layui-btn-normal bdc-major-btn" id="search">查询</a>
                        <button type="reset" class="layui-btn layui-btn-normal bdc-secondary-btn" id="reset">重置</button>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <!--表格-->
    <div class="bdc-table-box">
        <table id="pageTable" lay-filter="pageTable"></table>
    </div>

    <!--表格上面功能按钮-->
    <script type="text/html" id="toolbarDemo">
        <div class="layui-btn-container">
            <button class="layui-btn layui-btn-sm bdc-major-btn" lay-event="exportExcel" id="exportExcel">导出Excel</button>
            <span style="font-size:14px;padding-left: 50px ;">合计： <i id="count" style="padding-left: 10px ;color:blue;font-style:normal"></i></span>
        </div>
    </script>

    <script src="../../static/lib/jquery/jquery.min.js"></script>
    <script src="../../static/lib/layui/layui.js"></script>
    <script src="../../static/lib/bdcui/js/common.js"></script>
    <script src="../../static/js/common.js"></script>
    <script src="../../static/js/commonSearch.js"></script>
    <script src="../../static/js/formCenter.js"></script>
    <script src="../../static/js/yancheng/count/sjltj.js"></script>
</div>
<!--隐藏form，为了Excel导出缓存数据使用-->
<form style="display: none" action="/realestate-inquiry-ui/excel/export" method="POST" id="form">
    <input type="hidden" id="fileName"   name="fileName"/>
    <input type="hidden" id="sheetName"  name="sheetName"/>
    <input type="hidden" id="cellTitle"  name="cellTitle"/>
    <input type="hidden" id="cellWidth"  name="cellWidth"/>
    <input type="hidden" id="cellKey"    name="cellKey"/>
    <input type="hidden" id="data"       name="data"/>
    <input type="hidden" id="totalInfo"  name="totalInfo"/>
    <input type="hidden" id="expandServiceName"  name="expandServiceName"/>
</form>
</body>
</html>