

.dc-btn,
.print-btn {
    padding: 0 5px 0 8px;
}

.max-width{
    width: 950px !important;
}

.layui-icon-print {
    display: block;
    width: 20px;
    height: 20px;
    background: url(../image/printing.png) no-repeat 0 0;
    background-size: cover;
}

.layui-icon-dc {
    display: block;
    width: 20px;
    height: 22px;
    background: url(../image/dc.png) no-repeat 0 0;
    background-size: cover;
}

.page-operate-parent {
    width: 100%;
    background-color: #fff;
    z-index: 9999;
    position: fixed;
    left: 0;
    top: 0;
    height: 40px;
}

.page-operate {
    margin: 0 20px;
    padding: 10px 0 !important;
    border-bottom: 1px solid #eaedf1;
    height: 40px;
    background-color: #fff;
}

.page-operate p {
    font-size: 20px;
    color: #333;
    float: left;
    line-height: 38px;
}

.page-operate .title-btn-area {
    float: right;
    position: relative;
}

.main-container {
    padding: 0 10px;
    min-width: 1000px;
    position: relative;
    margin-top: 61px;
}


.table-module {
    width: 100%;
    height: 100%;
    padding: 15px !important;
    box-sizing: border-box;
    padding: 15px 0;
}

.new-add {
    display: block;
    position: absolute;
    background-color: #fff;
    border: 1px solid #d0d5da;
    top: 44px;
    left: 105px;
    display: none;
}
.print {
    display: block;
    position: absolute;
    background-color: #fff;
    border: 1px solid #d0d5da;
    top: 44px;
    left: 105px;
    display: none;
}

.new-add li {
    width: 250px;
    padding: 0 14px 0 14px;
    height: 40px;
    line-height: 40px;
    font-size: 14px;
    color: #999;
}
.print li {
    width: 250px;
    padding: 0 14px 0 14px;
    height: 40px;
    line-height: 40px;
    font-size: 14px;
    color: #999;
}

.new-add li:hover {
    background-color: #f3f4f6;
    color: #333;
}
.print li:hover {
    background-color: #f3f4f6;
    color: #333;
}

.layui-form-label {
    width: 150px !important;
}

.pannel-container .layui-form-item .layui-input-inline {
    float: left;
    width: 190px;
    margin-right: 10px;
}

.data-row {
    padding: 20px 0;
    border-bottom: 1px dotted #eaedf1;
    background: #fff;
    clear: both;
    position: relative;
}

.data-row .row-title {
    height: 30px;
    position: relative;
    width: 100%;
    clear: both;
}

.data-row .row-title p {
    font-size: 16px;
    color: #333;
    height: 16px;
    border-left: 4px solid #1d87d1;
    margin-left: 15px;
    padding-left: 10px;
    line-height: 16px;
}

.data-row .row-title .row-operate {
    position: absolute;
    right: 15px;
    top: -13px;
}

.data-row .row-info {
    float: left;
    margin: 5px 0;
}

.data-row .table-row {
    margin: 0 15px;
    padding-bottom: 15px;
    position: relative;
}

.pannel-container {
    margin: 30px 0px 0;
}

.pannel-container .pannel-form-btns {
    margin: 30px 0;
    text-align: center;
}

.pannel-padding {
    padding: 0 15px;
}
.layui-form-item .layui-inline {
    width: 33%;
    margin-bottom: 0;
    margin-top: 15px;
    margin-right: 0;
}

.change-textarea-margin .layui-input-inline {
    width: 90.5%;
}
.change-textarea-margin {
    margin-top: 15px;
}

.layui-input-inline .fa-ban {
    position: absolute;
    top: 50%;
    right: 10px;
    transform: translateY(-50%);
    color: #f54743;
    font-size: 18px;
}

.sl-from table td{
    text-align: center;
}