<!--常州证书实际样式预览-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <title>不动产证书（预览）</title>
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css">
    <link rel="stylesheet" href="../../static/css/zsxx/zsView.css">
    <style type="text/css">
        .layui-table td, .layui-table th {
            position: relative;
            padding: 9px 15px;
            min-height: 8px;
            line-height: 16px;
            font-size: 14px;
        }

        .layui-table {
            width: 100%;
        }

        #dklb  td {
            border: none;
            height: 8px;
            font-size: 14px;
            padding: 5px 5px;
            color: #333;
        }

        #dklb  th {
            font-size: 14px;
            padding: 5px 5px;
        }

        .bdc-notes textarea {
            width: 100%;
            height: auto;
            border: none;
            color: #333;
            padding: 6px 15px;
        }

        textarea {
            font-size: 14px;
        }
        #jtcy  td {
            border: none;
            height: 8px;
            font-size: 14px;
            padding: 5px 5px;
            color: #333;
        }

        #jtcy  th {
            font-size: 14px;
            padding: 5px 5px;
        }
    </style>
</head>
<body>
<div class="layui-carousel" id="zsView" style="margin: 0 auto">
    <div carousel-item>
        <!--封面页-->
        <div>
            <form class="layui-form setOverflow" action="" lay-filter="coverform">
                <div id="cover" class="clear cover-background-img">
                    <div class="bdc-details-left">
                    </div>

                    <!--右侧-->
                    <div class="bdc-details-right">
                        <p class="bdc-notes-title"></p>
                        <div class="bdc-notes" style="height: 485px">
                            <div id="qrcode" class="qrcodeDiv" style=""></div>
                        </div>
                        <div class="bdc-notes-top">
                            <span class="bdc-notes-top-content">
                               <span><input type="text" class="layui-input" placeholder="" value="" name="szrqYear" id="szrqYear" readonly="readonly" style="margin-right: 5px"></span>
                               <span><input type="text" class="layui-input" placeholder="" value="" name="szrqMonth" id="szrqMonth" readonly="readonly"></span>
                               <span><input type="text" class="layui-input" placeholder="" value="" name="szrqDay" id="szrqDay" readonly="readonly"></span>
                           </span>
                        </div>
                        <div class="bdc-notes-top qzysxlh-div">
                            <span class="bdc-notes-top-content">
                               <span><input type="text" class="layui-input" placeholder="" value="" name="qzysxlh" id="qzysxlh" readonly="readonly" style="width:200px;margin-left: -30px "></span>
                           </span>
                        </div>
                    </div>
                </div>
            </form>
        </div>

        <!--内容页-->
        <div class="">
            <form class="layui-form setOverflow" action="" lay-filter="form">
                <div id="details" class="clear details-background-img">
                    <div class="bdc-details-left">
                        <p class="bdc-table-tips">
                            <input type="text" class="layui-input" placeholder="" value="" name="sqsjc" id="sqsjc" readonly="readonly">
                            <input type="text" class="layui-input" placeholder="" value="" name="nf" id="nf" readonly="readonly">
                            <input type="text" class="layui-input szsxqc" placeholder="" value="" name="szsxqc" id="szsxqc" readonly="readonly">
                            <input type="text" class="layui-input zhlsh" placeholder="" value="" name="zhlsh" id="zhlsh" readonly="readonly">
                        </p>

                        <table class="layui-table bdc-zs-details-table">
                            <colgroup>
                                <col width="160">
                                <col>
                            </colgroup>
                            <tbody>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="qlr" id="qlr" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="gyfsmc" id="gyfsmc" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td class="zl-textarea">
                                    <!--<input type="text" class="layui-input" placeholder="" value="" name="zl" id="zl" readonly="readonly">-->
                                    <textarea name="zl" id="zl" style="resize: none;" class="layui-textarea" value=""></textarea>
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="bdcdyh" id="bdcdyh" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="qllxmc" id="qllxmc" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="qlxz"
                                           id="qlxz" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="yt" id="yt" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="mj" id="mj" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td"></td>
                                <td>
                                    <input type="text" class="layui-input" placeholder="" value="" name="syqx" id="syqx" readonly="readonly">
                                </td>
                            </tr>
                            <tr>
                                <td class="bdc-title-td bdc-title-td-qt"></td>
                                <td class="bdc-title-nr">
                                    <textarea name="qlqtzk" id="qlqtzk" style="resize: none;" class="layui-textarea" value=""></textarea>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                    <!--右侧-->
                    <div class="bdc-details-right">
                        <p class="bdc-notes-title"></p>
                        <div class="bdc-notes">
                            <!--地块列表：常州土地承包经营权证书页面展示，其它隐藏-->
                            <table id="dklb" class="layui-table new-table" style="border-color: white;display: none">
                                <colgroup>
                                    <col width="120">
                                    <col width="100">
                                    <col width="110">
                                    <col width="110">
                                    <col width="120">
                                </colgroup>
                                <tbody id="dklbxx">
                                <tr>
                                    <td class="set-back-color">地块名称</td>
                                    <td class="set-back-color">&nbsp;&nbsp;&nbsp;&nbsp;不动产单元号</td>
                                    <td class="set-back-color">合同面积</td>
                                    <td class="set-back-color">实测面积</td>
                                    <td class="set-back-color">&nbsp;&nbsp;&nbsp;&nbsp;四至</td>
                                </tr>
                                </tbody>
                            </table>
                            <!--家庭成员列表：常州农村不动产确权登记页面展示，其它隐藏-->
                            <table id="jtcy" class="layui-table new-table" style="border-color: white;display: none">
                                <colgroup>
                                    <col width="90">
                                    <col width="100">
                                    <col width="90">
                                    <col width="90">

                                </colgroup>
                                <tbody id="jtcyxx">
                                <tr>
                                    <td class="set-back-color">姓名</td>
                                    <td class="set-back-color">性别</td>
                                    <td class="set-back-color">与户主关系</td>
                                    <td class="set-back-color">证件号</td>

                                </tr>
                                </tbody>
                            </table>
                            <textarea name="fj" id="fj" style="resize: none;" class="layui-textarea" readonly="readonly"></textarea>
                        </div>
                    </div>
                </div>
            </form>
        </div>

        <!--附图页-->
        <div>
            <form class="layui-form setOverflow" action="" lay-filter="coverform">
                <div id="attachedPage" class="clear attachedPage-background-img">
                    <div class="bdc-details-left">
                        <img src="" id="zdt" style="margin: 12%;
    width: 500px;
    height: 84%;
    margin-top: 8%;"/>
                    </div>

                    <!--右侧-->
                    <div class="bdc-details-right">
                        <p class="bdc-notes-title"></p>
                        <div class="bdc-notes" style="height: 485px">
                        </div>
                        <div class="bdc-notes-top">
                            <span class="bdc-notes-top-content">
                           </span>
                        </div>
                        <div class="bdc-notes-top qzysxlh-div">
                            <span class="bdc-notes-top-content">
                           </span>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
</body>
<script src="../../static/lib/layui/layui.js"></script>
<script type="text/javascript" src="../../static/lib/jquery/jquery.min.js"></script>
<script type="text/javascript" src="../../static/lib/qrcode/jquery.qrcode.js"></script>
