<!-- 蚌埠移交单「已打印」查询台账-->
<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>不动产移交单查询</title>
    <meta content="webkit" name="renderer">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <link href="../static/lib/layui/css/layui.css" media="all" rel="stylesheet">
    <link href="../static/lib/form-select/formSelects-v4.css" rel="stylesheet"/>
    <link href="../static/lib/bdcui/css/search.css" rel="stylesheet"/>
    <link href="../static/lib/bdcui/css/table.css" rel="stylesheet"/>
    <link href="../static/lib/bdcui/css/mask.css" rel="stylesheet"/>
    <link href="../static/lib/registerui/css/percentage-layout.css" rel="stylesheet"/>
    <!--<link rel="stylesheet" href="../static/lib/bdcui/css/popup.css"/>-->

    <style type="text/css">
        .bdc-percentage-container {
            width: 100%;
            height: 100%;
            padding: 10px 10px 10px;
            box-sizing: border-box;
            position: relative;
            background-color: #eaedf1;
        }

        .required-span {
            color: #f54743;
            margin-right: 3px;
        }
    </style>
</head>
<body>
<!--************ 条件搜索 ************-->
<div class="bdc-percentage-container">

    <div class="bdc-search-box">
        <form action="" class="layui-form">
            <div class="layui-form-item pf-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">移交单号</label>
                    <div class="layui-input-inline">
                        <input autocomplete="off" class="layui-input search" id="yjdh" name="yjdh" placeholder="请输入"
                               type="text">
                        <i class="layui-icon layui-icon-close bdc-hide"></i>
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">受理编号</label>
                    <div class="layui-input-inline">
                        <input autocomplete="off" class="layui-input search" id="slbh" name="slbh" placeholder="请输入"
                               type="text">
                        <i class="layui-icon layui-icon-close bdc-hide"></i>
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">权利人</label>
                    <div class="layui-input-inline">
                        <input autocomplete="off" class="layui-input search" id="qlr" name="qlr" placeholder="请输入"
                               type="text">
                        <i class="layui-icon layui-icon-close bdc-hide"></i>
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">坐落</label>
                    <div class="layui-input-inline">
                        <input autocomplete="off" class="layui-input search" id="zl" name="zl" placeholder="请输入"
                               type="text">
                        <i class="layui-icon layui-icon-close bdc-hide"></i>
                    </div>
                </div>

                <div class="layui-inline bdc-button-box-four">
                    <a class="layui-btn layui-btn-normal bdc-major-btn" href="javascript:" id="search">查询</a>
                    <button class="layui-btn layui-btn-normal bdc-secondary-btn" type="reset">重置</button>
                </div>
            </div>
        </form>
    </div>


    <div class="bdc-table-box">
        <table id="bdcYjdTable" lay-filter="bdcYjdTable"></table>
    </div>

</div>


<script src="../static/lib/jquery/jquery.min.js"></script>
<script src="../static/lib/layui/layui.js"></script>
<!--添加水印-->
<script src="../static/lib/bdcui/js/common.js"></script>
<script src="../static/lib/bdcui/js/mask.js"></script>
<script src="../static/js/common.js"></script>
<script src="../bengbu/js/bdcYdyYjd.js"></script>

</body>
</html>