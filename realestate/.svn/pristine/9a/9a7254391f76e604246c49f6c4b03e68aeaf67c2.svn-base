.sjclForm {
    margin-top: 10px;
}
.bdc-title-sign-btn .bdc-title-sign-btn-box{
    position: absolute;
    top: 0;
    right: 10px;
}
.title-sign > .bdc-tab-a {
    position: absolute;
    top: 0;
    right: 0;
}
.bdc-jt-table .layui-form-checkbox[lay-skin=primary]{
    padding-left: 0;
    margin-top: 0;
}

.title-ui {
    z-index: 20;
    margin-right: 14px;
    position: absolute;
    background-color: #fff;
    border: 1px solid #d0d5da;
    top: 62px;
    right: -15px;
    display: none;
}

.title-ui li {
    padding: 0 14px 0 14px;
    height: 40px;
    line-height: 40px;
    font-size: 14px;
    color: #666;
    cursor: pointer;
}
input:-internal-autofill-selected{
    background-color: #fff!important;
}
.bdc-td-box .layui-form-danger + .layui-form-select,
.bdc-td-box .layui-form-danger.layui-input,
.bdc-td-box .layui-form-danger:focus {
     border: 1px solid #f54743 !important;
 }

#ycslzrfxx .bdc-basic-info:not(:first-child),
#ycslzcfxx .bdc-basic-info:not(:first-child){
    margin-top: 10px;
}

/*用于调整下拉列表高度*/
.select-height .layui-form-select dl {
    max-height: 210px;
}
