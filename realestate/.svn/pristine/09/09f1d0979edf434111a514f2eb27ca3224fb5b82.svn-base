<!--多个-->
<div class="bdc-search-box">
    <form class="layui-form" action="">
        <div class="layui-form-item pf-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">抽取批次号</label>
                <div class="layui-input-inline">
                    <input type="text" autocomplete="off" placeholder="请输入" class="layui-input">
                    <i class="layui-icon layui-icon-close bdc-hide"></i>
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">测试</label>
                <div class="layui-input-inline">
                    <input type="text" autocomplete="off" placeholder="请输入" class="layui-input">
                    <i class="layui-icon layui-icon-close bdc-hide"></i>
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">测试1</label>
                <div class="layui-input-inline">
                    <input type="text" autocomplete="off" placeholder="请输入" class="layui-input">
                    <i class="layui-icon layui-icon-close bdc-hide"></i>
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">行政区</label>
                <div class="layui-input-inline">
                    <input type="text" autocomplete="off" placeholder="请输入" class="layui-input">
                    <i class="layui-icon layui-icon-close bdc-hide"></i>
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">质检人</label>
                <div class="layui-input-inline">
                    <input type="text" autocomplete="off" placeholder="请输入" class="layui-input">
                    <i class="layui-icon layui-icon-close bdc-hide"></i>
                </div>
            </div>
            <div class="layui-inline pf-xf-state">
                <label class="layui-form-label">精确时分秒</label>
                <div class="layui-input-inline">
                    <input type="text" id="exact1" class="layui-input test-item">
                </div>
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">权利人</label>
                <div class="layui-input-inline">
                    <input type="text" lay-verify="title" autocomplete="off" placeholder="请输入" class="layui-input">
                    <i class="layui-icon layui-icon-close bdc-hide"></i>
                </div>
            </div>
            <div class="pf-senior-show bdc-hide">
                <div class="layui-inline">
                    <label class="layui-form-label">质检起始时间</label>
                    <div class="layui-input-inline pf-start-time-box">
                        <input type="text" placeholder="起始时间" id="startTime" autocomplete="off" class="layui-input test-item">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">质检结束时间</label>
                    <div class="layui-input-inline pf-end-time-box">
                        <input type="text" placeholder="结束时间" id="endTime" autocomplete="off" class="layui-input test-item">
                    </div>
                </div>
            </div>
            <div class="layui-inline bdc-button-box">
                <a href="javascript:;" class="layui-btn layui-btn-normal bdc-major-btn" id="cx">查询</a>
                <button type="reset" class="layui-btn layui-btn-normal bdc-secondary-btn">重置</button>
                <a href="javascript:;" class="layui-btn layui-btn-normal bdc-secondary-btn" id="seniorSearch">高级查询</a>
            </div>
        </div>
    </form>
</div>
</body>
