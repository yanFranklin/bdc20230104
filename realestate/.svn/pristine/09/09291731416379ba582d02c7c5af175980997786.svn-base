/**
* author: 前端组
* date: 2018-12-14
* version 3.0.0
* describe: 通用less，主要定义了主题色，一级次级按钮颜色及类名
*/
body,
html {
    font-family: "Microsoft YaHei", Helvetica, Arial, sans-serif !important;
    font-size: 14px;
}

.clear:after {
    content: '';
    display: block;
    clear: both;
}

.bdc-hide {
    display: none;
}

body {
    background-color: #eaedf1;
}

html,
body {
    width: 100%;
    height: 100%;
}

.content-div {
    width: 100%;
    height: 100%;
    padding: 84px 15px 15px 15px;
    box-sizing: border-box;
    margin: 0 auto;
}

.content-div .bdc-content-box {
    width: 100%;
    height: 100%;
    overflow-x: auto;
    background-color: #fff;
}

.content-div form {
    background-color: #fff;
    width: 1200px;
    margin: 0 auto;
}

.content-title {
    padding: 15px 0;
    border-bottom: 1px solid #d0d5da;
    background-color: #fff;
    height: 38px;
    clear: both;
    top: 15px;
    left: 15px;
    z-index: 9;
}

.content-title p {
    font-size: 20px;
    color: #333;
    float: left;
    margin-left: 20px;
    line-height: 38px;
}

.content-title .title-btn-area {
    float: right;
    margin-right: 20px;
}

.content-main {
    width: 100%;
    font: 600 16px "SimSun";
}

.layui-table td {
    overflow: hidden;
}

table {
    position: relative !important;
}

.content-main table input[type='text'] {
    display: block;
    border: none;
    outline: none;
    width: 90%;
    height: 100%;
    box-sizing: border-box;
}

.bdc-table-box {
    width: 100%;
}

.layui-table {
    max-width: 600px;
    margin: 0 auto;
    margin-bottom: 20px;
}

.content-main .layui-table tbody tr:hover,
.content-main .layui-table thead tr,
.content-main .layui-table-click,
.content-main .layui-table-header,
.content-main .layui-table-hover,
.content-main .layui-table-mend,
.content-main .layui-table-patch,
.content-main .layui-table-tool,
.content-main .layui-table[lay-even] tr:nth-child(even) {
    background-color: transparent;
}

table.layui-table .quarantine {
    border-left-color: transparent !important;
    border-right-color: transparent !important;
}

table {
    border: 0;
}

tr,
td {
    color: #333;
}

tr textarea,
td textarea {
    color: #333;
}

.layui-table td,
.layui-table th,
.layui-table-col-set,
.layui-table-fixed-r,
.layui-table-grid-down,
.layui-table-header,
.layui-table-page,
.layui-table-tips-main,
.layui-table-tool,
.layui-table-total,
.layui-table-view,
.layui-table[lay-skin=line],
.layui-table[lay-skin=row] {
    border-color: #d0d5da;
    font-size: 16px;
}

.layui-table td.set-back-color {
    background-color: #f3f4f6;
    padding: 0 6px;
}

.layui-table td,
.layui-table th {
    padding: 0;
    height: 39px;
    overflow: visible;
    position: static;
}

.layui-table-edit {
    box-shadow: none;
    position: relative;
    color: #333;
    padding: 0 10px;
}

td {
    text-align: right;
}

td textarea {
    display: block;
    width: 100%;
    height: 100%;
    border: none;
    outline: none;
    resize: none;
    padding-left: 5px;
    box-sizing: border-box;
}

td p {
    text-align: left;
}

.set-center {
    text-align: center;
}

.set-tr-height {
    height: 60px;
}

.set-tr-height158 {
    height: 158px;
}

.statement {
    font-size: 14px;
    color: #333;
    line-height: 23px;
    text-indent: 28px;
    margin: 20px 10px;
    text-align: left;
}

.content-main table .gnature {
    width: 800px;
    text-align: left;
}

.content-main table .gnature input[type='text'] {
    display: block;
    width: 182px;
    padding-left: 10px;
    height: 38px;
    line-height: 38px;
    background-color: #fff;
    border-radius: 2px;
    border: none;
}

.content-main table .setTimes .setTimes-float {
    float: left;
    margin-left: 90px;
}

.content-main table .setTimes .setTimes-float label {
    width: 18px !important;
    padding: 9px 0;
    margin: 0;
    text-align: left;
}

.content-main table .setTimes .setTimes-float input,
.content-main table .setTimes .setTimes-float label {
    float: left;
}

.content-main table .setTimes input[type='text'] {
    max-width: 38px;
    border: none;
    text-align: center;
}

.change-label-width {
    width: 138px !important;
    padding: 9px 8px;
    font-size: 14px;
}

input::-webkit-input-placeholder {
    color: #999;
}

.layui-form-select dl dd.layui-this {
    background-color: #1d87d1;
}

.layui-select-title {
    height: 38px;
}

.layui-form-select {
    text-align: left;
}

.layui-form-select .layui-input {
    color: #333;
}

.bdc-set-width {
    width: 80px;
    box-sizing: border-box;
}

.layui-input:hover,
.layui-textarea:hover {
    border-color: #D2D2D2 !important;
}

input:disabled {
    background-color: #fafbfb;
    cursor: not-allowed;
    color: #666 !important;
}

/**zmView**/
