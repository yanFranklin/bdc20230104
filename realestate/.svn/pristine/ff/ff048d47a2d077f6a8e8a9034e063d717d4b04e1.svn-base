 * @description 不动产系统业务配置UI应用：新增或者编辑证书模板配置页面
-->
<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>新增或者编辑证书模板配置</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link rel="stylesheet" href="../../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../../static/lib/bdcui/css/popup.css">
    <link rel="stylesheet" href="../../../static/lib/bdcui/css/form-tab.css"/>
    <link rel="stylesheet" href="../../../static/lib/bdcui/css/mask.css" />
    <style type="text/css">
        /*下拉框删除图标样式*/
        .layui-form-item .xzq-sele .layui-input-inline .layui-icon-close {
            right: 23px;

        }
        /*下拉框输入框样式（防止内容与删除图标覆盖）*/
        .layui-form-item .xzq-sele .layui-input-inline .layui-input{
            padding-right: 40px;
        }
    </style>
</head>
<body>
    <div id="bdc-popup-large">
        <form class="layui-form" action="" id="zsmbform" lay-filter="zsmbform">
            <input type="hidden" name="zsmbid"/>

            <div class="layui-form-item pf-form-item">
                <div class="layui-inline xzq-sele bdc-complete-line">
                    <label class="layui-form-label"><span class="required-span"><sub>*</sub></span>权利类型</label>
                    <div class="layui-input-inline">
                        <select name="qllx" lay-filter="qllx" id="qllx" lay-verify="required" lay-search="">
                            <option value="">请选择</option>
                        </select>
                        <i class="layui-icon layui-icon-close bdc-hide"></i>
                    </div>
                </div>

                <div class="layui-inline bdc-complete-line">
                    <label class="layui-form-label"><span class="required-span"><sub>*</sub></span>模板SQL</label>
                    <div class="layui-input-inline">
                        <textarea name="mbsql" lay-verify="required" class="layui-textarea" style="height:300px;" placeholder="请输入"></textarea>
                    </div>
                </div>

                <div class="layui-inline bdc-frame-button-box">
                    <button type = "button" class="layui-btn layui-btn-normal bdc-major-btn" id="check-btn">验证</button>
                    <button class="layui-btn layui-btn-normal bdc-major-btn" lay-submit="" lay-filter="submitBtn">保存</button>
                    <button class="layui-btn layui-btn-normal bdc-secondary-btn bdc-frame-close" lay-filter="closeBtn" onclick="closeWin();">取消</button>
                </div>
            </div>
        </form>
    </div>

    <script src="../../../static/lib/jquery/jquery.min.js"></script>
    <script src="../../../static/lib/layui/layui.all.js"></script>
    <!--base64加密-->
    <script src="../../../static/js/bjjk/base64.js"></script>
    <!--添加水印-->
    <script src="../../../static/lib/bdcui/js/common.js"></script>
    <script src="../../../static/js/config/common.js"></script>
    <script src="../../../static/js/config/zs/addOrEditZsmbPz.js"></script>
</body>
</html>