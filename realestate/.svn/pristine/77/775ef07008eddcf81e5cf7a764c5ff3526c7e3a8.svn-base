.bdc-form-div{
  padding-top: 10px;
}
.bdc-container{
  width: 100%;
  height: 100%;
}
.layui-tab-title li{
  font-size: 16px;
}
.bdc-search-box .layui-form-item{
  margin: 10px 0;
}

.basic-info{
  position: relative;
}
.title-sign{
  margin: 12px 0;
}
.title-sign p{
  font-size: 16px;
}
.bdc-xydm-info .layui-form-item{
  width: 100%;
}
.bdc-xydm-info .layui-inline{
  position: relative;
  padding-left: 150px;
  box-sizing: border-box;
}
.bdc-xydm-info .layui-inline .layui-form-label{
  position: absolute;
  top: 0;
  left: 0;
}
.bdc-xydm-info .layui-inline .layui-input-inline{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.basic-info .layui-form-item .layui-input-inline{
  height: 34px;
  line-height: 34px;
}
.bdc-sf-info .layui-form-item .layui-inline{
  width: 100%;
}
.bdc-xydm-info .layui-form-item .layui-inline{
  width: 50%;
}
.basic-info .layui-inline .layui-form-label{
  font-weight: 700;
}
.bdc-xydm-info .layui-inline .layui-form-label{
  width: 150px;
}
.bdc-tx{
  position: absolute;
  top: 50px;
  left: 400px;
}
.bdc-tx img{
  width: 150px;
}

.bdc-xydm-info .layui-inline .change-overflow{
  white-space: normal;
  word-break: break-all;
  overflow: hidden;
  line-height: normal;
  min-height: 34px;
  height: auto;
}