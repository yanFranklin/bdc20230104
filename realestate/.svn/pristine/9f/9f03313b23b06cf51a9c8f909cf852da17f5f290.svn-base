.bdc-percentage-container {
    width: 100%;
    height: 100%;
    padding: 103px 10px 10px;
    box-sizing: border-box;
    position: relative;
    background-color: #eaedf1;
}
.bdc-search-content {
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    padding: 10px;
    box-sizing: border-box;
}
.all-content {
    padding: 0 0px;
}
.all-content {
    height: 100%;
}
.layui-tab {
    padding-left: 0;
    padding-right: 0;
}
.layui-tab-item:first-child {
    min-height: 100%;
    height: auto;
    background-color: #fff;
}
.layui-tab-item {
    width: 100%;
    height: 100%;
    padding: 10px;
    box-sizing: border-box;
}
.chart-box-item{
    width: 950px;
    margin: 0 auto;
}
.chart-item {
    width: 950px;
    margin: auto;
    padding-top: 25px;
}