package cn.gtmap.realestate.common.core.dto.exchange.naturalresources.civil.marriagequery;

import io.swagger.annotations.ApiModelProperty;

/**
 * @author <a href="mailto:zhongjinpeng@gtmap.cn">zhongjinpeng</a>
 * @version 1.0
 * @date 2020/11/3 09:39
 */
public class CivilMarriageQueryParamDTO {

    @ApiModelProperty(value = "身份证号")
    private String certNum;

    @ApiModelProperty(value = "姓名")
    private String name;

    public String getCertNum() {
        return certNum;
    }

    public void setCertNum(String certNum) {
        this.certNum = certNum;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return "CivilMarriageQueryParamDTO{" +
                "certNum='" + certNum + '\'' +
                ", name='" + name + '\'' +
                '}';
    }

    public CivilMarriageQueryParamDTO() {
    }

    public CivilMarriageQueryParamDTO(String certNum, String name) {
        this.certNum = certNum;
        this.name = name;
    }
}
