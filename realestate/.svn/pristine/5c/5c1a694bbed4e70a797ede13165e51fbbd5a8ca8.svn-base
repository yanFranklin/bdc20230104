                            <input disabled="off" type="text" class="layui-input" title="{{bdcQlr.qlrmc || ''}}"
                                   value="{{bdcQlr.qlrmc || ''}}" >
                        </div>

                    </td>
                    <td class="ywr">

                        <div class="bdc-td-box">
                            <select disabled="off" name="qlrlx" class="qlr">
                                {{# layui.each(d.zd.qlrlx, function(index, item){ }}
                                <option value="{{item.DM || '0'}}" {{# if(bdcQlr.qlrlx===item.DM ){ }} selected="selected"
                                        {{#
                                        } }}>{{item.MC || ''}}
                                </option>
                                {{# }); }}
                            </select>
                        </div>
                    </td>
                    <td class="ywr">
                        <div class="bdc-td-box select-height">
                            <select disabled="off"  name="zjzl" class="qlr">
                                {{# layui.each(d.zd.zjzl, function(index, item){ }}
                                <option value="{{item.DM || '0'}}" {{# if(bdcQlr.zjzl==item.DM ){ }} selected="selected" {{#
                                        }
                                        }} title="{{item.MC || ''}}">{{item.MC || ''}}
                                </option>
                                {{# }); }}
