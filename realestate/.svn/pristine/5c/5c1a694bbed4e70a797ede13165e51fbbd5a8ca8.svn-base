                            <input disabled="off" type="text"  id="qlbl" name="qlbl"
                                   value="{{bdcQlr.qlbl || ''}}"
                                   class="layui-input qlr">
                        </div>
                    </td>
                    <td>
                        <div class="bdc-td-box">
                            <input disabled="off" type="text"  id="dh" name="dh"
                                   value="{{bdcQlr.dh || ''}}"
                                   class="layui-input qlr" title="{{bdcQlr.dh || ''}}">
                        </div>
                    </td>
                </tr>
