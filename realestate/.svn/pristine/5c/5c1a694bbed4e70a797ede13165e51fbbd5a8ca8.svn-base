                                <option value="{{item.DM || '0'}}" {{# if(bdcQlr.qlrlx===item.DM ){ }} selected="selected"
                                        {{#
                                        } }}>{{item.MC || ''}}
                                </option>
                                {{# }); }}
                            </select>
                        </div>
                    </td>
                    <td>
                        <div class="bdc-td-box select-height">
                            <select disabled="off" name="zjzl"  class="qlr">
