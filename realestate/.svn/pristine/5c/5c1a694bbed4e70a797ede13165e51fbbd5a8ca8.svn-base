                        <div class="layui-input-inline">
                            <input type="text" class="layui-input" disabled="off" id="djsj" name="djsj">
                            <img src="../../static/image/lock.png" alt="">
                        </div>
                    </div>
                    <div class="layui-inline bdc-two-column">
                        <label class="layui-form-label change-label-width">不动产权证号</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input" disabled="off" id="bdcqzh" name="bdcqzh">
                            <img src="../../static/image/lock.png" alt="">
                        </div>
                    </div>
                    <div class="layui-inline bdc-two-column">
                        <label class="layui-form-label change-label-width">不动产单元号</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input" disabled="off" id="bdcdyh" name="bdcdyh">
                            <img src="../../static/image/lock.png" alt="">
                        </div>
                    </div>
                    <div class="layui-inline bdc-two-column">
                        <label class="layui-form-label change-label-width">坐落</label>
                        <div class="layui-input-inline">
                            <input type="text" class="layui-input" disabled="off" id="zl" name="zl">
                            <img src="../../static/image/lock.png" alt="">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <label class="layui-form-label change-label-width">是否分别持证</label>
                        <div class="layui-input-inline">
                            <input type="radio" name="sqfbcz" value="1" title="是" class="change-radio-top" id="cz1">
                            <input type="radio" name="sqfbcz" value="0" title="否" class="change-radio-top" id="cz0">
                        </div>
                    </div>
                    <div class="layui-inline bdc-two-column">
                        <label class="layui-form-label change-label-width">面积单位</label>
                        <div class="layui-input-inline">
                            <input type="checkbox" name="mjdw" title="平方米" lay-skin="primary" id="mjdw1" disabled>
                            <input type="checkbox" name="mjdw" title="亩"     lay-skin="primary" id="mjdw2" disabled>
                            <input type="checkbox" name="mjdw" title="公顷"   lay-skin="primary" id="mjdw3" disabled>
                        </div>
                    </div>
                </div>

                <div class="layui-form-item change-textarea-margin">
                    <label class="layui-form-label change-label-width">备注</label>
                    <div class="layui-input-inline">
                        <textarea placeholder="请输入内容" class="layui-textarea change-textarea-width" id="bz" name="bz"></textarea>
                    </div>
                </div>
            </div>
            <div class="basic-info">
                <div class="title-sign"><p><a id="sqr" href="javascript:;">权利人信息</a></p></div>
                <div id="sqrView"></div>
            </div>
