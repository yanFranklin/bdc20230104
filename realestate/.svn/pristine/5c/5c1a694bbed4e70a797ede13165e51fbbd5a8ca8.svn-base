                        </div>
                    </td>
                    <td>
                        <div class="bdc-td-box">
                            <select disabled="off" name="qlrlb" id="qlrlb"
                                    class="qlrlb qlr">
                                {{# layui.each(d.zd.qlrlb, function(index, item){ }}
                                <option value="{{item.DM || '0'}}" {{# if(bdcQlr.qlrlb==item.DM ){ }} selected="selected"
                                        {{# }
                                        }} title="{{item.MC || ''}}">{{item.MC || ''}}
                                </option>
                                {{# }); }}
                            </select>
