.newpage{
    height: 35px;
    line-height: 35px;
    padding: 3px 0;
    font-size: 12px;
    border: 1px solid #d0d5da;
    border-top: none;
}

.pageInput{
    width: 40px;
    height: 30px;
    text-align: center;
    border-radius: 2px;
    border: 1px solid #d0d5da;
    font-size: 12px;
    margin: 0 10px;
    box-sizing: border-box;
}

.pageSelect{
    height: 30px;
    font-size: 12px;
    padding: 2px 5px;
    border: 1px solid #d0d5da;
    border-radius: 2px;
    margin: 0 10px;
}
.layui-laypage{
    margin: 7px 0 1px 0;
}
.pageInput:focus{
    border-color: #1d87d1;
}

.prev-page,
.next-page{
    /*background-image: url("../image/page-prev-1.png");*/
    display: inline-block;
    height: 28px;
    line-height: 28px;
    width: 46px;
    text-align: center;
    position: relative;
    top: 2px;
    cursor: pointer;
}

.prev-page:hover,
.next-page:hover{
    /*background-image: url("../image/page-prev-2.png");*/
    color: #1d87d1;
}

.is-last-page,
.is-first-page{
    /*background-image: url("../image/page-next-2.png");*/
    color: #d2d2d2!important;
    cursor: not-allowed!important;
}

.pageTotalButton{
    font-size: 12px;
    padding: 6px 10px;
    border-radius: 2px;
    border: 1px solid #d0d5da;
    background: transparent;
    cursor: pointer;
    margin-right: 10px;
}

.pageTotalButtonX{
    font-size: 12px;
    padding: 0 7px;
    border-radius: 2px;
    border: 1px solid #d0d5da;
    background: transparent;
    cursor: pointer;
    margin-right: 10px;
}

#pageToatl{
    display: none;
}


/*修改分页页面问题*/
.layui-laypage a{
    border: none;
}
.bdc-laypage-border{
    border: 1px solid #d0d5da;
    border-top: none;
    position: relative;
    top: -1px;
}