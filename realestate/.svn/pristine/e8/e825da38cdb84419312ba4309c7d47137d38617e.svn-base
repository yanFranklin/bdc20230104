<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <title>民政部-地名信息查询</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="../../static/lib/font-awesome-4.7.0/css/font-awesome.min.css" media="all">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/application.css">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css"/>
    <link rel="stylesheet" href="../../static/css/common.css"/>
</head>
<body>

<div class="content-div gxjklc-div">
    <div class="bdc-content-box">
        <form class="layui-form setOverflow" action="">
            <div class="bdc-content-fix">
                <div class="content-title">
                    <p>地名信息查询(民政部)</p>
                    <div class="title-btn-area .layui-layout-right">
                        <button type="reset" class="layui-btn bdc-secondary-btn upload-pdf">上传PDF</button>
                    </div>
                </div>
            </div>

            <div class="content-main">
                <div class="table-name"></div>

                <div class="bdc-table-box">
                    <table class="layui-table" border="1">
                        <colgroup>
                            <col width="100">
                            <col width="150">
                            <col width="100">
                            <col width="150">
                        </colgroup>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </form>
    </div>
</div>

<script type="text/html" id="cxywcsTpl">
    {{# if(d.cxywcs){ }}
    {{# layui.each(d.cxywcs,function(index,item){  }}
    <tr>
        <td class="set-back-color">地名</td>
        <td><input type="text" class="layui-table-edit" value="{{ item.name || '' }}" disabled="off"></td>
        <td class="set-back-color">6位区划代码</td>
        <td><input type="text" class="layui-table-edit" value="{{ item.code || '' }}" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">区划代码</td>
        <td colspan="3"><input type="text" class="layui-table-edit" value="" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名类别</td>
        <td><input type="text" class="layui-table-edit" value="" disabled="off"></td>
        <td class="set-back-color">罗马字母拼写</td>
        <td><input type="text" class="layui-table-edit" value="" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名含义</td>
        <td colspan="3" class="set-left"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名来历</td>
        <td colspan="3" class="set-left"></td>
    </tr>
    <tr>
        <td class="set-back-color">历史沿革</td>
        <td colspan="3" class="set-left"></td>
    </tr>
    <tr>
        <td colspan="4" class="quarantine"></td>
    </tr>
    {{# }); }}
    {{# }else{ }}
    <tr>
        <td class="set-back-color">地名</td>
        <td><input type="text" class="layui-table-edit" value="" disabled="off"></td>
        <td class="set-back-color">6位区划代码</td>
        <td><input type="text" class="layui-table-edit" value="" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">区划代码</td>
        <td colspan="3"><input type="text" class="layui-table-edit" value="" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名类别</td>
        <td><input type="text" class="layui-table-edit" value="" disabled="off"></td>
        <td class="set-back-color">罗马字母拼写</td>
        <td><input type="text" class="layui-table-edit" value="" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名含义</td>
        <td colspan="3" class="set-left"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名来历</td>
        <td colspan="3" class="set-left"></td>
    </tr>
    <tr>
        <td class="set-back-color">历史沿革</td>
        <td colspan="3" class="set-left"></td>
    </tr>
    {{# } }}
</script>
<script type="text/html" id="tableTpl">
    {{# layui.each(d,function(index,item){  }}
    <tr>
        <td class="set-back-color">地名</td>
        <td><input type="text" class="layui-table-edit" value="{{ item.standard_name || '' }}" disabled="off"></td>
        <td class="set-back-color">6位区划代码</td>
        <td><input type="text" class="layui-table-edit" value="{{ item.code || '' }}" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">区划代码</td>
        <td colspan="3"><input type="text" class="layui-table-edit" value="{{ item.place_code || '' }}" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名类别</td>
        <td><input type="text" class="layui-table-edit" value="{{ item.place_type || '' }}" disabled="off"></td>
        <td class="set-back-color">罗马字母拼写</td>
        <td><input type="text" class="layui-table-edit" value="{{ item.roman_alphabet_spelling || '' }}" disabled="off"></td>
    </tr>
    <tr>
        <td class="set-back-color">地名含义</td>
        <td colspan="3" class="set-left">{{ item.place_meaning || '' }}</td>
    </tr>
    <tr>
        <td class="set-back-color">地名来历</td>
        <td colspan="3" class="set-left">{{ item.place_origin || '' }}</td>
    </tr>
    <tr>
        <td class="set-back-color">历史沿革</td>
        <td colspan="3" class="set-left">{{ item.place_history || '' }}</td>
    </tr>
    <tr>
        <td colspan="4" class="quarantine"></td>
    </tr>
    {{# }); }}
</script>

<script src="../../static/lib/jquery/jquery.min.js"></script>
<script src="../../static/lib/layui/layui.js"></script>
<script src="../../static/lib/bdcui/js/mask.js"></script>
<!--添加水印-->
<script src="../../static/lib/bdcui/js/common.js"></script>
<script src="../../static/js/common.js"></script>
<script src="../../static/js/gxjklc/gxjkCommon.js"></script>
<script src="../../static/js/gxjklc/lcbjDmxx.js"></script>
</body>
</html>