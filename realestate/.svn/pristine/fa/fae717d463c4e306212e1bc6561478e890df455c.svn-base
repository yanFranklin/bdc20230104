body {
    padding: 0px;
    margin: 0px;
    color: #222;
    font-family: '微软雅黑';
}
.bdc-search-box h1 {
    font-weight: bold;
    text-align: center;
}
.contain{
    padding-left: 0px ;
    padding-right: 0px;
    height: calc( 100% - 60px);
}

.allnav {
    background-color: #FFFFFF;
}
.layui-col-md6{
    border-top: 10px solid #EAEDF1;
}

.div-col-left{
   padding-right: 20px;
}
.div-col-left,.div-col-right{
    padding-top: 10px;
    overflow: hidden;
}
.goback{
    font-size:30px;
    text-align: left;
    position: absolute;
    left: 40px;
    color: white;
    cursor: pointer;
    font-weight: bold;
    color: #748EDE;
}