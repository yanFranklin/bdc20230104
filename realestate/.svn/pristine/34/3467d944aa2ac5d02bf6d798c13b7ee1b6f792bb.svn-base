<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta charset="UTF-8">
    <title>房屋信息查询单</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="../../static/lib/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/table.css" media="all">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/form.css" media="all">
    <link rel="stylesheet" href="../../static/lib/bdcui/css/mask.css" media="all">
</head>
<body>
<div class="bdc-form-div bdc-form-not-full">
    <form class="layui-form setOverflow bdc-form-table" action="">
        <div class="bdc-content-fix">
            <div class="content-title layui-clear">
                <p>房屋信息查询单</p>
                <div class="title-btn-area">
                    <a class="layui-btn bdc-major-btn" id="fwxxcx">房屋查询</a>
                </div>
            </div>
        </div>
        <div class="form-margin-area">
            <div class="basic-info">
                <div class="title-sign"><p><a id="slick1" href="javascript:">转入方房屋信息</a></p></div>
                <div class="layui-form-item">
                    <div class="bdc-jt-table">
                        <table class="layui-table" border="1">
                            <colgroup>
                                <col width="230">
                                <col width="150">
                                <col width="150">
                                <col width="200">
                                <col width="220">
                                <col width="150">
                            </colgroup>
                            <thead>
                            <tr>
                                <th>产权证号</th>
                                <th>不动产单元号</th>
                                <th>权利人</th>
                                <th>权利人证件号</th>
                                <th>房屋坐落</th>
                                <th>房屋用途</th>
                            </tr>
                            </thead>
                            <tbody id="zrffwxx">
                            <tr class="bdc-table-none">
                                <td colspan="6">
                                    <div class="layui-none"><img src="../../static/lib/bdcui/images/table-none.png"
                                                                 alt="">无数据
                                    </div>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

            <div class="basic-info">
                <div class="title-sign bdc-title-sign-btn">
                    <p><a id="slick2" href="javascript:;">转出方房屋信息</a></p>
                </div>
                <div class="layui-form-item">
                    <div class="bdc-jt-table">
                        <table class="layui-table" border="1">
                            <colgroup>
                                <col width="230">
                                <col width="150">
                                <col width="150">
                                <col width="200">
                                <col width="220">
                                <col width="150">
                            </colgroup>
                            <thead>
                            <tr>
                                <th>产权证号</th>
                                <th>不动产单元号</th>
                                <th>权利人</th>
                                <th>权利人证件号</th>
                                <th>房屋坐落</th>
                                <th>房屋用途</th>
                            </tr>
                            </thead>
                            <tbody id="zcffwxx">
                            <tr class="bdc-table-none">
                                <td colspan="6">
                                    <div class="layui-none"><img src="../../static/lib/bdcui/images/table-none.png"
                                                                 alt="">无数据
                                    </div>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

<!--插入房屋信息模块-->
<script type="text/html" id="fwxxTpl">
    {{#if(d.fwxxlist &&d.fwxxlist.length >0){ }}
    <tbody>
    {{# for(var i = 0, len = d.fwxxlist.length; i < len; i++){ }}
    <tr>
        <td>
            {{ d.fwxxlist[i].bdcqzh  || '' }}
        </td>
        <td>
            {{ d.fwxxlist[i].bdcdyh  || '' }}
        </td>
        <td>
            {{ d.fwxxlist[i].qlrmc  || '' }}
        </td>
        <td>
            {{ d.fwxxlist[i].qlrzjh  || '' }}
        </td>
        <td>
            {{ d.fwxxlist[i].zl  || '' }}
        </td>
        <td>
            {{ d.fwxxlist[i].ghyt  || '' }}
        </td>
    </tr>
    {{# } }}
    </tbody>
    {{# }else { }}
    <tr class="bdc-table-none">
        <td colspan="6">
            <div class="layui-none"><img src="../../static/lib/bdcui/images/table-none.png"
                                         alt="">无数据
            </div>
        </td>
    </tr>
    {{# } }}
</script>
<script src="../../static/lib/layui/layui.js"></script>
<script src="../../static/lib/jquery/jquery.min.js"></script>
<script src="../../static/lib/bdcui/js/common.js"></script>
<script src="../../static/js/common.js"></script>
<script src="../../static/js/query/fwxx.js"></script>
</body>
</html>