public class BdcYzxxDTO {
    /**
     * 验证项数据ID
     */
    private String yzxsjid;

    /**
     * 提示信息
     */
    private String tsxx;

    /**
     * 验证模式（alert/confirm）
     */
    private String yzms;

    public String getYzxsjid() {
        return yzxsjid;
    }

    public void setYzxsjid(String yzxsjid) {
        this.yzxsjid = yzxsjid;
    }

    public String getTsxx() {
        return tsxx;
    }

    public void setTsxx(String tsxx) {
