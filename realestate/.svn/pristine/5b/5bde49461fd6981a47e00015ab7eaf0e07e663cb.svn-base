package cn.gtmap.realestate.common.core.dto.portal;

import java.util.List;
import java.util.Set;

/**
 * @author <a href ="mailto:songhaowen@gtmap.cn">songhaowen</a>
 * @version 1.3, 2019-10-15
 * @description 必填项验证dto
 */
public class BdcBtxyzDTO {
    /**
     * 表单名称
     */
    private String formName;
    /**
     * 工作流实例id
     */
    private String gzlslid;
    /**
     * 任务id
     */
    private String taskId;
    /**
     * 条件
     */
    private String condition;
    /**
     * 批量查询sql
     */
    private List<String> gzlslidSqlList;
    /**
     * 单个查询sql
     */
    private List<String> xmidSqlList;
    /**
     *
     */
    private Set<String> set;
    /**
     * 字段名称
     */
    private Set<String> columnNameSet;

    public String getFormName() {
        return formName;
    }

    public void setFormName(String formName) {
        this.formName = formName;
    }

    public String getGzlslid() {
        return gzlslid;
    }

    public void setGzlslid(String gzlslid) {
        this.gzlslid = gzlslid;
    }

    public String getTaskId() {
        return taskId;
    }

    public void setTaskId(String taskId) {
        this.taskId = taskId;
    }

    public String getCondition() {
        return condition;
    }

    public void setCondition(String condition) {
        this.condition = condition;
    }

    public List<String> getGzlslidSqlList() {
        return gzlslidSqlList;
    }

    public void setGzlslidSqlList(List<String> gzlslidSqlList) {
        this.gzlslidSqlList = gzlslidSqlList;
    }

    public List<String> getXmidSqlList() {
        return xmidSqlList;
    }

    public void setXmidSqlList(List<String> xmidSqlList) {
        this.xmidSqlList = xmidSqlList;
    }

    public Set<String> getSet() {
        return set;
    }

    public void setSet(Set<String> set) {
        this.set = set;
    }

    public Set<String> getColumnNameSet() {
        return columnNameSet;
    }

    public void setColumnNameSet(Set<String> columnNameSet) {
        this.columnNameSet = columnNameSet;
    }

    @Override
    public String toString() {
        return "BdcBtxyzDTO{" +
                "formName='" + formName + '\'' +
                ", gzlslid='" + gzlslid + '\'' +
                ", taskId='" + taskId + '\'' +
                ", condition='" + condition + '\'' +
                ", gzlslidSqlList=" + gzlslidSqlList +
                ", xmidSqlList=" + xmidSqlList +
                ", set=" + set +
                ", columnNameSet=" + columnNameSet +
                '}';
    }
}
