                    <th>实测建筑面积</th>
                    <th>本幢实测面积占比例（万分比）</th>
                </tr>
                </thead>
                <tbody>
                {{# if(d.length==0) { }}
                <tr class="bdc-table-none">
                    <td colspan="7">
                        <div class="layui-none"><img src="../static/lib/bdcui/images/table-none.png" alt="">无数据</div>
                    </td>
                </tr>
                {{# } else { }}
                {{#  layui.each(d, function(index, item){ }}
                <tr class="gray-tr">
                    <td>{{ item.dh || '' }}</td>
                    <td>{{ item.gcjdmc || '' }}</td>
                    <td>{{ item.ghjzmj || '' }}</td>
                    <td>{{ item.bzghmjbl || '' }}</td>
                    <td>{{ item.ycjzmj || '' }}</td>
                    <td>{{ item.scjzmj || '' }}</td>
                    <td>{{ item.bzscmjbl || '' }}</td>
                </tr>
                {{#  }); }}
                {{# } }}
                </tbody>
            </table>

