<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="false" map-empty-string="false" type="one-way" wildcard="false" >
        <class-a>com.alibaba.fastjson.JSONObject</class-a>
        <class-b>java.util.HashMap</class-b>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.BdcDmToDsfZdCustomConvert"
               custom-converter-param="DSF_ZD_AUTH,ems">
            <a key="qxdm">this</a>
            <b key="authorization">this</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.ConstantsCustomConvert"
               custom-converter-param="UTF-8">
            <a key="wlydh">this</a>
            <b key="charset">this</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="ems.appkey">
            <a key="wlydh">this</a>
            <b key="app_key">this</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.ConstantsCustomConvert"
               custom-converter-param="ems.inland.trace.query">
            <a key="wlydh">this</a>
            <b key="method">this</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.ConstantsCustomConvert"
               custom-converter-param="json">
            <a key="wlydh">this</a>
            <b key="format">this</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.ConstantsCustomConvert"
               custom-converter-param="V3.01">
            <a key="wlydh">this</a>
            <b key="version">this</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.NewDateCustomConverter"
               custom-converter-param="yyyy-MM-dd hh:mm:ss">
            <a key="wlydh">this</a>
            <b key="timestamp">this</b>
        </field>
        <field>
            <a key="wlydh">this</a>
            <b key="mailNo">this</b>
        </field>
    </mapping>
</mappings>
