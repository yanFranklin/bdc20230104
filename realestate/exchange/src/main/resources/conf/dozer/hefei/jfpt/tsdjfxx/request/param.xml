<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="false" map-empty-string="false" type="one-way">
        <class-a>cn.gtmap.realestate.exchange.core.dto.hefei.jfpt.request.JfptTsDjfxxRequestPrePayInfoJson</class-a>
        <class-b>cn.gtmap.realestate.exchange.core.dto.hefei.jfpt.request.JfptTsDjfxxRequestPrePayInfoJson</class-b>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ZdDmToMcCustomConvert"
        custom-converter-param="djxl">
            <a>itemName</a>
            <b>itemName</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.BdcDmToDsfZdCustomConvert"
               custom-converter-param="DSF_ZD_AREACODE,jfpt">
            <a>payAreaCode</a>
            <b>payAreaCode</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.BdcDmToDsfZdCustomConvert"
               custom-converter-param="DSF_ZD_AREANAME,jfpt">
            <a>payAreaCode</a>
            <b>payAreaName</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.BdcDmToDsfZdCustomConvert"
               custom-converter-param="DSF_ZD_DEPTCODE,jfpt">
            <a>payDeptCode</a>
            <b>payDeptCode</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.BdcDmToDsfZdCustomConvert"
               custom-converter-param="DSF_ZD_DEPTNAME,jfpt">
            <a>payDeptName</a>
            <b>payDeptName</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.BdcDmToDsfZdCustomConvert"
               custom-converter-param="DSF_ZD_ITEMCODE,jfpt">
            <a>payItemCode</a>
            <b>payItemCode</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.BdcDmToDsfZdCustomConvert"
               custom-converter-param="DSF_ZD_ITEMNAME,jfpt">
            <a>payItemName</a>
            <b>payItemName</b>
        </field>
    </mapping>
</mappings>
