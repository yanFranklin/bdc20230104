<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="true" map-empty-string="true" wildcard="false" type="one-way">
        <class-a>com.alibaba.fastjson.JSONObject</class-a>
        <class-b>com.alibaba.fastjson.JSONObject</class-b>
        <field>
            <a>slbh</a>
            <b>centerNumber</b>
        </field>
        <field>
            <a>bdcqzh</a>
            <b>cardNumber</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.FormatDateStringCustomConverter"
               custom-converter-param="yyyyMMdd HH:mm:ss">
            <a>djsj</a>
            <b>actionTime</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.ConstantsCustomConvert"
               custom-converter-param="1">
            <a>slbh</a>
            <b>result</b>
        </field>
    </mapping>
</mappings>
