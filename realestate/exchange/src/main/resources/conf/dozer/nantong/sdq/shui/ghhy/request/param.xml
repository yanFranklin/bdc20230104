<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="true" map-empty-string="true" wildcard="false" type="one-way">
        <class-a>cn.gtmap.realestate.common.core.dto.exchange.sdqgh.dian.sqgh.request.SdqHyRequestDTO</class-a>
        <class-b>com.alibaba.fastjson.JSONObject</class-b>
        <field>
            <a>consno</a>
            <b>electricFeeNum</b>
        </field>
        <field>
            <a>dwdm</a>
            <b>org_no</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.ConstantsCustomConvert"
               custom-converter-param="1">
            <a>sfcxqf</a>
            <b>flag</b>
        </field>

<!--        qxdm,dwdm,ywlx,jklx是为了获取配置项接口地址-->
        <field>
            <a>dwdm</a>
            <b>dwdm</b>
        </field>
        <field>
            <a>qxdm</a>
            <b>qxdm</b>
        </field>
        <field>
            <a>ywlx</a>
            <b>ywlx</b>
        </field>
        <field>
            <a>jklx</a>
            <b>jklx</b>
        </field>
    </mapping>


</mappings>
