<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="false" map-empty-string="false" type="one-way" wildcard="false">
        <class-a>java.util.Map</class-a>
        <class-b>com.alibaba.fastjson.JSONObject</class-b>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.EmptyStringCustomConvert">
            <a key="number">this</a>
            <b>regionCode</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.EmptyStringCustomConvert">
            <a key="number">this</a>
            <b>orgid</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ConstantsCustomConvert"
               custom-converter-param="0000">
            <a key="number">this</a>
            <b>statusCode</b>
        </field>
    </mapping>
</mappings>
