<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="true" map-empty-string="true" type="one-way" wildcard="false">
        <class-a>com.alibaba.fastjson.JSONObject</class-a>
        <class-b>com.alibaba.fastjson.JSONObject</class-b>
        <field>
            <a>head.msg</a>
            <b>msg</b>
        </field>
        <field map-id="sfhc_data">
            <a>data.ResourceInfos[0].ReturnInfos</a>
            <b>data</b>
            <b-deep-index-hint>com.alibaba.fastjson.JSONArray</b-deep-index-hint>
        </field>
    </mapping>
    <mapping map-id="sfhc_data"  type="one-way" wildcard="false">
        <class-a>com.alibaba.fastjson.JSONObject</class-a>
        <class-b>com.alibaba.fastjson.JSONObject</class-b>
        <field>
            <a>CheckSourceInfos.xm</a>
            <b>xm</b>
        </field>
        <field>
            <a>CheckCode</a>
            <b>CheckCode</b>
        </field>
        <field>
            <a>CheckCodeDesc</a>
            <b>CheckCodeDesc</b>
        </field>
    </mapping>

</mappings>