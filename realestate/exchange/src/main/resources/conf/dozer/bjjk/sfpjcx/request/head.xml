<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="false" map-empty-string="false" type="one-way" wildcard="false" >
        <class-a>com.alibaba.fastjson.JSONObject</class-a>
        <class-b>cn.gtmap.realestate.exchange.core.dto.bjjk.request.BjjkRequestHead</class-b>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
                custom-converter-param="bjjk.id">
            <a key="slbh">this</a>
            <b>id</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="bjjk.token">
            <a key="slbh">this</a>
            <b>token</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="bjjk.deptName">
            <a key="slbh">this</a>
            <b>deptName</b>
        </field>

        <field custom-converter="cn.gtmap.realestate.common.core.dozer.converters.gtmap.ApplicationConfigCustomConvert"
               custom-converter-param="bjjk.xzqh">
            <a key="slbh">this</a>
            <b>xzqh</b>
        </field>

        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.CurrentUserNameCustomConvert">
            <a key="slbh">this</a>
            <b>userName</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.ConstantsCustomConvert"
               custom-converter-param="GF01">
            <a key="slbh">this</a>
            <b>requestType</b>
        </field>
        <field custom-converter="cn.gtmap.realestate.exchange.core.dozer.converters.SourceServiceCustomConvert"
               custom-converter-param="bjjk_cxqqdh,false">
            <a key="slbh">this</a>
            <b>cxqqdh</b>
        </field>
        <field>
            <a key="slbh">this</a>
            <b>businessNumber</b>
        </field>
    </mapping>
</mappings>
