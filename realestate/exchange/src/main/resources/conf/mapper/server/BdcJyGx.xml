<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--收件信息-->
<mapper namespace="cn.gtmap.realestate.exchange.core.mapper.server.BdcJyGxMapper">

    <!-- 南通自助查询收费信息 -->
    <select id="queryBdcJyGxList" resultType="map">
        SELECT bdcdyh,fwbm,djbh from bdc_jy_gx a
        where a.bdcdyh = #{bdcdyh}
    </select>

</mapper>