<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:ex="http://www.gtmap.cn/schema/exchange-bean"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.gtmap.cn/schema/exchange-bean http://www.gtmap.cn/schema/exchange-bean.xsd">

    <!--  委托信息附件信息查询 -->
    <ex:bean id="wtxx_fjcx">
        <ex:service-info request-type="httpGet">
            <ex:property name="logEventName" value="委托信息附件信息查询"/>
            <ex:property name="beanName" value="BdcSjclServiceImpl"/>
            <ex:property name="requester" value="BDC"/>
            <ex:property name="responser" value="WTXX"/>
            <ex:property name="dsfFlag" value="WTXX"/>
            <ex:property name="url" value="${wtxx.fjcx.url}"/>
            <ex:property name="contentType" value="application/x-www-form-urlencoded"/>
        </ex:service-info>
    </ex:bean>

</beans>