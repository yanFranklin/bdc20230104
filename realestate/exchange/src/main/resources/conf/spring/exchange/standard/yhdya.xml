<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:ex="http://www.gtmap.cn/schema/exchange-bean"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.gtmap.cn/schema/exchange-bean http://www.gtmap.cn/schema/exchange-bean.xsd">
    <ex:bean id="delete_yhhd">
        <!-- 请求结构体 -->
        <ex:service-info request-type="httpPost">
            <ex:property name="requester" value="BDC"/>
            <ex:property name="responser" value="YH"/>
            <ex:property name="dsfFlag" value="YH"/>
            <ex:property name="url" value="${delete.yhhd.url}"/>
            <ex:property name="replaceUrl" value="yanchengReplaceUrl"/>
            <ex:property name="replaceUrlHandlerType" value="delete"/>
        </ex:service-info>
    </ex:bean>
</beans>